		epiButton1.Click -= new System.EventHandler(Script.epiButton1_Click);
		epiUltraCombo1.AfterCloseUp -= new System.EventHandler(Script.epiUltraCombo1_AfterCloseUp);
		epiButton2.Click -= new System.EventHandler(Script.epiButton2_Click);
		epiUltraGrid1.CellChange -= new Infragistics.Win.UltraWinGrid.CellEventHandler(Script.epiUltraGrid1_CellChange);
		epiButton4.Click -= new System.EventHandler(Script.epiButton4_Click);
		cmdFilterDemand.Click -= new System.EventHandler(Script.cmdFilterDemand_Click);
		cmdComponents.Click -= new System.EventHandler(Script.cmdComponents_Click);
		ComponentGrid.CellChange -= new Infragistics.Win.UltraWinGrid.CellEventHandler(Script.ComponentGrid_CellChange);
		cmdInventory.Click -= new System.EventHandler(Script.cmdInventory_Click);
		LocationGrid.CellChange -= new Infragistics.Win.UltraWinGrid.CellEventHandler(Script.LocationGrid_CellChange);
		cmdIssue.Click -= new System.EventHandler(Script.cmdIssue_Click);
		cboInvPlant.AfterCloseUp -= new System.EventHandler(Script.cboInvPlant_AfterCloseUp);
		cmdTest.Click -= new System.EventHandler(Script.cmdTest_Click);
		cmdReprint.Click -= new System.EventHandler(Script.cmdReprint_Click);
		epiUltraCombo2.AfterCloseUp -= new System.EventHandler(Script.epiUltraCombo2_AfterCloseUp);
		cmdGetAllocated.Click -= new System.EventHandler(Script.cmdGetAllocated_Click);
		cmdDeAllocated.Click -= new System.EventHandler(Script.cmdDeAllocated_Click);		
		LocationGrid.AfterSortChange -= new Infragistics.Win.UltraWinGrid.BandEventHandler(Script.LocationGrid_AfterSortChange);
		cmdRemoveOutOfStock.Click -= new System.EventHandler(Script.cmdRemoveOutOfStock_Click);
		
		// End Wizard Added Object Disposal 
		// Begin Custom Code Disposal 

		// End Custom Code Disposal 

		
	} 

	private static void UD14Form_Load(object sender, EventArgs args)
	{
		try
		{
			bool selected = false;
			

			DataSet ds = SearchFunctions.listLookup(UD14Form, "PlantAdapter", out selected, false, "");
		
			DataTable dt = ds.Tables[0].Copy();

			foreach (DataColumn dc in ds.Tables[0].Columns)
			{
				if (dc.ColumnName.ToUpper() != "NAME")
					if (dc.ColumnName.ToUpper() != "PLANT")
						dt.Columns.Remove(dc.ColumnName);
			}
		
			epiUltraCombo1.DataSource = dt;
			cboInvPlant.DataSource = dt.Copy();
		
			epiUltraCombo1.DataBind();
			cboInvPlant.DataBind();
			
			//epiUltraCombo1.Text = dt.Rows[0]["Plant"].ToString();
			
			epiUltraCombo1.Text = ((Ice.Core.Session)UD14Form.Session).PlantID;
			
			epiUltraCombo1_AfterCloseUp(null, args);
			cboInvPlant_AfterCloseUp(null,args);
			
			epiButton4.ReadOnly = true;
			cmdRemoveOutOfStock.ReadOnly = true;
			epiTextBox4.Text = "";
			epiTextBox3.Text = "";
		
			txtStyle.Text = "";
			txtOrderNum.Text = "";
			txtJobNumber.Text = "";
			txtLine.Text = "";
			txtRelease.Text = "";
			
			cmdTest.Visible = false;
			
			CompanyID = ((Ice.Core.Session)UD14Form.Session).CompanyID;
			
			baseToolbarsManager.Visible = false;
		
			((Infragistics.Win.UltraWinDock.WindowDockingArea)Script.csm.PersonalizeCustomizeManager.ControlsHT["UD14Form.windowDockingArea1"]).Visible = false;
			
			
		}
		catch
		{
		}
	}


	private static void cboInvPlant_AfterCloseUp(object sender, System.EventArgs args)
	{
		bool selected = false;
		
		if(cboInvPlant.Value != null)
		try
		{
			DataSet ds = SearchFunctions.listLookup(UD14Form, "WarehseAdapter", out selected, false, "Plant = '" + cboInvPlant.Value.ToString() + "'");
			
			DataTable dt = ds.Tables[0].Copy();

			foreach (DataColumn dc in ds.Tables[0].Columns)
			{				
				if (dc.ColumnName.ToUpper() != "WAREHOUSECODE")
					if (dc.ColumnName.ToUpper() != "DESCRIPTION")
						dt.Columns.Remove(dc.ColumnName);
			}
			
			cboInvWarehouse.DataSource = dt;
			cboInvWarehouse.DataBind();
		}
		catch
		{
		}
	}


	private static void epiUltraCombo2_AfterCloseUp(object sender, System.EventArgs args)
	{
		try
		{
			cboInvWarehouse.Text = epiUltraCombo2.Text;
		}
		catch{}
	}

	private static void epiUltraCombo1_AfterCloseUp(object sender, System.EventArgs args)
	{
		bool selected = false;
		
		if(epiUltraCombo1.Value != null)
		try
		{
			cboInvPlant.Value = epiUltraCombo1.Value;
			cboInvPlant_AfterCloseUp(sender, args);
			
			DataSet ds = SearchFunctions.listLookup(UD14Form, "WarehseAdapter", out selected, false, "Plant = '" + epiUltraCombo1.Value.ToString() + "'");
			
			DataTable dt = ds.Tables[0].Copy();

			foreach (DataColumn dc in ds.Tables[0].Columns)
			{				
				if (dc.ColumnName.ToUpper() != "WAREHOUSECODE")
					if (dc.ColumnName.ToUpper() != "DESCRIPTION")
						dt.Columns.Remove(dc.ColumnName);
			}
			
			epiUltraCombo2.DataSource = dt;
			epiUltraCombo2.DataBind();
			
			epiUltraCombo2.Text = dt.Rows[0]["WAREHOUSECODE"].ToString();
			epiUltraCombo2_AfterCloseUp(sender,args);
			
			if(((Ice.Core.Session)UD14Form.Session).PlantID != epiUltraCombo1.Value.ToString())			
				MessageBox.Show("You have selected a plant that you are not logged into.  You are able to see demand and allocations, but you will be unable to allocate to these jobs/orders.", "Plant Mis-match");
			
		}
		catch
		{
		}
	}

	private static DataTable GetInventoryTable()
	{
		DataTable dt = new DataTable();
		
		dt.Columns.Add("Sel", typeof(bool));
		dt.Columns.Add("Location", typeof(string));
		dt.Columns.Add("Lot", typeof(string));
		dt.Columns.Add("Batch", typeof(string));
		dt.Columns.Add("Comments", typeof(string));
		dt.Columns.Add("Age", typeof(DateTime));
		dt.Columns.Add("OH Qty", typeof(decimal));
		dt.Columns.Add("PartNum", typeof(string));
		dt.Columns.Add("WarehouseCode", typeof(string));
		dt.Columns.Add("Plant", typeof(string));
		dt.Columns.Add("RowID", typeof(int));
		dt.Columns.Add("JobNumber", typeof(string));
		dt.Columns.Add("OrderNum", typeof(int));
		dt.Columns.Add("OrderLine", typeof(int));
		dt.Columns.Add("OrderRel", typeof(int));
		dt.Columns.Add("AssemblySeq", typeof(int));
		dt.Columns.Add("MaterialSeq", typeof(int));
		dt.Columns.Add("Default", typeof(bool));
		dt.Columns.Add("ReceiptDate", typeof(DateTime));
		
		return dt;
	}
	private static DataTable GetComponentTable()
	{
		DataTable dt = new DataTable();		
		
		dt.Columns.Add("Sel", typeof(bool));
		dt.Columns.Add("Component", typeof(string));
		dt.Columns.Add("Qty", typeof(decimal));
		dt.Columns.Add("Accum", typeof(decimal));
		dt.Columns.Add("TempAccum", typeof(decimal));
		dt.Columns.Add("JobNumber", typeof(string));
		dt.Columns.Add("OrderNum", typeof(int));
		dt.Columns.Add("OrderRel", typeof(int));
		dt.Columns.Add("OrderLine", typeof(int));
		dt.Columns.Add("AssemblySeq", typeof(int));
		dt.Columns.Add("MaterialSeq", typeof(int));
		dt.Columns.Add("RowID", typeof(int));
		dt.Columns.Add("Plant", typeof(string));
		dt.Columns.Add("WarehouseCode", typeof(string));
		dt.Columns.Add("PartNum", typeof(string));
		dt.Columns.Add("OrderOnly", typeof(bool));
		dt.Columns.Add("TFOrderOnly", typeof(bool));
		dt.Columns.Add("Code", typeof(string));
		dt.Columns.Add("Style", typeof(string));
		dt.Columns.Add("Color", typeof(string));
		dt.Columns.Add("Material", typeof(string));
		dt.Columns.Add("DueDate", typeof(DateTime));
		return dt;
	}

	private static DataTable CreateDemandTable()
	{
		DataTable dt = new DataTable();
		
		dt.Columns.Add("Selected", typeof(bool));
		dt.Columns.Add("Job Number", typeof(string));
		dt.Columns.Add("Qty", typeof(decimal));
		dt.Columns.Add("Start Date", typeof(DateTime));
		dt.Columns.Add("Part", typeof(string));
		dt.Columns.Add("Due Date", typeof(DateTime));
		dt.Columns.Add("Code", typeof(string));
		dt.Columns.Add("Style", typeof(string));
		dt.Columns.Add("Color", typeof(string));
		dt.Columns.Add("Width", typeof(decimal));
		dt.Columns.Add("Length", typeof(decimal));		
		dt.Columns.Add("Material", typeof(string));
		dt.Columns.Add("Order #", typeof(string));
		dt.Columns.Add("Order Line", typeof(string));
		dt.Columns.Add("Order Rel", typeof(string));		
		dt.Columns.Add("CM", typeof(string));
		dt.Columns.Add("SB", typeof(string));
		dt.Columns.Add("BTO", typeof(bool));
		dt.Columns.Add("DS", typeof(bool));
		dt.Columns.Add("RowID", typeof(int));
		dt.Columns.Add("Plant", typeof(string));
		dt.Columns.Add("WarehouseCode", typeof(string));
		dt.Columns.Add("AssemblySeq", typeof(string));
		dt.Columns.Add("OrderOnly", typeof(bool));
		dt.Columns.Add("TFOrderOnly", typeof(bool));
		dt.Columns.Add("ColorCode", typeof(string));
		dt.Columns.Add("MaterialCode", typeof(string));	
		dt.Columns.Add("Contract", typeof(string));	
		
		
		return dt;
	}

	private static DataTable getIssuedGrid()
	{
		DataTable dt = new DataTable();
		
		dt.Columns.Add("Job Number", typeof(string));
		dt.Columns.Add("Start Date", typeof(DateTime));
		dt.Columns.Add("Part", typeof(string));
		dt.Columns.Add("Qty", typeof(decimal));
		dt.Columns.Add("Due Date", typeof(DateTime));
		dt.Columns.Add("Code", typeof(string));
		dt.Columns.Add("Style", typeof(string));
		dt.Columns.Add("Color", typeof(string));
		dt.Columns.Add("Wid", typeof(decimal));
		dt.Columns.Add("Len", typeof(decimal));
		dt.Columns.Add("Material", typeof(string));		
		dt.Columns.Add("CM", typeof(string));
		dt.Columns.Add("SB", typeof(string));
		
		return dt;
	}

	public static string getConnectionInfo()
	{
	
			//Get Program Name of Current Program
			String strProgramName = oTrans.EpiBaseForm.FormName;
			String connString = "";	
			//MessageBox.Show(strProgramName);	
			//Lookup Program Name in UD01 Adapter	

			bool recSelected = false;
			string whereClause = "Key1 = 'PROGRAM' and Key2 = '" + strProgramName + "' and Key3 = '1'";
			System.Data.DataSet dsUD01Adapter = Ice.UI.FormFunctions.SearchFunctions.listLookup(oTrans, "UD01Adapter",
				out recSelected, false, whereClause);
			if (recSelected)
			{
			try
			{
		        connString = dsUD01Adapter.Tables[0].Rows[0]["Character01"].ToString();
			}
				
			catch (Exception e)
				{
				MessageBox.Show("Unable to retrieve connection string");
				connString = "";
				}
			}
			return connString;
	
	}



	public class srtComparer : IComparer
	{
		public srtComparer()
		{}
			
		public int Compare(object x, object y)
		{
			Infragistics.Win.UltraWinGrid.UltraGridCell xCell = (Infragistics.Win.UltraWinGrid.UltraGridCell)x;
			Infragistics.Win.UltraWinGrid.UltraGridCell yCell = (Infragistics.Win.UltraWinGrid.UltraGridCell)y;
			
			return Decimal.Compare((Decimal)xCell.Row.Cells["OH Qty"].Value, (Decimal)yCell.Row.Cells["OH Qty"].Value);
		}
	}

	private static void epiButton1_Click(object sender, System.EventArgs args)
	{
		epiUltraCombo1_AfterCloseUp(null, args);
	}

	private static void epiButton2_Click(object sender, System.EventArgs args)
	{
		oTrans.PushStatusText("Obtaining Open Demand Lines...", true);
		
		if(epiUltraCombo2.Value == null || epiUltraCombo1.Value == null)
		{
			MessageBox.Show("You must first select a Plant and Warehouse before searching for Demand.", "Error");
			return;
		}
		
		DateTime startTime;
		DateTime endTime;

		//MessageBox.Show(epiCheckBox1.Checked.ToString());		
			if(chkTFOrds.Checked)
			{
				startTime = DateTime.Now;
				fillTFOrdsDemandSql();
				endTime = DateTime.Now;

			}else if(!epiCheckBox1.Checked)
			{
				startTime = DateTime.Now;
				fillJobDemandSql();
				endTime = DateTime.Now;
				//MessageBox.Show("Start: " + startTime.ToString("hh:mm:ss.ffff") + "\nEnd: " + endTime.ToString("hh:mm:ss.ffff"));
			}
			else
			{
				startTime = DateTime.Now;
				fillOrderDemand();
				endTime = DateTime.Now;
				//MessageBox.Show("Start: " + startTime.ToString("hh:mm:ss.ffff") + "\nEnd: " + endTime.ToString("hh:mm:ss.ffff"));
			}	

			
		
		hasData = true;
		
		//cmdFilterDemand_Click(sender, args);		
		
		//Reset the Selection data as a new search should clear selection criteria
		epiGroupBox3.Text = "2 Selection Data for Job";
		epiTextBox3.Text = "";
		epiTextBox4.Text = "";
		epiButton4.ReadOnly = true;
		cmdRemoveOutOfStock.ReadOnly = true;
		
		//reset all of the other datagrids
		IssueGrid.DataSource = getIssuedGrid();
		IssueGrid.DataBind();
		
		ComponentGrid.DataSource = GetComponentTable();
		ComponentGrid.DataBind();
		
		LocationGrid.DataSource = GetInventoryTable();
		LocationGrid.DataBind();
		
		oTrans.PushStatusText("",false);

	}
	private static void fillJobDemandSql()
	{
		SqlConnection dbConn = new SqlConnection(sqlConnection);			
		
		System.Text.StringBuilder sb = new System.Text.StringBuilder();

		string includeUD11 = "";
		string joinType = "";
		if(!chkSelected.Checked)
			{
			includeUD11 = "and UD11.Key1 is null ";
			joinType = "left outer join ";
			}
		else
			joinType = "inner join ";
		
		sb.Append("select distinct cast(0 as bit) as selected, " + 
					"JobHead.JobNum as [Job Number], " + 
					"JobProd.ProdQty as Qty, " + 
					"JobHead.ReqDueDate as [Start Date], " + 
					"JobProd.PartNum as Part, " + 
					"JobHead.DueDate as [Due Date], " + 
					"JobHead.ContractID as [Contract], " + 
					"isnull(OrderDtl_UD.ShortChar02,'') as Code, " + 
					"isnull(OrderDtl_UD.ShortChar03,'') as Style, " + 
					"isnull(Colors.Character01,'') as [Color], " + 
					"isnull(OrderDtl_UD.Number01,0) as Width, " + 
					"isnull(OrderDtl_UD.Number02,0) as Length, " + 
					"isnull(materials.Character01,'') as Material, " + 
					"cast(isnull(OrderRel.Ordernum,0) as varchar(10)) as [Order #], " + 
					"cast(isnull(OrderRel.OrderLine,0) as varchar(10)) as [Order Line], " + 
					"cast(isnull(OrderRel.OrderRelNum, 0) as varchar(10)) as [Order Rel], " + 
					"case when len(OrderHed.OrderComment) &amp;gt; 0 then 'Y' else 'N' end as CM, " + 
					"isnull(OrderDtl_UD.ShortChar07,'') as SB, " + 
					"isnull(OrderRel.BuyToOrder,0) as BTO, " + 
					"isnull(OrderRel.DropShip, 0) as DS, " + 
					"0 as RowID," + 
					"'' as Plant, " + 
					"'' as WarehouseCode," + 
					"cast(JobAsmbl.AssemblySeq as varchar(10)) as [AssemblySeq] " + 
					",cast(0 as bit) as OrderOnly " + 
					",cast(0 as bit) as TFOrderOnly, " + 
					"isnull(OrderDtl_UD.ShortChar05,'') as ColorCode " + 
					",isnull(OrderDtl_UD.ShortChar04,'') as MaterialCode " + 
					"from jobhead (nolock) " + 
					"inner join JobHead_UD (nolock) " + 
					"on JobHead.SysRowID = JobHead_UD.ForeignSysRowID " +
					"inner join jobasmbl (nolock) " + 
					"on jobasmbl.company = jobhead.company " + 
					"and jobasmbl.jobnum = jobhead.jobnum " + 
					"inner join jobProd (nolock) " + 
					"on JobProd.company = jobhead.company " + 
					"and jobprod.jobnum = jobhead.jobnum " + joinType +
					//"left outer join 
					" Ice.UD11 (nolock) " + 
					"on UD11.Company = Jobasmbl.company " + includeUD11 +
					"and UD11.Key2 = JObAsmbl.JObNum " + 
					"and UD11.ShortChar05 = cast(jobasmbl.assemblyseq as varchar(30)) " + 
					"and UD11.Key3 = 'Belt' " + 
					"left outer join OrderRel (nolock) " + 
					"on OrderRel.Company = JobProd.company " + 
					"and OrderRel.OrderNum = JobProd.Ordernum " + 
					"and OrderRel.OrderLine = JobProd.OrderLine " + 
					"and OrderRel.OrderRelNum = JobProd.OrderRelNum " + 
					"left outer join OrderHed (nolock) " + 
					"on OrderHed.Company = OrderRel.Company " + 
					"and OrderHed.OrderNum = OrderRel.OrderNum " + 
					"left outer join Customer (nolock) " + 
					"on customer.company = OrderHed.company " + 
					"and Customer.CustNum = OrderHed.CustNum " + 
					"left outer join OrderDtl (nolock) " + 
					"on OrderDtl.Company = OrderRel.Company " + 
					"and OrderDtl.OrderNum = OrderRel.OrderNum " + 
					"and OrderDtl.OrderLine = OrderRel.OrderLine " + 
					"left outer join OrderDtl_UD (nolock) " +
					"on OrderDtl.SysRowID = OrderDtl_UD.ForeignSysRowID " +
					"left outer join Ice.UD01 colors with(nolock) " + 
					"on colors.Key1 = 'color' " + 
					"and colors.Company = OrderDtl.Company " + 
					"and colors.Key2 = OrderDtl_UD.Shortchar05 " + 
					"left outer join Ice.UD01 materials (nolock) " + 
					"on materials.Key1 = 'material' " + 
					"and materials.company = orderdtl.company " + 
					"and materials.Key2 = OrderDtl_UD.ShortChar04 " + 
					"where ");
					if (!chkSelected.Checked)
						sb.Append("NOT EXISTS (select * from Ice.UD11 where UD11.Company = Erp.JobAsmbl.Company and UD11.Key2 = Erp.JobHead.JobNum) and ");
					sb.Append("jobhead.jobcomplete = 0 "); 
					sb.Append("and jobhead.jobclosed = 0 "); 
                    // Allow credit holds 
					//"and (isnull(Customer.CreditHold, 0)  = 0 or isnull(OrderHed.CreditOverride,1) = 1) " + 
					sb.Append("and isnull(OrderHed.ReadyToCalc,1) = 1");
		
		sb.Append(" and ((JobProd.WarehouseCode = '").Append(epiUltraCombo2.Value.ToString()).Append("' and (JobProd.Plant = '' OR JobProd.Plant = '").Append(epiUltraCombo1.Value.ToString()).Append("')) OR (OrderRel.WarehouseCode = '").Append(epiUltraCombo2.Value.ToString()).Append("' AND (OrderRel.Plant = '' or OrderRel.Plant = '").Append(epiUltraCombo1.Value.ToString()).Append("')) OR (isnull(OrderRel.WarehouseCode,'') = '' and isnull(JobProd.WarehouseCode, '') = '' AND OrderRel.Plant = '").Append(epiUltraCombo1.Value.ToString()).Append("'))");
		
		try
		{
			DateTime startDate = DateTime.Parse(epiDueDate.Value.ToString());
			DateTime endDate = DateTime.Parse(epiToDueDate.Value.ToString());
			
			sb.Append(" and JobHead.DueDate &amp;gt;= '").Append(startDate.ToString("MM/dd/yyyy")).Append("'");
			sb.Append(" and JobHead.DueDate &amp;lt;= '").Append(endDate.ToString("MM/dd/yyyy")).Append("'");
		}
		catch
		{}
		
		if(txtJobNumber.Text.Trim() != "")
			sb.Append(" and JobHead.JobNum = '").Append(txtJobNumber.Text.Trim()).Append("'");
			
		if(chkSelected.Checked)
			sb.Append(" and UD11.Key1 is not null");
		//else
		//	sb.Append(" and UD11.Key1 = '' ");
			
		if(chkBuyToOrder.Checked)
			sb.Append(" and OrderRel.BuyToOrder = 1");
		else
			sb.Append(" and isnull(OrderRel.BuyToOrder,0) = 0");
				
		if(chkOutOfStock.Checked)
			sb.Append(" and JobHead_UD.CheckBox04 = 1");
		else
			sb.Append(" and JobHead_UD.CheckBox04 = 0");
	
		if(txtStyle.Text.Trim() != "")
			sb.Append(" and OrderDtl_UD.ShortChar02 = '").Append(txtStyle.Text.Trim()).Append("'");
			

	
			if(((Ice.Core.Session)oTrans.Session).UserID.ToUpper() == "MANAGER")
				{
					txtInfo.Text = sb.ToString();
					txtInfo.Visible = true;
					/*System.IO.StreamWriter sw = new System.IO.StreamWriter(@"C:\temp\testing.txt");
					sw.WriteLine(sb.ToString());
					sw.Close();
					sw.Dispose();*/
					
				}	
	
		SqlCommand sqlCom = new SqlCommand(sb.ToString(), dbConn);
		
		SqlDataAdapter da = new SqlDataAdapter(sqlCom);
		
		DataTable dt = new DataTable();
		
		da.Fill(dt);
		
		//now that we have the values, we need to fill the RowID with the proper values
		int i = 0;
		foreach(DataRow dr in dt.Rows)
		{
			dr["RowID"] = i;
			dr["Plant"] = epiUltraCombo1.Value.ToString();
			dr["WarehouseCode"] = epiUltraCombo2.Value.ToString();
			i++;
		}
		
		DataView dv = new DataView(dt);
		
		epiUltraGrid1.DataSource = dv;
		
		da.Dispose();
		sqlCom.Dispose();
		dbConn.Dispose();
			
	}

	private static void fillOrderDemand()
	{
		SqlConnection dbConn = new SqlConnection(sqlConnection);
		
		System.Text.StringBuilder sb = new System.Text.StringBuilder();

		string includeUD11 = "";
		string joinType = "";
		if(!chkSelected.Checked)
			{
			//includeUD11 = " and UD11.Key1 is null ";
			joinType = "left outer join ";
			}
		else
			joinType = "inner join ";
		
		sb.Append("select distinct cast(0 as bit) as selected, " + 
					"cast(OrderRel.OrderNum as varchar(10)) + '-' + cast(OrderRel.OrderLine as varchar(10)) + '-' + cast(OrderRel.OrderRelNum as varchar(10)) as [Job Number], " + 
					"OrderRel.OurReqQty as Qty," + 
					"OrderRel.ReqDate as [Start Date], " + 
					"OrderRel.PartNum as Part," + 
					"OrderRel.ReqDate as [Due Date], " + 
					"OrderDtl.ContractID as [Contract], " + 
					"OrderDtl_UD.ShortChar02 as [Code], " + 
					"OrderDtl_UD.ShortChar03 as [Style], " + 
					"isnull(colors.Character01,'') as Color, " + 
					"OrderDtl_UD.Number01 as Width, " + 
					"OrderDtl_UD.Number02 as Length, " + 
					"isnull(materials.Character01,'') as [Material], " + 
					"cast(OrderRel.OrderNum as varchar(10)) as [Order #], " + 
					"cast(OrderRel.OrderLine as varchar(10)) as [Order Line], " + 
					"cast(OrderRel.OrderRelNum as varchar(10)) as [Order Rel], " + 
					"case when len(OrderHed.OrderComment) &amp;gt; 0 then 'Y' else 'N' end as CM, " + 
					"OrderDtl_UD.ShortChar07 as SB, " + 
					"OrderRel.BuyToOrder as BTO, " + 
					"OrderRel.DropShip as DS, " + 
					"0 as RowID," + 
					"'' as Plant, " + 
					"'' as WarehouseCode, " + 
					"'0' as AssemblySeq, " + 
					"cast(1 as bit) as OrderOnly, " +
					"cast(0 as bit) as TFOrderOnly, " +  
					"OrderDtl_UD.ShortChar05 as ColorCode, " + 
					"OrderDtl_UD.ShortChar04 as MaterialCode " + 
					"from OrderRel (nolock) " + 
					"inner join OrderRel_UD (nolock) " +
					"on OrderRel.SysRowID = OrderRel_UD.ForeignSysRowID " +
					"inner join OrderHed (nolock) " + 
					"on OrderHed.Company = OrderRel.Company " + 
					"and OrderHed.OrderNum = OrderRel.OrderNum " + 
					"and OrderHed.OpenOrder = 1 " + 
					"and OrderHed.ReadyToCalc = 1 " + 
					"inner join OrderDtl (nolock) " + 
					"on OrderDtl.Company = OrderRel.Company " + 
					"and OrderDtl.OrderNum = OrderRel.OrderNum " + 
					"and OrderDtl.OrderLine = OrderRel.OrderLine " + 
					"inner join OrderDtl_UD (nolock) " +
					"on OrderDtl.SysRowID = OrderDtl_UD.ForeignSysRowID " +
					"left outer join JobProd (nolock) " + 
					"on JobProd.Company = OrderRel.Company " + 
					"and JobProd.OrderNum = OrderRel.OrderNum " + 
					"and JobProd.OrderLine = OrderRel.OrderLine " + 
					"and JobProd.OrderRelNum = OrderRel.OrderRelNum " + 
					 joinType + "Ice.UD11 (nolock) " + 
					"on UD11.Key3 = 'Belt' " + includeUD11 +
					"and UD11.Company = OrderDtl.Company " + 
					"and UD11.Key1 = OrderDtl.PartNum " + 
					"and UD11.Number01 = OrderDtl.OrderNum " + 
					"and UD11.Number02 = OrderDtl.OrderLine " + 
					"and UD11.Number03 = OrderRel.OrderRelNum " + 
					"inner join Customer (nolock) " + 
					"on Customer.Company = OrderDtl.Company " + 
					"and Customer.CustNum = OrderDtl.CustNum " + 
					"left outer join Ice.UD01 materials with(nolock) " + 
					"on materials.Key1 = 'material' " + 
					"and materials.company = OrderDtl.Company " + 
					"and materials.Key2 = OrderDtl_UD.shortChar04 " + 
					"left outer join Ice.UD01 colors (nolock) " + 
					"on colors.Key1 = 'color' " + 
					"and colors.company = OrderDtl.Company " + 
					"and materials.Key2 = OrderDtl_UD.shortChar05 " + 
					"where ");
 					if (!chkSelected.Checked)
						sb.Append("NOT EXISTS (select * from Ice.UD11 where UD11.Company = Erp.OrderRel.Company and UD11.Key1 = Erp.OrderRel.PartNum and UD11.Number01 = OrderRel.OrderNum and UD11.Number02 = OrderRel.OrderLine and UD11.Number03 = OrderRel.OrderRelNum) and ");

					sb.Append("OrderRel.OpenRelease = 1 "); 
					sb.Append("and JobProd.OrderNum is null "); 
                    // BMB - allow credit holds
					//"and (Customer.CreditHold = 0 or OrderHed.CreditOverride = 1) " + 
					sb.Append("and OrderRel.PartNum not in (select PartRev.PartNum from PartRev (nolock) where PartRev.Configured = 1) "); 
					sb.Append("and OrderRel.OurReqQty &amp;gt; 0");
		
		sb.Append(" and (OrderRel.WarehouseCode = '").Append(epiUltraCombo2.Value.ToString()).Append("' and (OrderRel.Plant = '' OR OrderRel.Plant = '").Append(epiUltraCombo1.Value.ToString()).Append("'))");
						
		try
		{
			DateTime startDate = DateTime.Parse(epiDueDate.Value.ToString());
			DateTime endDate = DateTime.Parse(epiToDueDate.Value.ToString());
			
			sb.Append(" and OrderRel.ReqDate &amp;gt;= '").Append(startDate.ToString("MM/dd/yyyy")).Append("'");
			sb.Append(" and OrderRel.ReqDate &amp;lt;= '").Append(endDate.ToString("MM/dd/yyyy")).Append("'");
		}
		catch
		{}
			
		if(txtOrderNum.Text.Trim() != "")
			sb.Append(" and OrderRel.OrderNum = '").Append(txtOrderNum.Text.Trim()).Append("'");
			
		if(txtLine.Text.Trim() != "")
			sb.Append(" and OrderRel.OrderLine = '").Append(txtLine.Text.Trim()).Append("'");
			
		if(txtRelease.Text.Trim() != "")
			sb.Append(" and OrderRel.OrderRelNum = '").Append(txtRelease.Text.Trim()).Append("'");
			
		if(chkSelected.Checked)
			sb.Append(" and UD11.Key1 is not null");
		//else
			//sb.Append(" and UD11.Key1 = ''");
			
		if(chkBuyToOrder.Checked)
			sb.Append(" and OrderRel.BuyToOrder = 1");
		else
			sb.Append(" and OrderRel.BuyToOrder = 0");
			
		if(chkOutOfStock.Checked)
			sb.Append(" and OrderRel_UD.CheckBox04 = 1");
		else
			sb.Append(" and OrderRel_UD.CheckBox04 = 0");	
			
		if(txtStyle.Text.Trim() != "")
			sb.Append(" and OrderDtl_UD.ShortChar02 = '").Append(txtStyle.Text.Trim()).Append("'");
			
			if(((Ice.Core.Session)oTrans.Session).UserID.ToUpper() == "MANAGER")
				{
					txtInfo.Text = sb.ToString();
					txtInfo.Visible = true;
					/*System.IO.StreamWriter sw = new System.IO.StreamWriter(@"C:\temp\testing.txt");
					sw.WriteLine(sb.ToString());
					sw.Close();
					sw.Dispose();*/
					
				}	



		SqlCommand sqlCom = new SqlCommand(sb.ToString(), dbConn);
		
		SqlDataAdapter da = new SqlDataAdapter(sqlCom);
		
		DataTable dt = new DataTable();
		
		da.Fill(dt);
		
		//fill row/plant/warehouse data
		int i = 0;
		foreach(DataRow dr in dt.Rows)
		{
			dr["RowID"] = i;
			dr["Plant"] = epiUltraCombo1.Value.ToString();
			dr["WarehouseCode"] = epiUltraCombo2.Value.ToString();
			i++;
		}	
				
		DataView dv = new DataView(dt);
		
		epiUltraGrid1.DataSource = dv;
		
		da.Dispose();
		sqlCom.Dispose();
		dbConn.Dispose();
	}	

	private static void epiUltraGrid1_CellChange(object sender, Infragistics.Win.UltraWinGrid.CellEventArgs args)
	{
		
		//MessageBox.Show(args.Cell.Row.Cells["Job Number"].Text);
		if(args.Cell.Text == "True") //The user has selected the Job
		{
			string orderComment = "";
			string jobComment = "";	
			int OrderNum = 0;
			string jobNumber = "";
			
			//fill base information for job selection
			
			epiGroupBox3.Text = "2 Selection Data for Job " + args.Cell.Row.Cells["Job Number"].Text;
			
			epiButton4.ReadOnly = false;
			cmdRemoveOutOfStock.ReadOnly = false;
			
			int.TryParse(args.Cell.Row.Cells["Order #"].Text.Trim(), out OrderNum);
			jobNumber = args.Cell.Row.Cells["Job Number"].Text.Trim();
			
			getComments(OrderNum, jobNumber, out orderComment, jobComment);
			
			epiTextBox3.Text = orderComment;
			epiTextBox4.Text = jobComment;
			Contract =  args.Cell.Row.Cells["Contract"].Text;
			lblCrossDockInfo.Text = args.Cell.Row.Cells["Job Number"].Text + "\t" + args.Cell.Row.Cells["Order #"].Text + "\t" + args.Cell.Row.Cells["Order Line"].Text +
				"\t" + args.Cell.Row.Cells["Order Rel"].Text + "\t" + args.Cell.Row.Cells["AssemblySeq"].Text + "\t" + args.Cell.Row.Cells["OrderOnly"].Text;
		}
		else //they are unselecting the "job", so clear the values
		{
			epiGroupBox3.Text = "2 Selection Data for Job";
			epiTextBox3.Text = "";
			epiTextBox4.Text = "";
			epiButton4.ReadOnly = true;
			Contract =  "";
			cmdRemoveOutOfStock.ReadOnly = true;
		}
	}

	//obtain order and job comment information
	private static void getComments(int OrderNumber, string JobNumber, out string OrderComment, string jobComment)
	{
		OrderComment = "";
		jobComment = "";
		
		Ice.Core.Session session = (Ice.Core.Session)oTrans.Session;
		
		/*E10 replace with Query
		//Progress.Open4GL.DynamicAPI.Session progressSession = session.ConnectionPool.Get();
		Ice.Proxy.BO.GetImpl progressSession = WCFServiceSupport.CreateImpl&amp;lt;Ice.Proxy.BO.GetImpl&amp;gt;((Ice.Core.Session)oTrans.Session, Ice.Proxy.BO.GetImpl.UriPath);
		
		PersistentProc proc = default(PersistentProc);
		
		ParameterSet parms = new ParameterSet(4);
		
		parms.setIntegerParameter(1, OrderNumber, ParameterSet.INPUT);
		parms.setStringParameter(2, JobNumber, ParameterSet.INPUT);
		parms.setStringParameter(3, null, ParameterSet.OUTPUT);
		parms.setStringParameter(4, null, ParameterSet.OUTPUT);
		
		try
		{
			proc = progressSession.runPersistentProcedure(@"ud\AllocWorkbench\getCommentInfo.p", parms);
			
			OrderComment = (string)parms.getOutputParameter(3);
			jobComment = (string)parms.getOutputParameter(4);
			
			proc.delete();
		}
		catch(Exception ex)
		{
			EpiMessageBox.Show(ex.ToString(), "Comment Information");
		}
		finally
		{
			//session.ConnectionPool.Release(progressSession);
			Ice.Proxy.BO.ReleaseImpl  = WCFServiceSupport.CreateImpl&amp;lt;Ice.Proxy.BO.ReleaseImpl&amp;gt;((Ice.Core.Session)oTrans.Session, Ice.Proxy.BO.ReleaseImpl.UriPath);
		}

		*/
		using (DynamicQueryAdapter adDynamic = new DynamicQueryAdapter(UD14Form))
		{
			adDynamic.BOConnect();
			QueryExecutionDataSet parameters = new QueryExecutionDataSet();
			parameters.Tables["ExecutionParameter"].Rows.Add("JobNum",JobNumber,"Character",false,null,"A"); 
			string baq = "JobComments";
			adDynamic.ExecuteByID(baq,parameters);

	        if (adDynamic.QueryResults.Tables[0].Rows.Count &amp;gt; 0) jobComment = adDynamic.QueryResults.Tables[0].Rows[0][0].ToString();

			parameters.Clear();
			
			parameters.Tables["ExecutionParameter"].Rows.Add("OrderNum",OrderNumber,"Character",false,null,"A"); 
			baq = "OrderComments";

			adDynamic.ExecuteByID(baq,parameters);

	        if (adDynamic.QueryResults.Tables[0].Rows.Count &amp;gt; 0) OrderComment = adDynamic.QueryResults.Tables[0].Rows[0][0].ToString();

		}



	}

	private static void epiButton4_Click(object sender, System.EventArgs args)
	{
		
		oTrans.PushStatusText("Generating Cross dock...", true);
		
		//information required to create a cross dock
		string OrderNum = "";
		string OrderLine = "";
		string OrderRel = "";
		string JobNum = "";
		string AssemblySeq = "";
		string MaterialSeq = "";
		bool OrderOnly = false;
		string PartNum = "";
		
		bool TFOrderOnly = false;

		bool selected = false;
		
		Infragistics.Win.UltraWinGrid.UltraGridRow rr = null;
		
		foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in ComponentGrid.Rows)
			if(row.Cells["Sel"].Text == "True")
			{
				
				//make sure that the plant that has been selected matches the plant that they are currently logged into
				if(((Ice.Core.Session)UD14Form.Session).PlantID != row.Cells["Plant"].Text)
				{
					MessageBox.Show("The Plant that you are currently logged into is not the selected plant for this inventory line.  Unable to generate the cross dock.", "Plant Mis-match");
					return;
				}
				
				selected = true;
				OrderNum = row.Cells["OrderNum"].Text;
				OrderLine = row.Cells["OrderLine"].Text;
				OrderRel = row.Cells["OrderRel"].Text;
				JobNum = row.Cells["JobNumber"].Text;
				AssemblySeq = row.Cells["AssemblySeq"].Text;
				MaterialSeq = row.Cells["MaterialSeq"].Text;
				OrderOnly = row.Cells["OrderOnly"].Text == "True" ? true : false;
				PartNum = row.Cells["Component"].Text.Trim();
				TFOrderOnly = row.Cells["TFOrderOnly"].Text == "True" ? true : false;

				rr = row;
			}
		
		if(!selected)
		{
			MessageBox.Show("You must first selected a component before generating a cross dock.","No Component Selected");
			oTrans.PushStatusText("", false);
			return;
		}
		
		string error = "";
		string message = "";
		
		Ice.Core.Session session = (Ice.Core.Session)UD14Form.Session;


		using (var oa = Ice.Lib.Framework.WCFServiceSupport.CreateImpl &amp;lt;Erp.Proxy.BO.OrderAllocImpl&amp;gt;((Ice.Core.Session)oTrans.Session, Erp.Proxy.BO.OrderAllocImpl.UriPath)) 
		{ 

		
//		OrderAlloc oa = new OrderAlloc(session.ConnectionPool);
		
			Erp.BO.OrderAllocListDataSet dsList = new Erp.BO.OrderAllocListDataSet();
			if(TFOrderOnly)
				dsList = oa.GetList("TFOrdNum = '" + JobNum.Split('/')[0].Trim() + "' and TFOrdLine = '" + JobNum.Split('/')[1].Trim() + "' and PartNum = '" + PartNum + "'", 0, 1, out selected, session.CompanyID);
			
			else if(OrderOnly)
				dsList = oa.GetList("ordernum = '" + OrderNum + "' and orderline = '" + OrderLine + "' and OrderRelNum = '" + OrderRel + "' and PartNum = '" + PartNum + "'", 0, 1, out selected, session.CompanyID);
			else
				dsList = oa.GetList("JobNum = '" + JobNum + "' and AssemblySeq = '" + AssemblySeq + "' and PartNum = '" + PartNum + "'", 0, 1, out selected, session.CompanyID);
			
			Erp.BO.OrderAllocDataSet ds = oa.OrderAllocationGetRows(dsList, 0);
			
			foreach (Erp.BO.OrderAllocDataSet.OrderAllocRow row in ds.OrderAlloc)
				if(TFOrderOnly)
					row.SelectedForAction = true;
				else if(OrderOnly)
					row.SelectedForAction = true;
				else if(row.MtlSeq.ToString() == MaterialSeq &amp;amp;&amp;amp; row.AssemblySeq.ToString() == AssemblySeq)
				{				
					row.SelectedForAction = true;
				}
			
	
			oa.OneDemandType(ds, out error, out message);
			
			//MessageBox.Show(message);//TEST
			//MessageBox.Show(ds.PartAllocTran.Rows.Count.ToString());
	
			foreach (Erp.BO.OrderAllocDataSet.PartAllocTranRow row in ds.PartAllocTran)
			{
				row.WarehouseCode = epiUltraCombo2.Text.ToString();
				row.CrossDocking = true;
				row.AddHoc = true;
			}
			bool released;
			oa.AutoAllocation(ds, "", "All", out message, out released);
			//MessageBox.Show(message);//TEST
			decimal ASRSID = 0;
			
			decimal total;
			
			//Once Auto allocation is done, find all of the allocated lines, so that we can create the UD11 Records	
			foreach(DataRow dr in getAllocForGen(rr).Rows)
			{
				ASRSID = 0;
				
				//see if the warehouse/bin is ASRS
				if (dr["PartAlloc.WarehouseCode"].ToString() == "ASRS" || dr["PartAlloc.BinNum"].ToString() == "ASRS")
					ASRSID = CreateASRSRequest(rr, dr);
				
					CreateNewUD11(rr, dr, ASRSID);
			}

			//MT		
			foreach(DataRow dr in getAllocRowsForTFOrder(rr).Rows)
			{  
				CreateNewTFOrderUD11(rr, dr,ASRSID);
			}
		}		
		//update the order/job for cross dock
		UpdateForCrossDock(OrderOnly, int.Parse(OrderNum), int.Parse(OrderLine), int.Parse(OrderRel), JobNum, TFOrderOnly);
		
		//now re-fill component
		cmdComponents_Click(null, null);
		
		//now update the order detail/job header as required	
		MessageBox.Show("Crossdock Generated!", "Success!");
		
		oTrans.PushStatusText("",false);
	}

	private static void UpdateForCrossDock(bool OrderOnly, int OrderNum, int OrderLine, int OrderRelNum, string JobNum, bool TFOrderOnly = false)
	{
		if(TFOrderOnly)
		{
			return;
			//MessageBox.Show("UpdateForCrossDock To Transfer Orders", "Success!");
		}
		else if(OrderOnly)
		{
			
			SalesOrderAdapter ad = new SalesOrderAdapter(UD14Form);
			ad.BOConnect();
			
			ad.GetByID(OrderNum);
			
			foreach(Erp.BO.SalesOrderDataSet.OrderRelRow r in ad.SalesOrderData.OrderRel.Rows)			
				if(r.OrderLine == OrderLine &amp;amp;&amp;amp; r.OrderRelNum == OrderRelNum)
				{
					r.Make = false;
					r["CheckBox04"] = true;
					r["CheckBox05"] = true;
				}
			
			ad.Update();
			
			ad.Dispose();
		}
		else //this is a job, we only need to update the job header
		{
			JobEntryAdapter ad = new JobEntryAdapter(UD14Form);
			ad.BOConnect();
			
			ad.GetByID(JobNum);
			
			//set the engineered flag to false, this will also set the released to false as well.
			ad.JobEntryData.JobHead[0].JobEngineered = false;
			
			ad.ChangeJobHeadJobEngineered();		
			
			ad.JobEntryData.JobHead[0]["CheckBox04"] = true;
			ad.Update();
			
			ad.Dispose();
		}
	}

	private static DataTable getAllocForGen(Infragistics.Win.UltraWinGrid.UltraGridRow row)
	{
		using (DynamicQueryAdapter dy = new DynamicQueryAdapter(UD14Form))
		{		
			dy.BOConnect();
			QueryExecutionDataSet parameters = new QueryExecutionDataSet();
			//parameters.Tables["ExecutionParameter"].Rows.Add("JobNum",JobNumber,"Character",false,null,"A"); 


//			QueryDesignDataSet data = dy.GetByID("GetAllocLines");
			//dy.GetByID("GetAllocLinesE10");
			bool OrderOnly = row.Cells["OrderOnly"].Text == "True" ? true : false;

			if (!OrderOnly) 
			{
				parameters.Tables["ExecutionParameter"].Rows.Add("JobNum",row.Cells["JobNumber"].Text,"Character",false,null,"A");
				parameters.Tables["ExecutionParameter"].Rows.Add("AssemblySeq",row.Cells["AssemblySeq"].Text,"Integer",false,null,"A");
				parameters.Tables["ExecutionParameter"].Rows.Add("MtlSeq",row.Cells["MaterialSeq"].Text,"Integer",false,null,"A");
			}
			else
			{
				parameters.Tables["ExecutionParameter"].Rows.Add("OrderNum",row.Cells["OrderNum"].Text,"Integer",false,null,"A");
				parameters.Tables["ExecutionParameter"].Rows.Add("OrderLine",row.Cells["OrderLine"].Text,"Integer",false,null,"A");
				parameters.Tables["ExecutionParameter"].Rows.Add("OrderRelNum",row.Cells["OrderRel"].Text,"Integer",false,null,"A");
			}
			
			/*e10 foreach (DynamicQueryDataSet.QueryWhereItemRow r in dy.QueryDesignData.QueryWhereItem)
			{
				if(r.FieldName.ToUpper() == "JOBNUM")
				{
					if(!OrderOnly)
					{
						r.CompOp = "=";
						r.RValue = row.Cells["JobNumber"].Text;
					}
					else
					{
						r.CompOp = "MATCHES";
						r.RValue = "*";
					}
				}
				else if(r.FieldName.ToUpper() == "ASSMEBLYSEQ")
				{
					if(!OrderOnly)
					{
						r.CompOp = "=";
						r.RValue = row.Cells["AssemblySeq"].Text;
					}
					else
					{
						r.CompOp = "=";
						r.RValue = "0";
					}
				}
				else if(r.FieldName.ToUpper() == "MTLSEQ")
				{
					if(!OrderOnly)
					{
						r.CompOp = "=";
						r.RValue = row.Cells["MaterialSeq"].Text;
					}
					else
					{
						r.CompOp = "=";
						r.RValue = "0";
					}
				}
				else if(r.FieldName.ToUpper() == "ORDERNUM")
				{
					if(OrderOnly)
					{
						r.CompOp = "=";					
						r.RValue = row.Cells["OrderNum"].Text;
					}
					else
					{
						r.CompOp = "&amp;gt;=";
						r.RValue = "0";
					}
				}
				else if(r.FieldName.ToUpper() == "ORDERLINE")
				{
					if(OrderOnly)
					{
						r.CompOp = "=";
						r.RValue = row.Cells["OrderLine"].Text;
					}
					else
					{
						r.CompOp = "&amp;gt;=";
						r.RValue = "0";
					}
				}
				else if(r.FieldName.ToUpper() == "ORDERRELNUM")
				{
					if(OrderOnly)
					{
						r.CompOp = "=";
						r.RValue = row.Cells["OrderRel"].Text;
					}
					else
					{
						r.CompOp = "&amp;gt;=";
						r.RValue = "0";
					}
				}
			}		
			
			DataSet ds = dy.Execute(data);
			
			DataView dv = new DataView(ds.Tables[0]);	*/
			string baq = "GetAllocLinesE10";			
			dy.ExecuteByID(baq,parameters);
			DataView dv = new DataView(dy.QueryResults.Tables[0]);
			if(!OrderOnly)
			{
				dv.RowFilter = "PartAlloc_MtlSeq = '" + row.Cells["MaterialSeq"].Text + "' and PartAlloc_AssemblySeq = '" + row.Cells["AssemblySeq"].Text + "'";
			}
			
			return dv.ToTable();		
		}
	}

	private static void cmdComponents_Click(object sender, System.EventArgs args)
	{
		oTrans.PushStatusText("Obtaining Component Information...", true);
		
		JobEntryDataSet jobDS = new JobEntryDataSet();
		int RowID = 0;
		
		int OrderNum = 0;
		int OrderLine = 0;
		int OrderRel = 0;
		
		DataTable CompTable = GetComponentTable();
		
		DataTable sendData = new DataTable();
		
		sendData.Columns.Add("JobNumber", typeof(string));
		sendData.Columns.Add("AssemblySeq", typeof(int));
		sendData.Columns.Add("OrderNum", typeof(int));
		sendData.Columns.Add("OrderLine", typeof(int));
		sendData.Columns.Add("OrderRelNum", typeof(int));
		sendData.Columns.Add("OrderOnly", typeof(bool));
		sendData.Columns.Add("TFOrderOnly", typeof(bool));
		sendData.Columns.Add("Plant", typeof(string));
		sendData.Columns.Add("Warehouse", typeof(string));
		sendData.Columns.Add("PartNum", typeof(string));
		sendData.Columns.Add("Code", typeof(string));
		sendData.Columns.Add("Style", typeof(string));
		sendData.Columns.Add("ColorCode", typeof(string));
		sendData.Columns.Add("MaterialCode", typeof(string));
		sendData.Columns.Add("Qty", typeof(decimal));
		sendData.Columns.Add("dueDate", typeof(DateTime));
		
		DataRow newRow;
		
		//get each selected row from the first UltraGrid
		foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in epiUltraGrid1.Rows)		
			if(row.Cells["Selected"].Text.Trim() == "True")
			{
/*				newRow = sendData.NewRow();
			
				newRow["JobNumber"] = row.Cells["Job number"].Text.Trim();
				newRow["AssemblySeq"] = int.Parse(row.Cells["AssemblySeq"].Text.Trim());
				newRow["orderNum"] = int.Parse(row.Cells["Order #"].Text.Trim());
				newRow["OrderLine"] = int.Parse(row.Cells["Order Line"].Text.Trim());
				newRow["OrderRelNum"] = int.Parse(row.Cells["Order Rel"].Text.Trim());
				newRow["OrderOnly"] = row.Cells["OrderOnly"].Text == "False" ? false : true;
				newRow["Plant"] = row.Cells["Plant"].Text.Trim();
				newRow["Warehouse"] = row.Cells["WarehouseCode"].Text.Trim();
				newRow["PartNum"] = row.Cells["Part"].Text.Trim();
				newRow["Code"] = row.Cells["Code"].Text.Trim();
				newRow["Style"] = row.Cells["Style"].Text.Trim();
				newRow["ColorCode"] = row.Cells["ColorCode"].Text.Trim();
				newRow["MaterialCode"] =row.Cells["MaterialCode"].Text.Trim();
				newRow["qty"] = decimal.Parse(row.Cells["Qty"].Text.Trim());
				try{newRow["DueDate"] = DateTime.Parse(row.Cells["Due Date"].Text.Trim());}
				catch{}
				finally{}
				sendData.Rows.Add(newRow);*/

				string pJobNumber = row.Cells["Job number"].Text.Trim();
				int pAssemblySeq = int.Parse(row.Cells["AssemblySeq"].Text.Trim());
				int pOrderNum = int.Parse(row.Cells["Order #"].Text.Trim());
				int pOrderLine = int.Parse(row.Cells["Order Line"].Text.Trim());
				int pOrderRelNum = int.Parse(row.Cells["Order Rel"].Text.Trim());
				//bool pOrderOnly = row.Cells["OrderOnly"].Text == "False" ? false : true;
				bool pOrderOnly = (bool)row.Cells["OrderOnly"].Value;
				string pPlant = row.Cells["Plant"].Text.Trim();
				string pWarehouse = row.Cells["WarehouseCode"].Text.Trim();
				string pPartNum = row.Cells["Part"].Text.Trim();
				string pCode = row.Cells["Code"].Text.Trim();
				string pStyle = row.Cells["Style"].Text.Trim();
				string pColorCode = row.Cells["ColorCode"].Text.Trim();
				string pMaterialCode =row.Cells["MaterialCode"].Text.Trim();
				decimal pQty = decimal.Parse(row.Cells["Qty"].Text.Trim());

				DateTime pDueDate = DateTime.Today;
				string pCODE = "";
				try
				{
					pDueDate = DateTime.Parse(row.Cells["Due Date"].Text.Trim());
				}
				catch{}
				finally{}

		
				using (DynamicQueryAdapter dy = new DynamicQueryAdapter(UD14Form))
				{		
					dy.BOConnect();
					QueryExecutionDataSet parameters = new QueryExecutionDataSet();
					//parameters.Tables["ExecutionParameter"].Rows.Add("JobNum",JobNumber,"Character",false,null,"A"); 
		
		
		//			QueryDesignDataSet data = dy.GetByID("GetAllocLines");
					//dy.GetByID("GetAllocLinesE10");
//					bool OrderOnly = row.Cells["OrderOnly"].Text == "True" ? true : false;
					bool OrderOnly = (bool)row.Cells["OrderOnly"].Value;
					bool TFOrderOnly = (bool)row.Cells["TFOrderOnly"].Value; //MT
					parameters.Clear();

					string baq = "";
					int rowCount = 0;
					if (TFOrderOnly) 
					{
						 
						
						decimal qty = GetAccumTFOrder(pPartNum, pJobNumber, TFOrderOnly);
						 
						DataRow newComp = CompTable.NewRow();

						newComp["Sel"] = false;
						newComp["ROWID"] = rowCount;
						newComp["Component"] = pPartNum;
						newComp["Qty"] = pQty;
						newComp["JobNumber"] = pJobNumber ;
						newComp["OrderNum"] = pOrderNum;
						newComp["OrderLine"] = pOrderLine;
						newComp["OrderRel"] = pOrderRelNum;
						newComp["MaterialSeq"] = 0;
						newComp["AssemblySeq"] = 0;
						newComp["Plant"] = pPlant;
						newComp["WarehouseCode"] = pWarehouse;
						newComp["PartNum"] = pPartNum;
						newComp["OrderOnly"] = false;
						newComp["TFOrderOnly"] = true;
						newComp["Style"] = pStyle;
						newComp["Color"] = pColorCode;
						newComp["Material"] = pMaterialCode;
						newComp["DueDate"] = pDueDate;
						newComp["CODE"] = pCODE;
						newComp["Accum"] = qty;
						newComp["TempAccum"] = 0;

						CompTable.Rows.Add(newComp);
						rowCount++;

					}else if (!OrderOnly) 
					{
						parameters.Tables["ExecutionParameter"].Rows.Add("JobNum",pJobNumber,"Character",false,null,"A");
						parameters.Tables["ExecutionParameter"].Rows.Add("AssemblySeq",pAssemblySeq,"Integer",false,null,"A");

						baq = "AllocJobMtl";
						dy.ExecuteByID(baq,parameters);
		
						foreach (DataRow dr in dy.QueryResults.Tables[0].Rows)
						{
							
							decimal qty = GetAccum(dr["JobMtl_PartNum"].ToString(), dr["JobMtl_JobNum"].ToString(), dr["JobMtl_AssemblySeq"].ToString(), dr["JobMtl_MtlSeq"].ToString(), pOrderNum, pOrderLine, pOrderRelNum, OrderOnly);

							if (dr["JobMtl_Plant"].ToString() != "SUW" || dr["JobMtl_WarehouseCode"].ToString() != "01PSTK") 
							{
								if (qty &amp;gt; Convert.ToDecimal(dr["JobMtl_RequiredQty"])) qty = Convert.ToDecimal(dr["JobMtl_RequiredQty"]);
							}

							DataRow newComp = CompTable.NewRow();

							newComp["Sel"] = false;
							newComp["ROWID"] = rowCount;
							newComp["Component"] = dr["JobMtl_PartNum"];
							newComp["Qty"] = dr["JobMtl_RequiredQty"];;
							newComp["JobNumber"] = dr["JobMtl_JobNum"];
							newComp["OrderNum"] = pOrderNum;
							newComp["OrderLine"] = pOrderLine;
							newComp["OrderRel"] = pOrderRelNum;
							newComp["MaterialSeq"] = dr["JobMtl_MtlSeq"];;
							newComp["AssemblySeq"] = dr["JobMtl_AssemblySeq"];;
							newComp["Plant"] = pPlant;
							newComp["WarehouseCode"] = pWarehouse;
							newComp["PartNum"] = pPartNum;
							newComp["OrderOnly"] = false;
							newComp["TFOrderOnly"] = false;
							newComp["Style"] = pStyle;
							newComp["Color"] = pColorCode;
							newComp["Material"] = pMaterialCode;
							newComp["DueDate"] = pDueDate;
							newComp["CODE"] = pCODE;
							newComp["Accum"] = qty;
							newComp["TempAccum"] = 0;

							CompTable.Rows.Add(newComp);
							rowCount++;
						}
					}
					else
					{

						//MessageBox.Show("jLP - performing order version of get components");	
						decimal qty = GetAccum(pPartNum, "", "0", "0",pOrderNum, pOrderLine, pOrderRelNum, OrderOnly);

							if (pPlant != "SUW" || pWarehouse != "01PSTK") 
							{
								if (qty &amp;gt; pQty) qty = pQty;
							}

							DataRow newComp = CompTable.NewRow();



							newComp["Sel"] = false;
							newComp["ROWID"] = rowCount;
							newComp["Component"] = pPartNum;
							newComp["Qty"] = pQty;
							newComp["JobNumber"] = pOrderNum.ToString() + "-" + pOrderLine.ToString() + "-" + pOrderRelNum.ToString() ;
							newComp["OrderNum"] = pOrderNum;
							newComp["OrderLine"] = pOrderLine;
							newComp["OrderRel"] = pOrderRelNum;
							newComp["MaterialSeq"] = 0;
							newComp["AssemblySeq"] = 0;
							newComp["Plant"] = pPlant;
							newComp["WarehouseCode"] = pWarehouse;
							newComp["PartNum"] = pPartNum;
							newComp["OrderOnly"] = true;
							newComp["TFOrderOnly"] = false;
							newComp["Style"] = pStyle;
							newComp["Color"] = pColorCode;
							newComp["Material"] = pMaterialCode;
							newComp["DueDate"] = pDueDate;
							newComp["CODE"] = pCODE;
							newComp["Accum"] = qty;
							newComp["TempAccum"] = 0;

							CompTable.Rows.Add(newComp);
							rowCount++;

					}
				}


			}	
		
		
		Ice.Core.Session session = (Ice.Core.Session)oTrans.Session;
		
		/*E10
		//Progress.Open4GL.DynamicAPI.Session progressSession = session.ConnectionPool.Get();
		Ice.Proxy.BO.GetImpl progressSession = WCFServiceSupport.CreateImpl&amp;lt;Ice.Proxy.BO.GetImpl&amp;gt;((Ice.Core.Session)oTrans.Session, Ice.Proxy.BO.GetImpl.UriPath);
		
		PersistentProc proc = default(PersistentProc);
		
		ParameterSet parms = new ParameterSet(2);
		
		parms.setDataTableParameter(1, CompTable, ParameterSet.INPUT_OUTPUT);
		parms.setDataTableParameter(2, sendData, ParameterSet.INPUT_OUTPUT);
		
		try
		{
			proc = progressSession.runPersistentProcedure(@"ud\AllocWorkbench\getComponents.p", parms);
			
			CompTable = (DataTable)parms.getOutputParameter(1);		
			
			proc.delete();
		}
		catch(Exception ex)
		{
			EpiMessageBox.Show(ex.ToString(), "Obtaining Components");
		}
		finally
		{
			//session.ConnectionPool.Release(progressSession);
			Ice.Proxy.BO.ReleaseImpl  = WCFServiceSupport.CreateImpl&amp;lt;Ice.Proxy.BO.ReleaseImpl&amp;gt;((Ice.Core.Session)oTrans.Session, Ice.Proxy.BO.ReleaseImpl.UriPath);
		}

    DEFINE TEMP-TABLE ttComp
        FIELD Sel AS LOGICAL
        FIELD Component AS CHARACTER
        FIELD Qty AS DECIMAL
        FIELD Accumulation AS DECIMAL
        FIELD TempAccum AS DECIMAL
        FIELD JobNumber AS CHARACTER
        FIELD OrderNum AS INTEGER
        FIELD OrderRel AS INTEGER
        FIELD OrderLine AS INTEGER
        FIELD AssemblySeq AS INTEGER
        FIELD MaterialSeq AS INTEGER
        FIELD ROWID AS INTEGER
        FIELD Plant AS CHARACTER
        FIELD WarehouseCode AS CHARACTER
        FIELD PartNum AS CHARACTER
        FIELD OrderOnly AS LOGICAL
        FIELD CODE AS CHARACTER
        FIELD Style AS CHARACTER
        FIELD ColorCode AS CHARACTER
        FIELD MaterialCode AS CHARACTER
        FIELD DueDate AS DATE.

		*/



		ComponentGrid.DataSource = CompTable;
		ComponentGrid.DataBind();
		
		//this will be needed for filtering later, so we don't have to do the whole thing over again
		FormCompTable = CompTable.Copy();
		hasCompData = true;
		
		//clear inventory grid
		LocationGrid.DataSource = GetInventoryTable();
		LocationGrid.DataBind();
		
		oTrans.PushStatusText("",false);
	}

	private static decimal GetAccum (string PartNumber, string JobNumber, string AssemblySequence, string MaterialSequence, int OrderNum, int OrderLine, int OrderRel, bool orderOnly)
	{
	    decimal AccumQty = 0;


	

		using (DynamicQueryAdapter dy = new DynamicQueryAdapter(UD14Form))
		{		
			dy.BOConnect();
			QueryExecutionDataSet parameters = new QueryExecutionDataSet();
			string baq = "";
			
			if (orderOnly)
			{
				parameters.Tables["ExecutionParameter"].Rows.Add("OrderNum",OrderNum,"Integer",false,null,"A");
				parameters.Tables["ExecutionParameter"].Rows.Add("OrderLine",OrderLine,"Integer",false,null,"A");
				parameters.Tables["ExecutionParameter"].Rows.Add("OrderRel",OrderRel,"Integer",false,null,"A");
				baq = "AllocPartAlloc";
				dy.ExecuteByID(baq,parameters);

				foreach (DataRow dr in dy.QueryResults.Tables[0].Rows)
				{
					AccumQty = AccumQty + Convert.ToDecimal(dr["Calculated_Qty"]);
				}
//                FOR EACH partAlloc WHERE partalloc.company = cur-comp AND partAlloc.OrderNum = OrderNum AND partAlloc.OrderLine = OrderLine AND partAlloc.OrderRelNum = OrderRel NO-LOCK.
//                    AccumQty = AccumQty + partAlloc.AllocatedQty + partAlloc.ReservedQty + partAlloc.PickingQty + partAlloc.PickedQty.
//                END.
			}
			else
			{
				//MessageBox.Show("Get accum not only order.   calling allocUD11 with: " + JobNumber + "Part " + PartNumber + "AssmSeq " + AssemblySequence + "MtlSeq: " + MaterialSequence + "Order: " + OrderNum.ToString() + "-" + OrderLine.ToString() + "-" + OrderRel.ToString());
				baq = "AllocUD11";
				parameters.Tables["ExecutionParameter"].Rows.Add("JobNum",JobNumber,"Character",false,null,"A");
				parameters.Tables["ExecutionParameter"].Rows.Add("PartNum",PartNumber,"Character",false,null,"A");
				parameters.Tables["ExecutionParameter"].Rows.Add("AssemblySeq",AssemblySequence,"Character",false,null,"A");
				parameters.Tables["ExecutionParameter"].Rows.Add("MaterialSeq",MaterialSequence,"Character",false,null,"A");
				parameters.Tables["ExecutionParameter"].Rows.Add("OrderNum",OrderNum,"Integer",false,null,"A");
				parameters.Tables["ExecutionParameter"].Rows.Add("OrderLine",OrderLine,"Integer",false,null,"A");
				parameters.Tables["ExecutionParameter"].Rows.Add("OrderRel",OrderRel,"Integer",false,null,"A");
				dy.ExecuteByID(baq,parameters);

				foreach (DataRow dr in dy.QueryResults.Tables[0].Rows)
				{
					AccumQty = AccumQty + Convert.ToDecimal(dr["UD11_Number05"]);
				}


//                FOR EACH UD11 WHERE UD11.Company = cur-comp AND UD11.Key1 = PartNumber AND UD11.Key2 = JobNumber AND UD11.Key3 = 'Belt' AND UD11.ShortChar05 = STRING(AssemblySequence) AND UD11.ShortChar06 = STRING(MaterialSequence) AND UD11.Number02 = OrderLine AND UD11.Number01 = OrderNum AND UD11.Number03 = OrderRel NO-LOCK.
//                    AccumQty = AccumQty + UD11.Number05.
//                END.
			}
		}
		return AccumQty;
	}


	private static decimal CreateASRSRequest(Infragistics.Win.UltraWinGrid.UltraGridRow ComponentRow, DataRow InventoryRow)
	{
/*E10
		CompanyAdapter ca = new CompanyAdapter(UD14Form);
		ca.BOConnect();
		
		ca.GetByID(((Ice.Core.Session)UD14Form.Session).CompanyID);
		
		string connStr = ca.CompanyData.Company[0].Character01;
		
		ca.Dispose();
*/
		string connStr = "";
		using (DynamicQueryAdapter adDynamic = new DynamicQueryAdapter(UD14Form))
		{
			adDynamic.BOConnect();
			adDynamic.ExecuteByID("CompanyControls");    

	        connStr = adDynamic.QueryResults.Tables[0].Rows[0]["Company_Character01"].ToString();

		}
		
		System.Data.SqlClient.SqlConnection dbConn = new System.Data.SqlClient.SqlConnection(connStr);
		dbConn.Open();
		
		string c_InsertASRSRecord = "INSERT INTO FKITXWO (FTTYPE, FTTEXT) VALUES ('RR', '{1}')";
		
		System.Text.StringBuilder sb = new System.Text.StringBuilder();
		
		if(ComponentRow.Cells["OrderOnly"].Text == "True")
			sb.Append(ComponentRow.Cells["OrderNum"].Text);
		else
			sb.Append(ComponentRow.Cells["JobNumber"].Text);
			
		sb.Append("|");
		
		//Lot
		sb.Append(InventoryRow["PartAlloc.LotNum"].ToString()).Append("|");
		
		//PartNum
		sb.Append(InventoryRow["PartAlloc.PartNum"].ToString()).Append("|");
		
		decimal qty = 0;
		
		qty = decimal.Parse(InventoryRow["PartAlloc.AllocatedQty"].ToString()) + decimal.Parse(InventoryRow["PartAlloc.PickingQty"].ToString()) + decimal.Parse(InventoryRow["PartAlloc.PickedQty"].ToString()) + decimal.Parse(InventoryRow["PartAlloc.ReservedQty"].ToString());
		
		//Quantity
		sb.Append(qty.ToString()).Append("|");
		
		//Priority
		sb.Append("5|");
		
		//Audit
		sb.Append("0|");	
		
		//Due Date
		try
		{
			sb.Append(DateTime.Parse(ComponentRow.Cells["DueDate"].Text.Trim()).ToString("yyyyMMdd")).Append("|");
		}
		catch
		{
			sb.Append(DateTime.Now.ToString("yyyyMMdd")).Append("|");
		}
		
		//destination (per Matt and Brad, this is not required for these types of movement requests)
		sb.Append("").Append("|");
		
		string commandText = "INSERT INTO FKITXWO (FTTYPE, FTTEXT) VALUES ('RR', '" + sb.ToString() + "')";
		
		//string commandText = string.Format(c_InsertASRSRecord, sb.ToString());
		
		try
		{
			System.Data.SqlClient.SqlCommand sqlCom = new System.Data.SqlClient.SqlCommand(commandText, dbConn);
			
			sqlCom.ExecuteNonQuery();
		}
		catch(Exception ex)
		{
			MessageBox.Show(ex.ToString());
		}
		
		dbConn.Close();
		dbConn.Dispose();
		
		return 1;
	}
	
	private static decimal CreateASRSRequest(Infragistics.Win.UltraWinGrid.UltraGridRow ComponentRow, Infragistics.Win.UltraWinGrid.UltraGridRow InventoryRow)
	{
/*E10
		CompanyAdapter ca = new CompanyAdapter(UD14Form);
		ca.BOConnect();
		
		ca.GetByID(((Ice.Core.Session)UD14Form.Session).CompanyID);
		
		string connStr = ca.CompanyData.Company[0].Character01;
		
		ca.Dispose();
*/
		string connStr = "";
		using (DynamicQueryAdapter adDynamic = new DynamicQueryAdapter(UD14Form))
		{
			adDynamic.BOConnect();
			adDynamic.ExecuteByID("CompanyControls");    

	        connStr = adDynamic.QueryResults.Tables[0].Rows[0]["Company_Character01"].ToString();

		}
		
		System.Data.SqlClient.SqlConnection dbConn = new System.Data.SqlClient.SqlConnection(connStr);
		dbConn.Open();
		
		string c_InsertASRSRecord = "INSERT INTO FKITXWO (FTTYPE, FTTEXT) VALUES ('RR','{1}')";
		
		System.Text.StringBuilder sb = new System.Text.StringBuilder();
		
		//build transaction text from the rows that were given
		if(ComponentRow.Cells["TFOrderOnly"].Text == "True")
			sb.Append(ComponentRow.Cells["JobNumber"].Text.ToString().Split('/')[0].Trim()); //MT TFOrderNum
		else if(ComponentRow.Cells["OrderOnly"].Text == "True")
			sb.Append(ComponentRow.Cells["OrderNum"].Text);
		else
			sb.Append(ComponentRow.Cells["JobNumber"].Text);
		
		sb.Append("|");
		
		//Lot
		sb.Append(InventoryRow.Cells["Lot"].Text).Append("|");
		
		//PartNum
		sb.Append(ComponentRow.Cells["Component"].Text).Append("|");
		
		//Quantity
		sb.Append(InventoryRow.Cells["OH Qty"].Text).Append("|");
		
		//Priority
		sb.Append("5|");
		
		//Audit
		sb.Append("0|");
		
		//Due Date
		sb.Append(ComponentRow.Cells["DueDate"].Text).Append("|");
		
		string commandText = "INSERT INTO FKITXWO (FTTYPE, FTTEXT) VALUES ('RR', '" + sb.ToString() + "')";
		
		//string commandText = string.Format(c_InsertASRSRecord, sb.ToString());
		
		try
		{
			System.Data.SqlClient.SqlCommand sqlCom = new System.Data.SqlClient.SqlCommand(commandText, dbConn);
			
			sqlCom.ExecuteNonQuery();
		}
		catch(Exception ex)
		{
			MessageBox.Show(ex.ToString());
		}
		
		dbConn.Close();
		dbConn.Dispose();
		
		return 1;
	}

	private static void CreateNewUD11(Infragistics.Win.UltraWinGrid.UltraGridRow ComponentRow, DataRow InventoryRow, decimal ASRSTranID)
	{
		/*E10
		
		//Progress.Open4GL.DynamicAPI.Session progressSession = session.ConnectionPool.Get();
		Ice.Proxy.BO.GetImpl progressSession = WCFServiceSupport.CreateImpl&amp;lt;Ice.Proxy.BO.GetImpl&amp;gt;((Ice.Core.Session)oTrans.Session, Ice.Proxy.BO.GetImpl.UriPath);
		
		PersistentProc proc = default(PersistentProc);
		
		ParameterSet parms = new ParameterSet(15);
		
		parms.setStringParameter(1, ComponentRow.Cells["Component"].Text.Trim(), ParameterSet.INPUT);
		parms.setStringParameter(2, ComponentRow.Cells["JobNumber"].Text.Trim(), ParameterSet.INPUT);
		parms.setStringParameter(3, InventoryRow["PartAlloc.WarehouseCode"].ToString(), ParameterSet.INPUT);
		parms.setIntegerParameter(4, (int)ComponentRow.Cells["OrderNum"].Value, ParameterSet.INPUT);
		parms.setIntegerParameter(5, (int)ComponentRow.Cells["OrderLine"].Value, ParameterSet.INPUT);
		parms.setIntegerParameter(6, (int)ComponentRow.Cells["OrderRel"].Value, ParameterSet.INPUT);
		parms.setDecimalParameter(7, (decimal)ComponentRow.Cells["Qty"].Value, ParameterSet.INPUT);
		parms.setDecimalParameter(8, (decimal)InventoryRow["PartAlloc.AllocatedQty"] + (decimal)InventoryRow["PartAlloc.PickingQty"] + (decimal)InventoryRow["PartAlloc.PickedQty"] + (decimal)InventoryRow["PartAlloc.ReservedQty"], ParameterSet.INPUT);
		parms.setDecimalParameter(9, ASRSTranID, ParameterSet.INPUT);
		parms.setStringParameter(10, InventoryRow["PartAlloc.BinNum"].ToString(), ParameterSet.INPUT);
		parms.setStringParameter(11, InventoryRow["PartAlloc.LotNum"].ToString(), ParameterSet.INPUT);
		parms.setStringParameter(12, InventoryRow["PartAlloc.AssemblySeq"].ToString(), ParameterSet.INPUT);
		parms.setStringParameter(13, InventoryRow["PartAlloc.MtlSeq"].ToString(), ParameterSet.INPUT);
		parms.setBooleanParameter(14, (bool)ComponentRow.Cells["OrderOnly"].Value, ParameterSet.INPUT);
		parms.setStringParameter(15, session.UserID, ParameterSet.INPUT);
		
		try
		{
			proc = progressSession.runPersistentProcedure(@"ud\AllocWorkbench\createUD11.p", parms);
			
			proc.delete();
		}
		catch(Exception ex)
		{
			EpiMessageBox.Show(ex.ToString(), "Writing UD11 Record");
		}
		finally
		{
			//session.ConnectionPool.Release(progressSession);
			Ice.Proxy.BO.ReleaseImpl  = WCFServiceSupport.CreateImpl&amp;lt;Ice.Proxy.BO.ReleaseImpl&amp;gt;((Ice.Core.Session)oTrans.Session, Ice.Proxy.BO.ReleaseImpl.UriPath);
		}		
		*/

		//Need to use SQl to update Number02, as it's not in the Adapter.

		Ice.Core.Session session = (Ice.Core.Session)oTrans.Session;

		SqlConnection dbConn = new SqlConnection(sqlConnection);
		dbConn.Open();
		//jlpchanged to use company_UD instead of just Number02

		SqlCommand sqlCom = new SqlCommand("select distinct Company_UD.Number02 as Number02, Company.SysRowID as SysRowID from Company inner join Company_UD on Company.SysRowID = Company_UD.ForeignSysRowID where Company.Company = '" + session.CompanyID + "'", dbConn);
		
		SqlDataAdapter da = new SqlDataAdapter(sqlCom);
		
		DataTable dt = new DataTable();
		
		da.Fill(dt);

/*		CompanyAdapter adpComp = new CompanyAdapter(UD14Form);
		adpComp.BOConnect();

		adpComp.GetByID(session.CompanyID);*/

		int nextUD11 = Convert.ToInt32(dt.Rows[0][0]);
		string companyRowID = dt.Rows[0][1].ToString();
		//MessageBox.Show("JLP - about to create UD11 record");
//		int nextUD11 =  Convert.ToInt32(adpComp.CompanyData.Company.Rows[0]["Number02"]);

		string pComponent =  ComponentRow.Cells["Component"].Text.Trim();
		string pJobNumber = ComponentRow.Cells["JobNumber"].Text.Trim();
		string pWarehouse = InventoryRow["PartAlloc_WarehouseCode"].ToString();
		int pOrderNum =  (int)ComponentRow.Cells["OrderNum"].Value;
		int pOrderLine = (int)ComponentRow.Cells["OrderLine"].Value;
		int pOrderRel = (int)ComponentRow.Cells["OrderRel"].Value;
		decimal pQty = (decimal)ComponentRow.Cells["Qty"].Value;
		decimal pAllocatedQty =  (decimal)InventoryRow["PartAlloc_AllocatedQty"] + (decimal)InventoryRow["PartAlloc_PickingQty"] + (decimal)InventoryRow["PartAlloc_PickedQty"] + (decimal)InventoryRow["PartAlloc_ReservedQty"];
		decimal pASRSTranID =  ASRSTranID;
		string pBin =  InventoryRow["PartAlloc_BinNum"].ToString();
		string pLot =  InventoryRow["PartAlloc_LotNum"].ToString();
		string pAssemblySeq = "0";
		try {
			pAssemblySeq = InventoryRow["PartAlloc_AssemblySeq"].ToString();
			}
		catch (Exception e) {}
		string pMtlSeq =  InventoryRow["PartAlloc_MtlSeq"].ToString();
		bool pOrderOnly = (bool)ComponentRow.Cells["OrderOnly"].Value;
		string pUser =  session.UserID;
		bool pASRS = false;
		if (pASRSTranID &amp;gt; 0) pASRS = true;

		UD11Adapter adpUD11 = new UD11Adapter(UD14Form);
		adpUD11.BOConnect();

		adpUD11.GetaNewUD11();
		// MessageBox.Show("nextUD11 = "+ nextUD11.ToString());
		adpUD11.UD11Data.UD11.Rows[0]["Key1"] = pComponent;
		adpUD11.UD11Data.UD11.Rows[0]["Key2"] = pJobNumber;
		adpUD11.UD11Data.UD11.Rows[0]["Key3"] = "Belt";
		adpUD11.UD11Data.UD11.Rows[0]["Key4"] = nextUD11.ToString();
		adpUD11.UD11Data.UD11.Rows[0]["Key5"] = pWarehouse;
		adpUD11.UD11Data.UD11.Rows[0]["Number01"] = pOrderNum;
		adpUD11.UD11Data.UD11.Rows[0]["Number02"] = pOrderLine;
		adpUD11.UD11Data.UD11.Rows[0]["Number03"] = pOrderRel;
		adpUD11.UD11Data.UD11.Rows[0]["Number04"] = pQty;
		adpUD11.UD11Data.UD11.Rows[0]["Number05"] = pAllocatedQty;
		adpUD11.UD11Data.UD11.Rows[0]["Number06"] = pASRSTranID;
		adpUD11.UD11Data.UD11.Rows[0]["ShortChar01"] = session.PlantID;
		adpUD11.UD11Data.UD11.Rows[0]["ShortChar02"] = pWarehouse;
		adpUD11.UD11Data.UD11.Rows[0]["ShortChar03"] = pBin;
		adpUD11.UD11Data.UD11.Rows[0]["ShortChar04"] = pLot;
		adpUD11.UD11Data.UD11.Rows[0]["ShortChar05"] = pAssemblySeq;
		adpUD11.UD11Data.UD11.Rows[0]["ShortChar06"] = pMtlSeq;
		adpUD11.UD11Data.UD11.Rows[0]["ShortChar07"] = pUser;
		adpUD11.UD11Data.UD11.Rows[0]["CheckBox01"] = pOrderOnly;
		adpUD11.UD11Data.UD11.Rows[0]["CheckBox02"] = pASRS;
		adpUD11.UD11Data.UD11.Rows[0]["Character01"] = pUser;

		adpUD11.Update();
		adpUD11.Dispose();


/*     ttUD11.Key1 = PartNum
            ttUD11.Key2 = JobNum
            ttUD11.Key3 = 'Belt'
            ttUD11.Key4 = string(newKeyID)
            ttUD11.Key5 = WarehouseCode
            ttUD11.Number01 = OrderNum
            ttUD11.Number02 = OrderLine
            ttUD11.Number03 = OrderRel
            ttUD11.Number04 = Qty
            ttUD11.Number05 = AllocQty
            ttUD11.Number06 = ASRSTranID
            ttUD11.ShortChar01 = cur-plant
            ttUD11.ShortChar02 = WarehouseCode
            ttUD11.ShortChar03 = BinNum
            ttUD11.ShortChar04 = LotNum
            ttud11.ShortChar05 = AssemblySeq
            ttUD11.ShortChar06 = MaterialSeq
            ttUD11.CheckBox01 = OrderOnly
            ttUD11.Character01 = dcd-userid
            ttUD11.ShortChar07 = BatchNum
            ttUD11.CheckBox02 = IF ASRSTranID &amp;gt; 0 THEN TRUE ELSE FALSE.
*/




		nextUD11++;
/*		adpComp.CompanyData.Company.Rows[0]["Number02"] = nextUD11;
		adpComp.Update();
		adpComp.Dispose();*/



		try {
		sqlCom = new SqlCommand("update Company_UD set Number02 = " + nextUD11.ToString()+ "where Company_UD.ForeignSysRowID = '" + companyRowID + "'", dbConn);
		sqlCom.ExecuteNonQuery();
		}
		catch (Exception e)
		{
			MessageBox.Show("1876: " + e.Message);
		}
		//adpComp.Dispose();
		dbConn.Close();
		dbConn.Dispose();


	}

	private static void CreateNewUD11(Infragistics.Win.UltraWinGrid.UltraGridRow ComponentRow, Infragistics.Win.UltraWinGrid.UltraGridRow InventoryRow, decimal ASRSTranID)
	{
		/*E10
		Ice.Core.Session session = (Ice.Core.Session)oTrans.Session;
		
		//Progress.Open4GL.DynamicAPI.Session progressSession = session.ConnectionPool.Get();
		Ice.Proxy.BO.GetImpl progressSession = WCFServiceSupport.CreateImpl&amp;lt;Ice.Proxy.BO.GetImpl&amp;gt;((Ice.Core.Session)oTrans.Session, Ice.Proxy.BO.GetImpl.UriPath);
		
		PersistentProc proc = default(PersistentProc);
		
		ParameterSet parms = new ParameterSet(15);
		
		parms.setStringParameter(1, ComponentRow.Cells["Component"].Text.Trim(), ParameterSet.INPUT);
		parms.setStringParameter(2, ComponentRow.Cells["JobNumber"].Text.Trim(), ParameterSet.INPUT);
		parms.setStringParameter(3, InventoryRow.Cells["WarehouseCode"].Text.Trim(), ParameterSet.INPUT);
		parms.setIntegerParameter(4, (int)ComponentRow.Cells["OrderNum"].Value, ParameterSet.INPUT);
		parms.setIntegerParameter(5, (int)ComponentRow.Cells["OrderLine"].Value, ParameterSet.INPUT);
		parms.setIntegerParameter(6, (int)ComponentRow.Cells["OrderRel"].Value, ParameterSet.INPUT);
		parms.setDecimalParameter(7, (decimal)ComponentRow.Cells["Qty"].Value, ParameterSet.INPUT);
		parms.setDecimalParameter(8, (decimal)InventoryRow.Cells["OH Qty"].Value, ParameterSet.INPUT);
		parms.setDecimalParameter(9, ASRSTranID, ParameterSet.INPUT);
		parms.setStringParameter(10, InventoryRow.Cells["Location"].Text.Trim(), ParameterSet.INPUT);
		parms.setStringParameter(11, InventoryRow.Cells["Lot"].Text.Trim(), ParameterSet.INPUT);
		parms.setStringParameter(12, ComponentRow.Cells["AssemblySeq"].Text.ToString(), ParameterSet.INPUT);
		parms.setStringParameter(13, ComponentRow.Cells["MaterialSeq"].Text.ToString(), ParameterSet.INPUT);
		parms.setBooleanParameter(14, (bool)ComponentRow.Cells["OrderOnly"].Value, ParameterSet.INPUT);
		parms.setStringParameter(15, InventoryRow.Cells["Batch"].Text.Trim(), ParameterSet.INPUT);
		
		try
		{
			proc = progressSession.runPersistentProcedure(@"ud\AllocWorkbench\createUD11.p", parms);
			
			proc.delete();
		}
		catch(Exception ex)
		{
			EpiMessageBox.Show(ex.ToString(), "Writing UD11 Record");
		}
		finally
		{
			//session.ConnectionPool.Release(progressSession);
			Ice.Proxy.BO.ReleaseImpl  = WCFServiceSupport.CreateImpl&amp;lt;Ice.Proxy.BO.ReleaseImpl&amp;gt;((Ice.Core.Session)oTrans.Session, Ice.Proxy.BO.ReleaseImpl.UriPath);
		}	
	*/	

	/*	string connStr = "";
		using (DynamicQueryAdapter adDynamic = new DynamicQueryAdapter(UD14Form))
		{
			adDynamic.BOConnect();
			adDynamic.ExecuteByID("CompanyControls");    

	        connStr = adDynamic.QueryResults.Tables[0].Rows[0]["Company_Character01"].ToString();

		}


		System.Data.SqlClient.SqlConnection dbConn = new System.Data.SqlClient.SqlConnection(connStr);
		dbConn.Open(); */

		Ice.Core.Session session = (Ice.Core.Session)oTrans.Session;

		SqlConnection dbConn = new SqlConnection(sqlConnection);
		dbConn.Open();
	    SqlCommand sqlCom = new SqlCommand("select distinct Company_UD.Number02 as Number02, Company.SysRowID as SysRowID from Company inner join Company_UD on Company.SysRowID = Company_UD.ForeignSysRowID where Company.Company = '" + session.CompanyID + "'", dbConn);

		//SqlCommand sqlCom = new SqlCommand("select distinct Number02 from Company_UD where Company_UD.Company = '" + session.CompanyID + "'", dbConn);
		
		SqlDataAdapter da = new SqlDataAdapter(sqlCom);
		
		DataTable dt = new DataTable();
		
		da.Fill(dt);

		string rowID = dt.Rows[0][1].ToString();
		int nextUD11 = Convert.ToInt32(dt.Rows[0][0]);

		string pComponent =  ComponentRow.Cells["Component"].Text.Trim();
		string pJobNumber = ComponentRow.Cells["JobNumber"].Text.Trim();
		string pWarehouse = InventoryRow.Cells["WarehouseCode"].Text.Trim();
		int pOrderNum =  (int)ComponentRow.Cells["OrderNum"].Value;
		int pOrderLine = (int)ComponentRow.Cells["OrderLine"].Value;
		int pOrderRel = (int)ComponentRow.Cells["OrderRel"].Value;
		decimal pQty = (decimal)ComponentRow.Cells["Qty"].Value;
		//(decimal)InventoryRow["PartAlloc.AllocatedQty"] + (decimal)InventoryRow["PartAlloc.PickingQty"] + (decimal)InventoryRow["PartAlloc.PickedQty"] + (decimal)InventoryRow["PartAlloc.ReservedQty"];
		decimal pAllocatedQty =  (decimal)InventoryRow.Cells["OH Qty"].Value;
		decimal pASRSTranID =  ASRSTranID;
		string pBin =  InventoryRow.Cells["Location"].Text.Trim();
		string pLot =  InventoryRow.Cells["Lot"].Text.Trim();
		string pAssemblySeq = ComponentRow.Cells["AssemblySeq"].Text.ToString();
		string pMtlSeq =  ComponentRow.Cells["MaterialSeq"].Text.ToString();
		bool pOrderOnly = (bool)ComponentRow.Cells["OrderOnly"].Value;
		string pBatch = InventoryRow.Cells["Batch"].Text.Trim();
		string pUser =  session.UserID;
		bool pASRS = false;
		if (pASRSTranID &amp;gt; 0) pASRS = true;

		UD11Adapter adpUD11 = new UD11Adapter(UD14Form);
		adpUD11.BOConnect();

		adpUD11.GetaNewUD11();


		//MessageBox.Show("Creating UD11 with: " + pOrderNum.ToString() + pOrderLine.ToString()+ pOrderRel.ToString()+  pAssemblySeq + pMtlSeq);
		adpUD11.UD11Data.UD11.Rows[0]["Key1"] = pComponent;
		adpUD11.UD11Data.UD11.Rows[0]["Key2"] = pJobNumber;
		adpUD11.UD11Data.UD11.Rows[0]["Key3"] = "Belt";
		adpUD11.UD11Data.UD11.Rows[0]["Key4"] = nextUD11.ToString();
		adpUD11.UD11Data.UD11.Rows[0]["Key5"] = pWarehouse;
		adpUD11.UD11Data.UD11.Rows[0]["Number01"] = pOrderNum;
		adpUD11.UD11Data.UD11.Rows[0]["Number02"] = pOrderLine;
		adpUD11.UD11Data.UD11.Rows[0]["Number03"] = pOrderRel;
		adpUD11.UD11Data.UD11.Rows[0]["Number04"] = pQty;
		adpUD11.UD11Data.UD11.Rows[0]["Number05"] = pAllocatedQty;
		adpUD11.UD11Data.UD11.Rows[0]["Number06"] = pASRSTranID;
		adpUD11.UD11Data.UD11.Rows[0]["ShortChar01"] = session.PlantID;
		adpUD11.UD11Data.UD11.Rows[0]["ShortChar02"] = pWarehouse;
		adpUD11.UD11Data.UD11.Rows[0]["ShortChar03"] = pBin;
		adpUD11.UD11Data.UD11.Rows[0]["ShortChar04"] = pLot;
		adpUD11.UD11Data.UD11.Rows[0]["ShortChar05"] = pAssemblySeq;
		adpUD11.UD11Data.UD11.Rows[0]["ShortChar06"] = pMtlSeq;
		adpUD11.UD11Data.UD11.Rows[0]["ShortChar07"] = pBatch;
		adpUD11.UD11Data.UD11.Rows[0]["CheckBox01"] = pOrderOnly;
		adpUD11.UD11Data.UD11.Rows[0]["CheckBox02"] = pASRS;
		adpUD11.UD11Data.UD11.Rows[0]["Character01"] = pUser;

		adpUD11.Update();
		adpUD11.Dispose();

		nextUD11++;
//MessageBox.Show("Josh About to reference Number02");
//		adpComp.CompanyData.Company.Rows[0]["Number02"] = nextUD11;
		try{
		sqlCom = new SqlCommand("update Company_UD set Number02 = " + nextUD11.ToString()+ "where Company_UD.ForeignSysRowID = '" +  rowID  + "'", dbConn);
		
		sqlCom.ExecuteNonQuery();
		}
		catch (Exception e)
		{
			MessageBox.Show("2023: " + e.Message);
		}


//MessageBox.Show("Josh About to reference Number02");
//		adpComp.Update();
		//adpComp.Dispose();
		dbConn.Close();
		dbConn.Dispose();



	}
	
	private static void cmdFilterDemand_Click(object sender, System.EventArgs args)
	{
		if(hasData)
		{
			string RowFilters = "";
			
			if(txtStyle.Text.Trim() != "")
			{
				RowFilters = "Code ='" + txtStyle.Text.Trim() + "' and ";
			}
			
			if(txtOrderNum.Text.Trim() != "")
			{
				RowFilters += "[Order #] = '" + txtOrderNum.Text.Trim() + "' and ";
			}
			
			if(txtLine.Text.Trim() != "")
			{
				RowFilters += "[Order Line] = '" + txtLine.Text.Trim() + "' and ";
			}
			
			if(txtRelease.Text.Trim() != "")
			{
				RowFilters += "[Order Rel] = '" + txtRelease.Text.Trim() + "' and ";
			}
			
			if(txtJobNumber.Text.Trim() != "")
			{
				RowFilters += "[Job Number] = '" + txtJobNumber.Text.Trim() + "' and ";
			}
			
			//if out of stock searching is checked, and the user has not entered an order number/job, clear the search results
			if(chkOutOfStock.Checked &amp;amp;&amp;amp; epiCheckBox1.Checked &amp;amp;&amp;amp; txtOrderNum.Text.Trim() == "")
				FormTable.Rows.Clear();
			else if(chkOutOfStock.Checked &amp;amp;&amp;amp; !epiCheckBox1.Checked &amp;amp;&amp;amp; txtJobNumber.Text.Trim() == "")
				FormTable.Rows.Clear();
			
			DateTime due;
			
			if(epiDueDate.Value != null)
				if(DateTime.TryParse(epiDueDate.Value.ToString(), out due))
					RowFilters += "[Due Date] &amp;gt;= '" + due.ToString("MM/dd/yyyy") + "' and ";
					
			if(epiToDueDate.Value != null)
				if(DateTime.TryParse(epiToDueDate.Value.ToString(), out due))
					RowFilters += "[Due Date] &amp;lt;= '" + due.ToString("MM/dd/yyyy") + "' and ";
			
			if(RowFilters.Length &amp;gt; 0)
				RowFilters = RowFilters.Substring(0, RowFilters.Length - 4);
				
			DataView dv = new DataView(FormTable);
			
			dv.RowFilter = RowFilters;
			
			epiUltraGrid1.DataSource = dv;
			epiUltraGrid1.DataBind();
			
			epiGroupBox3.Text = "2 Selection Data for Job";
			epiTextBox3.Text = "";
			epiTextBox4.Text = "";
			epiButton4.ReadOnly = true;
		}
		else
		{
			MessageBox.Show("You must first search for data before you are capable of filtering it.");
		}
	}

	private static void ComponentGrid_CellChange(object sender, Infragistics.Win.UltraWinGrid.CellEventArgs args)
	{		
		try
		{
		//only allow for one component to be selected at a time		
		DataTable dt = (DataTable)ComponentGrid.DataSource;
		
		foreach(DataColumn dc in dt.Columns)
		
			foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in ComponentGrid.Rows)
				if(args.Cell.Row.Cells["RowID"].Text != row.Cells["RowID"].Text)
				{
					row.Cells["Sel"].Value = "False";
					row.Update();
					decimal accumTemp = 0;
					decimal.TryParse(row.Cells["tempAccum"].Text, out accumTemp);
					
					if(accumTemp &amp;gt; 0)
					{
						row.Cells["tempAccum"].Value = "0";
						row.Cells["Accum"].Value = "0";
					}					
				}
		}
		catch{}
		
		if(args.Cell.Row.Cells["Sel"].Text == "False")
		{
			LocationGrid.DataSource = GetInventoryTable();
			LocationGrid.DataBind();
		}
		else
			cmdInventory_Click(null, null);
	}

	private static void cmdInventory_Click(object sender, System.EventArgs args)
	{
		DataTable InventoryData = GetInventoryTable();		
		DataRow newRow;
		
		allocate = true;
		//cmdDeAllocated.Enabled = false;		
		//cmdIssue.Enabled = true;
		cmdDeAllocated.ReadOnly = true;		
		cmdIssue.ReadOnly = false;
		
		int RowID = 0;
		
		string Style = "";
		string Code = "";
		string Color = "";
		string Material = "";
		bool OrderOnly = false;
		string PartNum = "";
	
		bool TFOrderOnly = false; //MT
		
		oTrans.PushStatusText("Obtaining Inventory Location Data...",true);
		
		foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in ComponentGrid.Rows)		
			if(row.Cells["Sel"].Text == "True")
			{
				Style = row.Cells["Style"].Text;
				Code = row.Cells["Code"].Text;
				Color = row.Cells["Color"].Text;
				Material = row.Cells["Material"].Text;
				OrderOnly = row.Cells["OrderOnly"].Text == "True" ? true : false;
				PartNum = row.Cells["Component"].Text;

				TFOrderOnly = row.Cells["TFOrderOnly"].Text == "True" ? true : false; //MT

				SqlConnection dbConn = new SqlConnection(sqlConnection);
				
				System.Text.StringBuilder sb = new System.Text.StringBuilder();
				
				//sb.Append("select distinct cast(0 as bit) as Sel, cast(isnull(min(jobmtl.issuedcomplete) over(partition by PartBin.BinNum, PartBin.LotNum, PartLot.Batch),1) as bit) as selectable, PartBin.BinNum as Location, PartBin.LotNum as Lot, isnull(PartLot.Batch,'') as Batch, isnull(PartLot.PartLotDescription,'') as Comments, PartLot.FirstRefDate as Age, partbin.onhandqty - partbin.allocatedqty as [OH Qty], PartBin.PartNum as PartNum, PartBin.WarehouseCode as WarehouseCode, PlantWhse.Plant as Plant, 0 as RowID, '' as JobNumer, 0 as OrderNum, 0 as OrderLine, 0 as OrderRel, 0 as AssemblySeq, 0 as MaterialSeq, case when PartBin.BinNum = PlantWhse.PrimBin and PartBin.WarehouseCode = PartPlant.PrimWhse then cast(1 as bit) else cast(0 as bit) end as [Default], PartLot.FirstRefDate as ReceiptDate from PlantWhse inner join PartWhse on PartWhse.Company = PlantWhse.Company and PartWhse.WarehouseCode = PlantWhse.WarehouseCode and PartWhse.PartNum = PlantWhse.PartNum inner join PartPlant on PartPlant.Company = PlantWhse.Company and PartPlant.PartNum = PlantWhse.PartNum and PartPlant.Plant = PlantWhse.Plant inner join PartBin on PartBin.Company = PartWhse.Company and PartBin.PartNum = PartWhse.PartNum and PartBin.WarehouseCode = PartWhse.WarehouseCode left outer join PartLot on PartLot.Company = PartBin.Company and PartLot.PartNum = PartBin.PartNum and PartLot.LotNum = PartBin.LotNum left outer join UD11 on UD11.Company = PartBin.Company and UD11.Key3 = 'Belt' and UD11.Key1 = PartBin.PartNum and UD11.ShortChar03 = PartBin.BinNum and UD11.ShortChar04 = PartBin.LotNum and UD11.ShortChar02 = PartBin.WarehouseCode left outer join JobMtl on JobMtl.Company = UD11.Company and JobMtl.JobNum = UD11.Key2 and cast(JobMtl.AssemblySeq as varchar(10)) = UD11.ShortChar05 and cast(JobMtl.MtlSeq as varchar(10)) = UD11.ShortChar06 left outer join OrderRel on OrderRel.Company = UD11.Company and OrderRel.OrderNum = UD11.Number01 and OrderRel.OrderLine = UD11.Number02 and OrderRel.OrderRelNum = UD11.Number03 where PlantWhse.PartNum = '").Append(PartNum.Replace("'", "''")).Append("' and partbin.onhandqty - partbin.allocatedqty &amp;gt; 0");
				sb.Append("select distinct cast(0 as bit) as Sel, case when cast(min(case when isnull(JObHead.JobClosed,1) = 1 then 1 else isnull(jobmtl.issuedcomplete,1) end) over (partition by PartBin.BinNum, PartBin.LotNum, PartLot.Batch) as bit) = 0 and PartBin.BinNum &amp;lt;&amp;gt; 'ASRS' then 1 when cast(min(case when isnull(JObHead.JobClosed,1) = 1 then 1 else isnull(jobmtl.issuedcomplete,1) end) over (partition by PartBin.BinNum, PartBin.LotNum, PartLot.Batch) as bit) = 1 then 1 else 0 END as selectable, PartBin.BinNum as Location, PartBin.LotNum as Lot, isnull(PartLot.Batch,'') as Batch, isnull(PartLot.PartLotDescription,'') as Comments, PartLot.FirstRefDate as Age, partbin.onhandqty - partbin.allocatedqty as [OH Qty], PartBin.PartNum as PartNum, PartBin.WarehouseCode as WarehouseCode, PlantWhse.Plant as Plant, 0 as RowID, '' as JobNumer, 0 as OrderNum, 0 as OrderLine, 0 as OrderRel, 0 as AssemblySeq, 0 as MaterialSeq, case when PartBin.BinNum = PlantWhse.PrimBin and PartBin.WarehouseCode = PartPlant.PrimWhse then cast(1 as bit) else cast(0 as bit) end as [Default], PartLot.FirstRefDate as ReceiptDate from Erp.PlantWhse (nolock) inner join Erp.PartWhse (nolock) on PartWhse.Company = PlantWhse.Company and PartWhse.WarehouseCode = PlantWhse.WarehouseCode and PartWhse.PartNum = PlantWhse.PartNum inner join Erp.PartPlant (nolock) on PartPlant.Company = PlantWhse.Company and PartPlant.PartNum = PlantWhse.PartNum and PartPlant.Plant = PlantWhse.Plant inner join Erp.PartBin (nolock) on PartBin.Company = PartWhse.Company and PartBin.PartNum = PartWhse.PartNum and PartBin.WarehouseCode = PartWhse.WarehouseCode left outer join Erp.PartLot (nolock) on PartLot.Company = PartBin.Company and PartLot.PartNum = PartBin.PartNum and PartLot.LotNum = PartBin.LotNum left outer join Ice.UD11 (nolock) on UD11.Company = PartBin.Company and UD11.Key3 = 'Belt' and UD11.Key1 = PartBin.PartNum and UD11.ShortChar03 = PartBin.BinNum and UD11.ShortChar04 = PartBin.LotNum and UD11.ShortChar02 = PartBin.WarehouseCode left outer join Erp.JobMtl (nolock) on JobMtl.Company = UD11.Company and JobMtl.JobNum = UD11.Key2 and cast(JobMtl.AssemblySeq as varchar(10)) = UD11.ShortChar05 and cast(JobMtl.MtlSeq as varchar(10)) = UD11.ShortChar06 left outer join OrderRel (nolock) on OrderRel.Company = UD11.Company and OrderRel.OrderNum = UD11.Number01 and OrderRel.OrderLine = UD11.Number02 and OrderRel.OrderRelNum = UD11.Number03 left Outer Join JobHead with(nolock) on JobHead.Company = JobMtl.Company and JobHead.JobNum = JobMtl.JobNum where PlantWhse.PartNum = '").Append(PartNum.Replace("'", "''")).Append("' and partbin.onhandqty - partbin.allocatedqty &amp;gt; 0");
				if(cboInvWarehouse.Value != null)
					sb.Append(" and PartBin.WarehouseCode = '").Append(cboInvWarehouse.Value.ToString()).Append("'");
					
				if(cboInvPlant.Value != null)
					sb.Append(" and PlantWhse.Plant = '").Append(cboInvPlant.Value.ToString()).Append("'");	

				if (Contract != "") 
				{
					bool recSelected = false;
					//DataSet dsContract = Ice.UI.FormFunctions.SearchFunctions.listLookup(oTrans, "PlanContractAdapter", out recSelected, false, "ContractID ='" + Contract +"'");
//					if (dsContract.Tables[0].Rows.Count &amp;gt; 0) sb.Append(" and (PartBin.BinNum = '" + dsContract.Tables[0].Rows[0]["BinNum"].ToString() + "' or PartBin.BinNum = '" + dsContract.Tables[0].Rows[0]["RcvBin"].ToString() + "' " );
					using (PlanContractAdapter adpContract = new PlanContractAdapter(UD14Form))
					{
						adpContract.BOConnect();
						adpContract.GetByID(Contract);
						if (adpContract.PlanContractData.Tables[0].Rows.Count &amp;gt; 0) sb.Append(" and (PartBin.BinNum = '" + adpContract.PlanContractData.Tables[0].Rows[0]["BinNum"].ToString() + "' or PartBin.BinNum = '" + adpContract.PlanContractData.Tables[0].Rows[0]["RcvBin"].ToString() + "') " );
					}

				}

				if(((Ice.Core.Session)oTrans.Session).UserID.ToUpper() == "MANAGER")
				{
					txtInfo.Text = sb.ToString();
					txtInfo.Visible = true;
					/*System.IO.StreamWriter sw = new System.IO.StreamWriter(@"C:\temp\testing.txt");
					sw.WriteLine(sb.ToString());
					sw.Close();
					sw.Dispose();*/
					
				}	
		//JLP - Commented out for testing
					//if(((Erp.Core.Session)oTrans.Session).UserID.ToUpper() == "MANAGER")
					//MessageBox.Show(sb.ToString());				
				SqlCommand sqlCom = new SqlCommand(sb.ToString(), dbConn);
				
				SqlDataAdapter da = new SqlDataAdapter(sqlCom);
				
				da.Fill(InventoryData);
				
				da.Dispose();
				sqlCom.Dispose();
				dbConn.Dispose();	
				
				if(epiUltraCombo2.Text.Trim() == "01PSTK")
				{
					DataView dvv = new DataView(InventoryData);
					dvv.RowFilter = "Selectable = true";
				
					InventoryData = dvv.ToTable();
				}
				
				InventoryData.Columns.Remove("Selectable");
				
				foreach(DataRow dr in InventoryData.Rows)
				{
					dr["OrderNum"] = int.Parse(row.Cells["OrderNum"].Text);
					dr["OrderLine"] = int.Parse(row.Cells["OrderLine"].Text);
					dr["OrderRel"] = int.Parse(row.Cells["OrderRel"].Text);
					dr["AssemblySeq"] = int.Parse(row.Cells["AssemblySeq"].Text);
					dr["MaterialSeq"] = (int)row.Cells["MaterialSeq"].Value;
					dr["JobNumber"] = row.Cells["JobNumber"].Text;
					dr["RowID"] = RowID;
					RowID++;
				}				
			
				DataView dv;
				DataTable temp;
				
				//only return the default + 1, so a total of two rows(at the most)
				if(chkPrimaryPlusOne.Checked)
				{
					dv = new DataView(InventoryData);
					
					dv.Sort = "Default desc, Location Asc";
					
					temp = dv.ToTable();
					
					temp.Columns["RowID"].ReadOnly = false;
					
					int newRowID = 0;
					
					foreach(DataRow dr in temp.Rows)
					{
						dr["RowID"] = newRowID;
						newRowID++;
					}
					
					temp.Columns["RowID"].ReadOnly = true;
					
					dv = new DataView(temp);
					dv.RowFilter = "RowID &amp;lt; 2";
					dv.Sort = "RowID Asc";
					
					InventoryData = dv.ToTable();
				}
		
			
		
				FormInventoryTable = InventoryData.Copy();			
		
				hasInventoryData = true;			
				
				//now bind the Grid to the source
				BindInventoryData();
					
				if(!OrderOnly)
					PartNum = "";
				
				//try{fillIssuedGrid(Code, Style, Color, Material, PartNum);}
				//catch{}
			}
			
			//clear the status message
			oTrans.PushStatusText("",false);
		
	}

	private static void BindInventoryData()
	{
		DataView dv = new DataView(FormInventoryTable);
		
		dv.Sort = "Age desc";
		
		LocationGrid.DataSource = dv;
		LocationGrid.DataBind();		
					
		//LocationGrid.DisplayLayout.AutoFitColumns = true;
		LocationGrid.DisplayLayout.AutoFitStyle =  AutoFitStyle.ResizeAllColumns;

		foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in LocationGrid.Rows)
		{
			if(row.Cells["Default"].Text == "True")
			{
				row.CellAppearance.BackColor = System.Drawing.Color.PaleGreen;
			}
		}
	}

	private static void LocationGrid_CellChange(object sender, Infragistics.Win.UltraWinGrid.CellEventArgs args)
	{
		decimal SelQty = 0;
		
		foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in LocationGrid.Rows)		
			if(row.Cells["Sel"].Text == "True")
			{
				SelQty += (decimal)row.Cells["OH qty"].Value;
			}
			
		SelQty = allocate ? SelQty : -1 * SelQty;
		
	
		//first allow editing of the fields
		DataTable dt = (DataTable)ComponentGrid.DataSource;				

		foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in ComponentGrid.Rows)
			if(row.Cells["Sel"].Text == "True")
			{
				bool OrderOnly = false;
				bool TFOrderOnly = false; //MT
				bool isMDT = false;
				
				//MT
				if(row.Cells["TFOrderOnly"].Text == "True")
					TFOrderOnly = true;

				if(row.Cells["OrderOnly"].Text == "True")
					OrderOnly = true;

				//MessageBox.Show("JLP Order Only: " + OrderOnly.ToString());

				if(row.Cells["Plant"].Text == "MDT" &amp;amp;&amp;amp; row.Cells["WarehouseCode"].Text == "04020")
					isMDT = true;
				

				//MessageBox.Show("JLP Get Accum with: " + int.Parse(row.Cells['OrderLine
				decimal originalQty = GetAccum(row.Cells["Component"].Text, row.Cells["JobNumber"].Text, row.Cells["AssemblySeq"].Text, row.Cells["MaterialSeq"].Text, int.Parse(row.Cells["OrderNum"].Text), int.Parse(row.Cells["OrderLine"].Text), int.Parse(row.Cells["OrderRel"].Text), OrderOnly);
				originalQty +=  GetAccumTFOrder(row.Cells["Component"].Text, row.Cells["JobNumber"].Text,TFOrderOnly);
				
	
					if(row.Cells["Plant"].Text != "SUW" || row.Cells["WarehouseCode"].Text != "01PSTK")
					{
						row.Cells["Accum"].Value = originalQty &amp;gt; decimal.Parse(row.Cells["Qty"].Text) ? decimal.Parse(row.Cells["Qty"].Text) : originalQty;
						originalQty = decimal.Parse(row.Cells["Accum"].Text);
					}

			
					row.Cells["Accum"].Value = SelQty + originalQty;
					row.Cells["TempAccum"].Value = SelQty + originalQty;
							
				
				if((OrderOnly || isMDT) &amp;amp;&amp;amp; decimal.Parse(row.Cells["Accum"].Text) &amp;gt; decimal.Parse(row.Cells["Qty"].Text))
				{
					row.Cells["Accum"].Value = row.Cells["Qty"].Value;
					row.Cells["TempAccum"].Value = row.Cells["Qty"].Value;
				}
				
			}
	}

	private static void cmdIssue_Click(object sender, System.EventArgs args)
	{
			oTrans.PushStatusText("Issue/Allocating Stock", true);
		
			bool SelectedLocation = false;
			
			Infragistics.Win.UltraWinGrid.UltraGridRow ComponentRow = null;
			
			foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in ComponentGrid.Rows)
				if(row.Cells["Sel"].Text == "True")
				{
					ComponentRow = row;
					
					//Also make sure that the plant they are logged into is the plant that is selected
					if(((Ice.Core.Session)UD14Form.Session).PlantID != row.Cells["Plant"].Text)
					{
						MessageBox.Show("The plant that you are currently logged into does not match the plant for this job/order.  You are unable to allocate against this.", "Plant Mis-match");
						return;
					}
				}
			
			//only continue if they have selected a component row
			if(ComponentRow != null)
			{
				foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in LocationGrid.Rows)				
					if(row.Cells["Sel"].Text == "True")
						SelectedLocation = true;
				
				//At least one location must be selected before issuance can occur		
				if(SelectedLocation)
				{
					bool doIssue = true;				
					
					//everything appears to be okay in terms of selection, now see if the selected amount is greater than or equal to the needed amount
					// Take check out for quantity per Brad as part of performance evaluation
//					if(decimal.Parse(ComponentRow.Cells["Accum"].Text) &amp;lt; decimal.Parse(ComponentRow.Cells["Qty"].Text))
//					{
//						if(MessageBox.Show("The inventory amount that you have selected to issue is not enough for the quantity required, do you wish to continue?", "Quantity Check", MessageBoxButtons.YesNo) == DialogResult.Yes)
//							doIssue = true;
//						else
//							doIssue = false;
//					}
//					else
//						doIssue = true;
					
					decimal tranID = 0;
					decimal allocAmount = 0;
					
					//only continue if doIssue is true, as doIssue would only be false if they would have selected not to continue	
					if(doIssue)
					{
						  
						  if(ComponentRow.Cells["TFOrderOnly"].Text == "True")	
						  {
							  List&amp;lt;string[]&amp;gt; preLots = DeAllocatePreTFORows(ComponentRow);
							  if(preLots.Count &amp;gt; 0 &amp;amp;&amp;amp; ComponentRow.Cells["TFOrderOnly"].Text == "True")
							  {
									cmdInventory_Click(sender, args);
									//MessageBox.Show("Reload Inventory");
									foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in LocationGrid.Rows)
									{
										for(int i = 0; i &amp;lt;preLots.Count; i++ )
										{
											string[] arr = preLots[i];
											if(arr[0] == row.Cells["WarehouseCode"].Text &amp;amp;&amp;amp; arr[1] == row.Cells["Location"].Text &amp;amp;&amp;amp; arr[2] == row.Cells["Lot"].Text)
											{
												row.Cells["Sel"].Value = true;
												//MessageBox.Show(row.Cells["Lot"].Text);
											}	
										} 
									}
							  }
						  }					  
						  

						  //JLP - Perform allocation in one call rather than looping
						  allocAmount = AllocateStock(ComponentRow );
						   					 
						  //MessageBox.Show("JLP - Performed AllocateStock:" + allocAmount.ToString());
						  if (allocAmount != -1)
						  	if(allocAmount &amp;gt; 0 || (ComponentRow.Cells["Plant"].Text.Trim().ToUpper() == "SUW" &amp;amp;&amp;amp; ComponentRow.Cells["WarehouseCode"].Text.Trim().ToUpper() == "01PSTK"))
								{

									//MessageBox.Show("allocAmount = " + allocAmount.ToString()); //TEST

									foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in LocationGrid.Rows)
									{
										tranID = 0;
										if(row.Cells["Sel"].Text == "True")
											{
												if(row.Cells["Location"].Text.ToUpper() == "ASRS")
													tranID = CreateASRSRequest(ComponentRow, row); //Generate the ASRS request	 
											}
									}	
									 
											//create new UD11 records
											//MessageBox.Show("JLP - About to call get alloc for gen");
											foreach(DataRow dr in getAllocForGen(ComponentRow).Rows)
												{
													//MessageBox.Show("JLP - about to create ud11");  
													CreateNewUD11(ComponentRow, dr, tranID);
												}	
									//MT		
									foreach(DataRow dr in getAllocRowsForTFOrder(ComponentRow).Rows)
									{  
										//MessageBox.Show("TranId = " + tranID.ToString());
										CreateNewTFOrderUD11(ComponentRow, dr, tranID);
									}	
 
									
								}

//						foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in LocationGrid.Rows)
//							if(row.Cells["Sel"].Text == "True")
//							{
//								tranID = 0;
//								
//								//changing from issue to allocate
//								MessageBox.Show("JLP - about to call allocate stock procedure");
//								allocAmount = AllocateStock(ComponentRow, row);
//								MessageBox.Show("JLP - finished allocate stock procedure");
//								//Allocation for ASRS now, make the request
//								if(allocAmount != -1)
//								if(allocAmount &amp;gt; 0 || (ComponentRow.Cells["Plant"].Text.Trim().ToUpper() == "SUW" &amp;amp;&amp;amp; ComponentRow.Cells["WarehouseCode"].Text.Trim().ToUpper() == "01PSTK"))
//								{
//									if(row.Cells["Location"].Text.ToUpper() == "ASRS")
//										tranID = CreateASRSRequest(ComponentRow, row); //Generate the ASRS request								
//								
//									//save record issuance
//									CreateNewUD11(ComponentRow, row, tranID);
//								}
//							}
//							
//


						//all records should now be created at this point, now it is time rebind the Inventory Grid to remove the already selected items.
						//MessageBox.Show("JLP About to rebind the inventory grid");
						cmdInventory_Click(sender, args);
						
						//also, mark tempAccum = 0 and the actual Accum = to what was selected
						DataTable dt  = (DataTable)ComponentGrid.DataSource;
							
						try
						{
							foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in ComponentGrid.Rows)
							{
								if(row.Cells["Sel"].Text == "True")
								{
									bool OrderOnly = false;
									bool TFOrderOnly = false;
									
									if(row.Cells["OrderOnly"].Text == "True")
										OrderOnly = true;
									
									row.Cells["Accum"].Value = GetAccum(row.Cells["Component"].Text, row.Cells["JobNumber"].Text, row.Cells["AssemblySeq"].Text,  row.Cells["MaterialSeq"].Text, int.Parse(row.Cells["OrderNum"].Text), int.Parse(row.Cells["OrderLine"].Text), int.Parse(row.Cells["OrderRel"].Text), OrderOnly);
									
									//MT
									if(row.Cells["TFOrderOnly"].Text == "True")
										TFOrderOnly = true;

									row.Cells["Accum"].Value = Convert.ToDecimal(row.Cells["Accum"].Value) + GetAccumTFOrder(row.Cells["Component"].Text, row.Cells["JobNumber"].Text, TFOrderOnly);
									

									if(row.Cells["Plant"].Text != "SUW" || row.Cells["WarehouseCode"].Text != "01PSTK")
										row.Cells["Accum"].Value = decimal.Parse(row.Cells["Accum"].Text) &amp;gt; decimal.Parse(row.Cells["Qty"].Text) ? decimal.Parse(row.Cells["Qty"].Text) : decimal.Parse(row.Cells["Accum"].Text);
									
									row.Cells["tempAccum"].Value = "0";
								}
							}
						}
						catch{}				
						
						bool first = true;
								
						//we may need to print, perhaps not, depends on if all lines for the particular job have been issued against
						if(ComponentRow.Cells["TFOrderOnly"].Text == "True")
						{
							string TFOrderNum = ComponentRow.Cells["JobNumber"].Text.ToString().Split('/')[0].Trim();
							PrintTFOrderPickTicket(TFOrderNum,true);
							//MessageBox.Show("PrintTFOrderPickTicket " +TFOrderNum );//TEST
							RefreshDemandInventory(TFOrderNum, ComponentRow.Cells["OrderNum"].Text, ComponentRow.Cells["OrderLine"].Text, ComponentRow.Cells["OrderRel"].Text, ComponentRow.Cells["AssemblySeq"].Text);	
							
						}
						else if(ComponentRow.Cells["OrderOnly"].Text == "True")
						{
							PrintPickTicket(ComponentRow.Cells["JobNumber"].Text, int.Parse(ComponentRow.Cells["OrderNum"].Text), int.Parse(ComponentRow.Cells["OrderLine"].Text),
								ComponentRow.Cells["AssemblySeq"].Text, true);
							PrintJobTraveler(ComponentRow.Cells["JobNumber"].Text, int.Parse(ComponentRow.Cells["OrderNum"].Text), int.Parse(ComponentRow.Cells["OrderLine"].Text),
								ComponentRow.Cells["AssemblySeq"].Text, true);
								
							RefreshDemandInventory(ComponentRow.Cells["OrderNum"].Text, ComponentRow.Cells["OrderLine"].Text, ComponentRow.Cells["OrderRel"].Text);	
						}
						else	
						{
							string printOption = "";
							
							ShouldPrint(ComponentRow.Cells["JobNumber"].Text, out printOption);
							
							bool shouldPrint = false;
							
							if(printOption == "Ask")
							{
								if(MessageBox.Show("All lines for this job have been issued but not all lines are fully issued.  Do  you wish to print the pick ticket and job traveler?", "Print?", MessageBoxButtons.YesNo) == DialogResult.Yes)
									shouldPrint = true;
							}
							else if(printOption == "Yes")
								shouldPrint = true;
							
							//if we are this far and shouldPrint == true, then print the pick ticket and job traveler, otherwise, no need	
							if(shouldPrint)
							{
								PrintPickTicket(ComponentRow.Cells["JobNumber"].Text, int.Parse(ComponentRow.Cells["OrderNum"].Text), int.Parse(ComponentRow.Cells["OrderLine"].Text),
									ComponentRow.Cells["AssemblySeq"].Text, false);
								PrintJobTraveler(ComponentRow.Cells["JobNumber"].Text, int.Parse(ComponentRow.Cells["OrderNum"].Text), int.Parse(ComponentRow.Cells["OrderLine"].Text),
									ComponentRow.Cells["AssemblySeq"].Text, false);
								
								if(ComponentRow.Cells["OrderOnly"].Text == "False")
									ReleaseJob(ComponentRow.Cells["JobNumber"].Text);								
								
								RefreshDemandInventory(ComponentRow.Cells["JobNumber"].Text, ComponentRow.Cells["OrderNum"].Text, ComponentRow.Cells["OrderLine"].Text, ComponentRow.Cells["OrderRel"].Text, ComponentRow.Cells["AssemblySeq"].Text);	
							}
							
							foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in ComponentGrid.Rows)
							{
								row.Cells["Sel"].Value = "False";
							}
						}
						//TW_JD 01202021 Start ************************
						//MessageBox.Show("Removing soft allocations: " + ComponentRow.Cells["JobNumber"].Text + "  " + ComponentRow.Cells["Component"].Text);
						//RemoveMBSoftAllocations(ComponentRow.Cells["JobNumber"].Text, ComponentRow.Cells["Component"].Text, Convert.ToDecimal(ComponentRow.Cells["Accum"].Text));
						RemoveMBSoftAllocations(Convert.ToInt32(ComponentRow.Cells["OrderNum"].Text), Convert.ToInt32(ComponentRow.Cells["OrderLine"].Text), Convert.ToInt32(ComponentRow.Cells["OrderRel"].Text), 
						ComponentRow.Cells["Component"].Text, Convert.ToDecimal(ComponentRow.Cells["Accum"].Text));
						//TW_JD 01202021 End   ************************
					}
				}
				else
				{
					MessageBox.Show("You must select at least one Inventory Location before you can issue inventory to this component.","Error");
				}
				
			}				
			else
			{
				MessageBox.Show("You must first select a component before you can issue inventory to it.","Error");
			}
			
			oTrans.PushStatusText("",false);
	}

//	private static decimal AllocateStock_DE(Infragistics.Win.UltraWinGrid.UltraGridRow ComponentRow, Infragistics.Win.UltraWinGrid.UltraGridRow InventoryRow )
//	{
//			return 0;
//	}

	private static decimal AllocateStock(Infragistics.Win.UltraWinGrid.UltraGridRow ComponentRow, string strSelected = "True")
	{

					decimal retValue = 0;
					
					Ice.Core.Session session = (Ice.Core.Session)oTrans.Session;
			
					using (var oa = Ice.Lib.Framework.WCFServiceSupport.CreateImpl &amp;lt;Erp.Proxy.BO.OrderAllocImpl&amp;gt;((Ice.Core.Session)oTrans.Session, Erp.Proxy.BO.OrderAllocImpl.UriPath)) 
					{ 
					using (var so = Ice.Lib.Framework.WCFServiceSupport.CreateImpl &amp;lt;Erp.Proxy.BO.SalesOrdHedDtlImpl&amp;gt;((Ice.Core.Session)oTrans.Session, Erp.Proxy.BO.SalesOrdHedDtlImpl.UriPath)) 
				
					{ 
						bool changeShipment = false;
						bool orderOnly = (bool)ComponentRow.Cells["OrderOnly"].Value ? true : false;
						int pOrderNum = (int)ComponentRow.Cells["OrderNum"].Value;
						int pOrderLine = (int)ComponentRow.Cells["OrderLine"].Value;
						int pOrderRel =  (int)ComponentRow.Cells["OrderRel"].Value;
						string pJobNum = ComponentRow.Cells["JobNumber"].Text.Trim();
						int pAssemblySeq =  0;
						
						bool TFOrderOnly = (bool)ComponentRow.Cells["TFOrderOnly"].Value ? true : false; //MT

						if (!orderOnly)
						{
							pAssemblySeq = (int)ComponentRow.Cells["AssemblySeq"].Value;
						}
						int pMtlSeq =  (int)ComponentRow.Cells["MaterialSeq"].Value;
						string pComponent =  ComponentRow.Cells["Component"].Text.Trim();
						
					
						OrderAllocDataSet dsAlloc = null;		
						bool pages;
						bool doReturn = false;
						string whereClause = "";
						decimal leftToReserve = 0;
						bool origShipOrderComplete = false;
					
						if(TFOrderOnly) //MT
						{
							whereClause = "TFOrdNum = '" + pJobNum.Split('/')[0].Trim() + "' and TFOrdLine = '" + pJobNum.Split('/')[1].Trim() + "'";
							//MessageBox.Show(whereClause); //TEST
						}  //MT
						else if (orderOnly)
							{

								
								SalesOrdHedDtlDataSet dsOrders = so.GetByID(pOrderNum);
								origShipOrderComplete = dsOrders.OrderHed[0].ShipOrderComplete;								
								dsOrders.OrderHed[0].ShipOrderComplete = false;
							    dsOrders.OrderHed[dsOrders.OrderHed.Count - 1].RowMod = "U";
								so.Update(dsOrders);
								changeShipment = true;

								whereClause = "OrderNum = " + pOrderNum.ToString() + " and OrderLine = " + pOrderLine.ToString() + " and OrderRelNum = " + pOrderRel.ToString();
							}
						else
							{
								whereClause = "JobNum = '" + pJobNum + "' and AssemblySeq = " + pAssemblySeq.ToString() + " and PartNum = '" + pComponent + "'";
							}

					//MessageBox.Show("JLP - About to OrderAllocationGetRows.  Where Clause: " + whereClause);
					dsAlloc = oa.OrderAllocationGetRows(oa.GetList(whereClause, 0, 0, out pages, session.CompanyID), 0);
					//MessageBox.Show("JLP - got rows for order allocation - rowcount = : " + dsAlloc.OrderAlloc.Rows.Count.ToString());
					if(TFOrderOnly) //MT
					{
						
						if (dsAlloc.OrderAlloc.Rows.Count &amp;gt; 0)
						{
							dsAlloc.OrderAlloc.Rows[0]["SelectedForAction"] = true;
							dsAlloc.OrderAlloc.Rows[0]["RowMod"] = "U";
							//MessageBox.Show("RemainingToReserve = " + dsAlloc.OrderAlloc.Rows[0]["RemainingToReserve"].ToString());
							leftToReserve = (Math.Truncate((Convert.ToDecimal(dsAlloc.OrderAlloc.Rows[0]["RemainingToReserve"]) * 100)) / 100);
							//MessageBox.Show("leftToReserve = " +leftToReserve.ToString());
						}
						else 
						{
							return 0;
						}
					} //MT
					else if (orderOnly)
					{
						if (dsAlloc.OrderAlloc.Rows.Count &amp;gt; 0)
						{
							dsAlloc.OrderAlloc.Rows[0]["SelectedForAction"] = true;
							dsAlloc.OrderAlloc.Rows[0]["RowMod"] = "U";
							leftToReserve = (Math.Truncate((Convert.ToDecimal(dsAlloc.OrderAlloc.Rows[0]["RemainingToReserve"]) * 100)) / 100);
						}
						else 
						{
							//doReturn = true;
							return 0;
						}
					}
					else
					{
						doReturn = true;
						foreach (DataRow dr in dsAlloc.OrderAlloc)
						{
							if (Convert.ToInt32(dr["MtlSeq"]) == pMtlSeq)
							{
								dr["SelectedForAction"] = true;
								dr["RowMod"] = "U";
								leftToReserve = (Math.Truncate((Convert.ToDecimal(dr["RemainingToReserve"]) * 100)) / 100);
								doReturn = false;
								break;
							}
						}
		
					}

					if (!doReturn)
					{
						string errors;
						string messages;
						oa.OneDemandType(dsAlloc,out errors, out messages);
		
						dsAlloc.PartAllocTran.Rows[0]["AddHoc"] = true;
		
						if (TFOrderOnly)
						{
								dsAlloc.OrderAlloc.Rows[0]["SelectedForAction"] = true;
								dsAlloc.OrderAlloc.Rows[0]["RowMod"] = "U";
						}else //MT
						if (orderOnly)
						{
								dsAlloc.OrderAlloc.Rows[0]["SelectedForAction"] = true;
								dsAlloc.OrderAlloc.Rows[0]["RowMod"] = "U";
						}
						else
						{
							foreach (DataRow dr in dsAlloc.OrderAlloc)
							{
								if (Convert.ToInt32(dr["MtlSeq"]) == pMtlSeq)
								{
									dr["SelectedForAction"] = true;
									dr["RowMod"] = "U";
									break;
								}
							}
						}
						
						if (TFOrderOnly)//MT
						{
							oa.GetLotBinOnHandByWhseCodeZoneBinType(dsAlloc,"","","Standard");
						}else{
						//MessageBox.Show("JLP - about to call oa.GetLotBinOnHandByWhseCode");
						oa.GetLotBinOnHandByWhseCode(dsAlloc,"");
						//MessageBox.Show("JLP - called getlotbinonhandbywhsecode");
						}
						decimal allocAmount = 0;
				
				 
				foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in LocationGrid.Rows)
				{		
					if(row.Cells["Sel"].Text == strSelected  )
					{
						//MessageBox.Show("JLP - Row Selected");

						Infragistics.Win.UltraWinGrid.UltraGridRow InventoryRow = row;
						string pWarehouse = InventoryRow.Cells["WarehouseCode"].Text;
						string pLocation = InventoryRow.Cells["Location"].Text;
						string pLot =  InventoryRow.Cells["Lot"].Text;
						string pBatch =  InventoryRow.Cells["Batch"].Text;



						foreach (DataRow partAllocLot in dsAlloc.PartAllocLot)
						{
										//MessageBox.Show("JLP - found record inpart alloc");

								bool lotInPartAlloc = false;
								if (partAllocLot["WarehouseCode"].ToString() == pWarehouse &amp;amp;&amp;amp; 
									partAllocLot["BinNum"].ToString() == pLocation &amp;amp;&amp;amp; 
									partAllocLot["LotNum"].ToString() == pLot &amp;amp;&amp;amp; 
									Convert.ToInt32(partAllocLot["MtlSeq"]) == pMtlSeq &amp;amp;&amp;amp; 
									Convert.ToInt32(partAllocLot["AssemblySeq"]) == pAssemblySeq &amp;amp;&amp;amp; 
									partAllocLot["PartNum"].ToString() == pComponent ) 
									{
											//MessageBox.Show("jLP Found Lot: " + pLot);
											//MessageBox.Show("JLP left to reserve: " + leftToReserve.ToString()); 
											//MessageBox.Show("JLP AvailableQty: " + Convert.ToDecimal(partAllocLot["AvailableQty"]).ToString()); 
											if (leftToReserve &amp;gt;= Convert.ToDecimal(partAllocLot["AvailableQty"]))
											{
												partAllocLot["Allocate"] = true; 
												partAllocLot["LotNum"] = pLot; 
												partAllocLot["BATCH"] = pBatch;
												partAllocLot["NewAllocatedQty"] = partAllocLot["AvailableQty"];
												//partAllocLot["AllocatedQty"] = partAllocLot["AvailableQty"];
												allocAmount = Convert.ToDecimal(partAllocLot["NewAllocatedQty"]);
												
												//JLP changing calculation for Left to Reserve
												leftToReserve = leftToReserve - allocAmount;
												//MessageBox.Show("jLP Found Lot: " + pLot);
												//MessageBox.Show("JLP left to reserve: " + leftToReserve.ToString()); 
											}
											else
											{
												//MessageBox.Show("JLP - left to reserve second: " + leftToReserve.ToString()); 
												partAllocLot["Allocate"] = true; 
												partAllocLot["LotNum"] = pLot; 
												partAllocLot["BATCH"] = pBatch;
												partAllocLot["NewAllocatedQty"] = leftToReserve;
												//partAllocLot["AllocatedQty"] = leftToReserve;
												allocAmount = leftToReserve;
												leftToReserve = 0;
			
											}
									}
								//JLP added this condition to reset allocated amounts from previously not being reset
								else if (((decimal)partAllocLot["NewAllocatedQty"] &amp;gt; 0) &amp;amp;&amp;amp; ((string)partAllocLot["LotNum"] != pLot))
										{
												 lotInPartAlloc = false;
												  foreach (DataRow partAlloc in dsAlloc.PartAlloc)
													{
														//MessageBox.Show("Found Part Alloc Record for: " + (string)partAlloc["Lotnum"]);
														if ((string)partAlloc["LotNum"] == (string)partAllocLot["LotNum"] )
															{
														    //MessageBox.Show("We found Lot Number in Part Alloc table:  " + (string)partAllocLot["LotNum"]);
															lotInPartAlloc = true;
															}
													}
												if (lotInPartAlloc)
												{
													 partAllocLot["Allocate"] = false;
													//need to create new allocation record for part
												}
										}
							}
					} //If row  selected
				} //Infragistics Selected Rows For Loop
				

				if (allocAmount &amp;gt; 0)
				{
					retValue = 1;
							//MessageBox.Show("JLP - alloc amount &amp;gt; 0");

					bool error = false;
					string xmlDataSet = dsAlloc.GetXml();
					txtDebugWindow.Text = xmlDataSet;


					
					try{
						//MessageBox.Show(dsAlloc.GetXml());
						//MessageBox.Show("JLP - about to call AllocateByLotBin");
						oa.AllocateByLotBin(dsAlloc,"","",out messages, out error);
						//MessageBox.Show(messages);
						}
					catch (Exception e)
						{ 
							oa.Dispose();
							throw new Exception(e.Message);
						}
					
						//MessageBox.Show("JLP - Allocate by lot bin finished");
					//MT
					if (TFOrderOnly)
					{
						retValue =1 ;
						dsAlloc.OrderAlloc.Rows[0]["SelectedForAction"] = true;
						dsAlloc.OrderAlloc.Rows[0]["RowMod"] = "U";
						oa.AutoPick(dsAlloc,"", out pages, out messages);
						
						dsAlloc.OrderAlloc.Rows[dsAlloc.OrderAlloc.Rows.Count - 1]["SelectedForAction"] = true;
						dsAlloc.OrderAlloc.Rows[dsAlloc.OrderAlloc.Rows.Count - 1]["RowMod"] = "U";
						int seq = 0;
						oa.SubmitForPicking(dsAlloc, 0, new DateTime(1901,1,1), false, out messages, out seq, out error);	
						
					 //MessageBox.Show(messages);
					}
					else if (orderOnly)
					{
						//MessageBox.Show("JLP - order only");
						retValue =1 ;
						dsAlloc.OrderAlloc.Rows[0]["SelectedForAction"] = true;
						dsAlloc.OrderAlloc.Rows[0]["RowMod"] = "U";
						oa.AutoPick(dsAlloc,"", out pages, out messages);
						//MessageBox.Show("JLP - performed AutoPick");

						dsAlloc.OrderAlloc.Rows[dsAlloc.OrderAlloc.Rows.Count - 1]["SelectedForAction"] = true;
						dsAlloc.OrderAlloc.Rows[dsAlloc.OrderAlloc.Rows.Count - 1]["RowMod"] = "U";
						int seq = 0;
						oa.SubmitForPicking(dsAlloc, 0, new DateTime(1901,1,1), false, out messages, out seq, out error);	
						//MessageBox.Show("JLP - called submit for picking");					

						markSalesAllocated(pOrderNum, pOrderLine, pOrderRel, true);
						//MessageBox.Show("JLP - changing shipordercomplete");					
						if (changeShipment)
						{
							SalesOrdHedDtlDataSet dsOrders = so.GetByID(pOrderNum);
							dsOrders.OrderHed[0].ShipOrderComplete = origShipOrderComplete;
						    dsOrders.OrderHed[dsOrders.OrderHed.Count - 1].RowMod = "U";
							so.Update(dsOrders);						
						}
					}
				else
				{
					if (session.PlantID == "MDT")
					{
						retValue = 1;
						dsAlloc.OrderAlloc.Rows[dsAlloc.OrderAlloc.Rows.Count - 1]["SelectedForAction"] = true;
						dsAlloc.OrderAlloc.Rows[dsAlloc.OrderAlloc.Rows.Count - 1]["RowMod"] = "U";
						oa.AutoPick(dsAlloc,"", out pages, out messages);

						dsAlloc.OrderAlloc.Rows[dsAlloc.OrderAlloc.Rows.Count - 1]["SelectedForAction"] = true;
						dsAlloc.OrderAlloc.Rows[dsAlloc.OrderAlloc.Rows.Count - 1]["RowMod"] = "U";
						int seq = 0;
						bool error2;
						oa.SubmitForPicking(dsAlloc, 0, new DateTime(1901,1,1), false, out messages, out seq, out error2);						

					}
				}
				}

			}

		}
			oa.Dispose();
		}
			
		return retValue;
		
	}

	private static void PrintPickTicket(string JobNum, int OrderNum, int OrderLine, string AssemblySeq, bool OrderOnly)
	{		
		Ice.Core.Session session = (Ice.Core.Session)oTrans.Session;
	
		string UserID =  session.UserID;
		
		string reportLocation = "";
		string dataLocation = "";
		string printerName = "";
		DataSet ds;
		
		if(OrderOnly)
		{
			ds = GetOrderAllocPickListData(OrderNum);
		
			ds.Tables[0].Columns.Add("UserID", typeof(string));
			
			foreach(DataRow dr in ds.Tables[0].Rows)
				dr["UserID"] = UserID;
		
			dataLocation = saveXML(ds);
		
            // BMB - Changed report Path to resolve errors.
			//reportLocation = FormFunctions.GetServerSideClientProgFolder(UD14Form) + "\\reports\\HabasitOrderAllocPick.rpt";
            reportLocation = "reports/CustomReports/HabasitOrderAllocPick.rpt";
			
			System.Windows.Forms.PrintDialog pd = new System.Windows.Forms.PrintDialog();					
			pd.UseEXDialog = true;					
			try
			{	
				DialogResult r = pd.ShowDialog();	
		
				if(r == DialogResult.OK)
				{					
					printerName = pd.PrinterSettings.PrinterName;
					int printCopy = 1;
					while(printCopy &amp;lt;= pd.PrinterSettings.Copies)
					{
						doPrint(dataLocation, reportLocation, printerName);
						printCopy++;
						System.Threading.Thread.Sleep(1000);
					}
				}
			}
			catch //this may fail because of invalid printer options.
			{
				//EpiMessageBox.Show(ex.ToString());
			}			
		}
		else
		{			
			using (DynamicQueryAdapter dy = new DynamicQueryAdapter(UD14Form))
			{
				dy.BOConnect();
				QueryExecutionDataSet parameters = new QueryExecutionDataSet();
				parameters.Tables["ExecutionParameter"].Rows.Add("JobNum",JobNum,"Character",false,null,"A"); 
				parameters.Tables["ExecutionParameter"].Rows.Add("AssemblySeq",AssemblySeq,"Integer",false,null,"A"); 
				string baq = "HabasitJobPrintListE10";
				dy.ExecuteByID(baq,parameters);

	    		//if (adDynamic.QueryResults.Tables[0].Rows.Count &amp;gt; 0) OrderComment = adDynamic.QueryResults.Tables[0].Rows[0][0].ToString();
			
			//DynamicQueryAdapter adapt = new DynamicQueryAdapter(UD14Form);
			//adapt.BOConnect();
		
			//adapt.GetByID(CompanyID + "-HabasitJobPrintList");

			//adapt.ExecuteByID(CompanyID + "-HabasitJobPrintList");
		
			//DataView dv = new DataView(adapt.QueryResults.Tables[0]);
		
			//dv.RowFilter = "JobMtl.JobNum = '" + JobNum + "' and JobMtl.AssemblySeq = '" + AssemblySeq + "'";		
			
			/*foreach(Ice.BO.QueryDesignDataSet.QueryWhereItemRow dr in data.QueryWhereItem)
			{
				if(dr.FieldName.ToUpper() == "JOBNUM")
				{
					dr.RValue = JobNum;
					dr.CompOp = "=";
				}
				else if(dr.FieldName.ToUpper() == "ASSEMBLYSEQ")
				{
					dr.RValue = AssemblySeq;
					dr.CompOp = "=";
				}
			}			*/
			
			ds = new DataSet();
			//ds.Tables.Add(dy.Execute(data).Tables[0].Copy());
			ds.Tables.Add(dy.QueryResults.Tables[0].Copy());
			//ds.Tables.Add(dv.ToTable());	
			//ds.Tables.Add(adapt.QueryResults.Tables[0].Copy());
			ds.Tables[0].Columns.Add("UserID", typeof(string));

			/*
			switch(((Ice.Core.Session)oTrans.Session).UserID.ToUpper())
			{
				case "AFFULL":
					printerName = @"\\haapp024\suwmodinv";
					break;
				case "THAOT":
					printerName = @"\\haapp024\suwmodinv";
					break;
				case "GREGORYC":
					printerName = @"\\haapp024\suwmodinv";		
					break;
				case "BEASLEYB":
					printerName = @"\\haapp024\suwmodinv";
					break;
				case "SLOAND":
					printerName = @"\\haapp024\suwmodinv";
					break;
				case "SETHA":
					printerName = @"\\haapp024\suwmodinv";
					break;
				case "SBRITO":
					printerName = @"\\haapp024\suwmodinv";
					break;
				case "ABDELA":
					printerName = @"\\haapp024\suwmodinv";
					break;
				case "BOUCHERL":
					printerName = @"\\haapp024\suwros2";
					break;
				case "FARROWS":
					printerName = @"\\haapp024\suwros2";
					break;
				case "PERKINSC":
					printerName = @"\\haapp024\suwros2";
					break;			
				
			}
			*/
			foreach(DataRow dr in ds.Tables[0].Rows)
				dr["UserID"] = UserID;
		
            // BMB - Changed report path to resolve errors.
			//reportLocation = FormFunctions.GetServerSideClientProgFolder(UD14Form) + "\\reports\\HabasitJobPick.rpt";		
		    reportLocation = "reports/CustomReports/HabasitJobPick.rpt";	

			dataLocation = saveXML(ds);
			
				System.Windows.Forms.PrintDialog pd = new System.Windows.Forms.PrintDialog();
				pd.UseEXDialog = true;		
				try
				{
						DialogResult r = pd.ShowDialog();	
		
						if(r == DialogResult.OK)
						{
							printerName = pd.PrinterSettings.PrinterName;	
							int printCopy = 1;
							while(printCopy &amp;lt;= pd.PrinterSettings.Copies)
							{
								doPrint(dataLocation, reportLocation, printerName);	
								printCopy++;
								System.Threading.Thread.Sleep(1000);
							}
						}	
				}
				catch (Exception ex)
				{					
					DialogResult r = pd.ShowDialog();	
		
					if(r == DialogResult.OK)
					{							
						printerName = pd.PrinterSettings.PrinterName;
						int printCopy = 1;
						
						while(printCopy &amp;lt;= pd.PrinterSettings.Copies)
						{
							doPrint(dataLocation, reportLocation, printerName);
							printCopy ++;
							System.Threading.Thread.Sleep(1000);
						}
					}			
				}		
			}
			//adapt.Dispose();
		}
		
	}

	[DllImport("kernel32.dll")]
	static extern bool ProcessIdToSessionId(uint dwProcessID, out uint pSessionId);


	private static void doPrint(string dataLocation, string reportLocation, string printerName)
	{

		try
			{
		Ice.Core.Session session = (Ice.Core.Session)oTrans.Session;
		
		ReportMonitorAdapter mon = new ReportMonitorAdapter(UD14Form);
		mon.BOConnect();
		
		//Ice.BO.ReportMonitorDataSet rs = new Ice.BO.ReportMonitorDataSet();
		
		int seconds = (DateTime.Now.Hour * 3600) + (DateTime.Now.Minute * 60) + DateTime.Now.Second;
		
		string workstationID = System.Environment.MachineName;
		
		uint pSession = 0;

		oTrans.PushStatusText("Printing...", false);		
		txtDebugWindow.Text = dataLocation;
		ProcessIdToSessionId((uint)Process.GetCurrentProcess().Id, out pSession);

		workstationID += " " + pSession.ToString("0"); //session.UserID;// + pSession.ToString("0");
		
		mon.GetNewSysRptLst(session.UserID, workstationID, DateTime.Now);
		
		mon.ReportMonitorData.SysRptLst[0].Archived = false;
		mon.ReportMonitorData.SysRptLst[0].Company = session.CompanyID;
		mon.ReportMonitorData.SysRptLst[0].FileName = dataLocation;
		mon.ReportMonitorData.SysRptLst[0].PrintDriver = "CRYSTAL";
		mon.ReportMonitorData.SysRptLst[0].PrintProgram = reportLocation;
		mon.ReportMonitorData.SysRptLst[0].RptDescription = "HA Pick List";

		if(printerName == "")
			mon.ReportMonitorData.SysRptLst[0].AutoAction = "Preview";
		else
		{
			mon.ReportMonitorData.SysRptLst[0].AutoAction = "Print";
			mon.ReportMonitorData.SysRptLst[0].PrinterName = printerName;
			mon.ReportMonitorData.SysRptLst[0].RptPageSettings = "Color=False,Landscape=False,Margins=[Left=100 Right=100 Top=100 Bottom=100],PaperSize=[Kind='Letter' Height=1100 Width=850],PaperSource=[SourceName='Auto Select' Kind='AutomaticFeed],PrinterResolution=[]";
			mon.ReportMonitorData.SysRptLst[0].RptPrinterSettings = "PrinterName=\"" + printerName + "\",Copies=1,Collate=False,Duplex=Simplex,FromPage=0,ToPage=0";
		}
		
		mon.Update();
		mon.Dispose();
		}

		catch (Exception e)
			{
				MessageBox.Show(e.Message);
			}
	}

	private static void PrintJobTraveler(string JobNum, int OrderNum, int OrderLine, string AssemblySeq, bool OrderOnly)
	{
		//to Print the Job Traveler, we just need to update the job header record checkbox01 = true, the BAM should do the rest of the work.
		
		if(!OrderOnly) //we can only print the job traveler if it is a job, orders do not have this
		{
			JobEntryAdapter adapt = new JobEntryAdapter(UD14Form);
			adapt.BOConnect();
		
			try //catch just in case there is an issue with finding/updating the job
			{
				adapt.GetByID(JobNum);
		
				Erp.BO.JobEntryDataSet ds = adapt.JobEntryData;
				
				if(!(bool)ds.JobHead[0]["CheckBox03"])
					ds.JobHead[0]["CheckBox03"] = true;
				else //this has already been printed once, first set it to false, then set it to true, so that it will fire off the BAM
				{
					ds.JobHead[0]["CheckBox03 "]= false;
					
					adapt.Update();
					
					ds.JobHead[0]["CheckBox03"] = true;
					
					adapt.Update();
				}
				
				adapt.Update();
			}	
			catch{}	
		
			adapt.Dispose();
		}
	}

	private static void RefreshDemandInventory(string JobNumber, string OrderNum, string OrderLine, string OrderRel, string AssemblySeq)
	{
		int index = 0;
		int found = -1;
		DataView dv = (DataView)epiUltraGrid1.DataSource;
		
		DataTable dt = dv.ToTable();
		
		foreach(DataRow dr in dt.Rows)
		{
			//MessageBox.Show(dr["Job Number"].ToString() + " - " + JobNumber + "\n" + dr["Order #"].ToString() + " - " + OrderNum + "\n" + dr["Order Line"].ToString() + " - " + OrderLine + "\n" + dr["Order Rel"].ToString() + " - " + OrderRel);
			if(dr["Job Number"].ToString() == JobNumber &amp;amp;&amp;amp; dr["Order #"].ToString() == OrderNum &amp;amp;&amp;amp; dr["Order Line"].ToString() == OrderLine &amp;amp;&amp;amp; dr["Order Rel"].ToString() == OrderRel &amp;amp;&amp;amp; dr["AssemblySeq"].ToString() == AssemblySeq)
				found = index;
			
			index++;
		}
		
		if(found &amp;gt; -1)
			dt.Rows.Remove(dt.Rows[found]);
			
		dv = new DataView(dt);
		
		epiUltraGrid1.DataSource = dv;
		epiUltraGrid1.DataBind();
		
		LocationGrid.DataSource = GetInventoryTable();
		LocationGrid.DataBind();
		
		foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in ComponentGrid.Rows)
			row.Cells["Sel"].Value = false;
	}
	
	private static void RefreshDemandInventory(string OrderNum, string OrderLine, string OrderRel)
	{
		bool allSelected = true;
		
		//first determine if this even needs to be cleared out by looking at all of the lines in the componentGrid, and seeing if all items of the specified order have been accumulated
		foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in ComponentGrid.Rows)
		{
			if(row.Cells["OrderNum"].Text == OrderNum &amp;amp;&amp;amp; decimal.Parse(row.Cells["Accum"].Text) &amp;lt;= 0)
				allSelected = false;
		}
		
		if(allSelected.Equals(true))
		{
			
			DataTable dtt = (DataTable)ComponentGrid.DataSource;
			DataView dvv = new DataView(dtt);
			dvv.RowFilter = "OrderNum = " + OrderNum;
			dtt = dvv.ToTable();
			
			foreach(DataRow drr in dtt.Rows)
			{
				int index = 0;
				int found = -1;
				DataView dv = (DataView)epiUltraGrid1.DataSource;
		
				DataTable dt = dv.ToTable();		
		
				foreach(DataRow dr in dt.Rows)
				{
					//MessageBox.Show(dr["Job Number"].ToString() + " - " + JobNumber + "\n" + dr["Order #"].ToString() + " - " + OrderNum + "\n" + dr["Order Line"].ToString() + " - " + OrderLine + "\n" + dr["Order Rel"].ToString() + " - " + OrderRel);
					if(dr["Order #"].ToString() == drr["OrderNum"].ToString() &amp;amp;&amp;amp; dr["Order Line"].ToString() == drr["OrderLine"].ToString() &amp;amp;&amp;amp; dr["Order Rel"].ToString() == drr["OrderRel"].ToString())
						found = index;
			
					index++;
				}
		
				if(found &amp;gt; -1)
					dt.Rows.Remove(dt.Rows[found]);
			
				dv = new DataView(dt);
		
				epiUltraGrid1.DataSource = dv;
				epiUltraGrid1.DataBind();
		
			}
		
			LocationGrid.DataSource = GetInventoryTable();
			LocationGrid.DataBind();
		
			foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in ComponentGrid.Rows)
				row.Cells["Sel"].Value = false;
		}
	}

	private static void ShouldPrint(string JobNum, out string ShouldPrint)
	{		
		ShouldPrint = "No"; //possible return values are "Yes", "No", "Ask"
		
		bool allIssued = true;
		bool FullAccum = true;
		
		foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in ComponentGrid.Rows)
		{
			if(row.Cells["Accum"].Text == "0")
				allIssued = false;
				
			if(decimal.Parse(row.Cells["Accum"].Text) &amp;lt; decimal.Parse(row.Cells["Qty"].Text))
				FullAccum = false;
		}
		
		if(!allIssued)
			ShouldPrint = "No";
		else if(allIssued &amp;amp;&amp;amp; !FullAccum)
			ShouldPrint = "Ask";
		else if(allIssued &amp;amp;&amp;amp; FullAccum)
			ShouldPrint = "Yes";
	}

	private static void ReleaseJob(string JobNumber)
	{	
		JobEntryAdapter ad = new JobEntryAdapter(UD14Form);
		ad.BOConnect();
	
		try
		{	
			ad.GetByID(JobNumber);
			
			ad.JobEntryData.JobHead[0].JobReleased = true;
			
			ad.Update();
		}
		catch{}
		finally{ad.Dispose();}
	}

