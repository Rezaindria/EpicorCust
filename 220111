	private static void cmdReprint_Click(object sender, System.EventArgs args)
	{
		bool selected = false;
		
		foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in ComponentGrid.Rows)
			if(row.Cells["Sel"].Text == "True")
			{				
				selected = true;
				
				//MT 
				if(row.Cells["TFOrderOnly"].Text == "True")
				{
					string TFOrderNum = row.Cells["JobNumber"].Text.ToString().Split('/')[0].Trim();
					PrintTFOrderPickTicket(TFOrderNum,true);
					//MessageBox.Show("PrintTFOrderPickTicket " +TFOrderNum ); //TEST
				}else{
					//reprint tickets/traverler
					PrintPickTicket(row.Cells["JobNumber"].Text, int.Parse(row.Cells["OrderNum"].Text), int.Parse(row.Cells["OrderLine"].Text),
					row.Cells["AssemblySeq"].Text, row.Cells["OrderOnly"].Text == "True" ? true : false);
					
					PrintJobTraveler(row.Cells["JobNumber"].Text, int.Parse(row.Cells["OrderNum"].Text), int.Parse(row.Cells["OrderLine"].Text),
					row.Cells["AssemblySeq"].Text, row.Cells["OrderOnly"].Text == "True" ? true : false);
				} 
			}
			
			if(!selected)
				MessageBox.Show("You must first select a component before you can reprint it.", "No Selection");
	}


	private static void cmdGetAllocated_Click(object sender, System.EventArgs args)
	{
		bool selected = false;
		
		foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in ComponentGrid.Rows)
		{
			if(row.Cells["Sel"].Text == "True")
			{
				selected = true;
				if(Convert.ToBoolean(row.Cells["TFOrderOnly"].Text.ToString()))
				{
					LocationGrid.DataSource = getAllocatedTFOrders(row);
					LocationGrid.DataBind();
				}else
				{
					LocationGrid.DataSource = getAllocated(row);
					LocationGrid.DataBind();
				}
				
				
				if(((DataTable)LocationGrid.DataSource).Rows.Count == 0)
					MessageBox.Show("There are no allocated records for this component.", "Get Allocated Rows");
					
				allocate = false;
				//cmdIssue.Enabled = false;
				//cmdDeAllocated.Enabled = true;
				cmdIssue.ReadOnly = true;
				cmdDeAllocated.ReadOnly = false;
			}
		}
		
		if(!selected)
			MessageBox.Show("You must first select a component before getting already allocated rows.","Get Allocated Rows");
	}


	private static DataTable getAllocated(Infragistics.Win.UltraWinGrid.UltraGridRow row)
	{
		bool OrderOnly = row.Cells["OrderOnly"].Text == "True" ? true : false;
		
		DataTable dt = GetInventoryTable();
		
		SqlConnection dbConn = new SqlConnection(sqlConnection);
		
		System.Text.StringBuilder sb = new System.Text.StringBuilder();
		
		sb.Append("select UD11.Key1, UD11.Key2, UD11.Key3, UD11.Key4, UD11.Key5, UD11.Number01, UD11.Number02, UD11.Number03, UD11.Number05, UD11.ShortChar01, UD11.ShortChar02, UD11.ShortChar03, UD11.ShortChar04, UD11.ShortChar05, UD11.ShortChar06, UD11.ShortChar07, UD11.Date01, UD11.Date02, isnull(PartAlloc.PickingQty,0) as [JobAlloc.PickingQty], isnull(PartAlloc.PickedQty,0) as [JobAlloc.PickedQty], isnull(PartAlloc.AllocatedQty,0) as [JobAlloc.AllocatedQty], isnull(PartAlloc.ReservedQty,0) as [JobAlloc.ReservedQty], PartAlloc.WarehouseCode as [JobAlloc.WarehouseCode], PartAlloc.binNum as [JobAlloc.BinNum], PartAlloc.PartNum as [JobAlloc.PartNum], isnull(PartAlloc.AssemblySeq,0) as [JobAlloc.AssemblySeq], isnull(PartAlloc.MtlSeq,0) as [JobAlloc.MtlSeq], ordAlloc.PartNum, isnull(ordAlloc.PickingQty,0) as PickingQty, isnull(ordAlloc.PickedQty,0) as PickedQty, isnull(ordAlloc.AllocatedQty,0) as AllocatedQty, isnull(ordAlloc.ReservedQty,0) as ReservedQty, ordAlloc.WarehouseCode, ordAlloc.BinNum from Ice.UD11 left outer join PartAlloc (nolock) on PartAlloc.Company = UD11.Company and PartAlloc.JobNum = UD11.Key2 and PartAlloc.PartNum = UD11.Key1 and PartAlloc.WarehouseCode = UD11.ShortChar02 and PartAlloc.BinNum = UD11.ShortChar03 and PartAlloc.LotNum = UD11.ShortChar04 left outer join PartAlloc ordAlloc (nolock) on ordAlloc.Company = UD11.Company and ordAlloc.OrderNum = UD11.Number01 and ordAlloc.OrderLine = UD11.Number02 and ordAlloc.OrderRelNum = UD11.Number03 and ordAlloc.PartNum = UD11.Key1 and ordAlloc.WarehouseCode = UD11.ShortChar02 and ordAlloc.BinNum = UD11.ShortChar03 and ordAlloc.LotNum = UD11.ShortChar04 where UD11.Key3 = 'Belt' ");
		 
		if(!OrderOnly)
		{
			sb.Append(" and UD11.Key2 = '").Append(row.Cells["JobNumber"].Text.Trim()).Append("'");
			sb.Append(" and UD11.ShortChar05 = '").Append(row.Cells["AssemblySeq"].Text.Trim()).Append("'");
			sb.Append(" and UD11.ShortChar06 = '").Append(row.Cells["MaterialSeq"].Text.Trim()).Append("'");
		}
		else
		{
			sb.Append(" and UD11.Number01 = ").Append(row.Cells["OrderNum"].Text.Trim());
			sb.Append(" and UD11.Number02 = ").Append(row.Cells["OrderLine"].Text.Trim());
			sb.Append(" and UD11.Number03 = ").Append(row.Cells["OrderRel"].Text.Trim());
		}
		
		//MessageBox.Show("JLP - getting allocated: " + sb.ToString());

		SqlCommand sqlCom = new SqlCommand(sb.ToString(), dbConn);
		
		SqlDataAdapter da = new SqlDataAdapter(sqlCom);
		
		DataTable data = new DataTable();
		
		da.Fill(data);	
		
		DataRow newRow;
		
		foreach(DataRow temp in data.Rows)
		{
			newRow = dt.NewRow();
			
			newRow["Sel"] = false;
			newRow["Batch"] = temp["ShortChar07"].ToString();
			try
			{
				newRow["ReceiptDate"] = (DateTime)temp["Date01"];
			}
			catch{}
			
			try
			{
				newRow["Age"] = (DateTime)temp["Date02"];			
			}
			catch{}
			
			newRow["PartNum"] = temp["Key1"].ToString();
			newRow["JobNumber"] = temp["Key2"].ToString();
			newRow["Lot"] = temp["ShortChar04"].ToString();
			newRow["WarehouseCode"] = temp["Key5"].ToString();
			try
			{
			newRow["OrderNum"] = int.Parse(temp["Number01"].ToString());
			newRow["OrderLine"] = int.Parse(temp["Number02"].ToString());			
			newRow["OrderRel"] = int.Parse(temp["Number03"].ToString());
			}
			catch{}
			newRow["AssemblySeq"] = int.Parse(temp["ShortChar05"].ToString());
			newRow["MaterialSeq"] = int.Parse(temp["ShortChar06"].ToString());
			newRow["Default"] = false;
			newRow["Plant"] = temp["ShortChar01"].ToString();
			try
			{
			newRow["RowID"] = int.Parse(temp["Key4"].ToString());
			}
			catch{}
			newRow["Location"] = temp["ShortChar03"].ToString();
			
			decimal parser = 0;
			decimal total = 0;
			if(OrderOnly)
			{
				decimal.TryParse(temp["ReservedQty"].ToString(), out parser);
				
				total += parser;
				
				decimal.TryParse(temp["AllocatedQty"].ToString(), out parser);		
				
				total += parser;
				
				decimal.TryParse(temp["PickingQty"].ToString(), out parser);
				
				total += parser;
				
				decimal.TryParse(temp["PickedQty"].ToString(), out parser);
				
				newRow["OH Qty"] = total;			
				
			}
			else
			{
				if(newRow["Plant"].ToString() == "SUW" &amp;amp;&amp;amp; newRow["WarehouseCode"].ToString() == "01PSTK")
				{
					decimal.TryParse(temp["Number05"].ToString(), out parser);
					newRow["OH Qty"] = parser;					
				}
				else
				{
					decimal.TryParse(temp["JobAlloc.ReservedQty"].ToString(), out parser);
				
					total += parser;
				
					decimal.TryParse(temp["JobAlloc.AllocatedQty"].ToString(), out parser);		
				
					total += parser;
				
					decimal.TryParse(temp["JobAlloc.PickingQty"].ToString(), out parser);
				
					total += parser;
				
					decimal.TryParse(temp["JobAlloc.PickedQty"].ToString(), out parser);
				
					newRow["OH Qty"] = total;			
					
				}
			}
			
			dt.Rows.Add(newRow);
		}
		
		return dt;
	}

	private static void cmdDeAllocated_Click(object sender, System.EventArgs args)
	{
		bool selected = false;
		
		foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in LocationGrid.Rows)		
			if(row.Cells["Sel"].Text == "True")
			{
				selected = true;
			}
			
		if(selected)
		{
			//at least one was selected to de-allocated
			
			oTrans.PushStatusText("Step 1: Find Component Row", true);
			/*Step 1:
				find the component row*/
				
			Infragistics.Win.UltraWinGrid.UltraGridRow ComponentRow = null;
			
			foreach(Infragistics.Win.UltraWinGrid.UltraGridRow r in ComponentGrid.Rows)			
				if(r.Cells["Sel"].Text == "True")
				{
					ComponentRow = r;
					
					//make sure that the plant that they are currently logged into is the plant for the component row
					if(r.Cells["Plant"].Text != ((Ice.Core.Session)UD14Form.Session).PlantID)
					{
						MessageBox.Show("The plant that you are currently logged into does not match the plant for the selected job/order.  You are unable to deallocate inventory.", "Plant Mis-match");
						return;
					}
				}
			
			if(ComponentRow != null)
			{
				oTrans.PushStatusText("Step 2: Deallocate Row", true);
				/*Step 2:
					De-allcoate the entire line
					We cannot just deallocate just part of the line, the entirety must be deallocated*/
				DeAllocate(ComponentRow);
				
				oTrans.PushStatusText("Step 3: Delete extra Records", true);
				try{
					/*Step 3:
					Go through each record and delete out the  unneeded UD11 records
					This step will also delete out the part alloc records that were not cleaned up
					from the unallocation setup*/
					foreach(Infragistics.Win.UltraWinGrid.UltraGridRow r in LocationGrid.Rows)
						if(r.Cells["Sel"].Text == "True")
							DeleteUD11Record(r);
				}catch{}
				
				try
				{
					/*Step 3.1 TFOrders:
					Go through each record and delete out the  unneeded UD11 records
					This step will also delete out the part alloc records that were not cleaned up
					from the unallocation setup*/
					foreach(Infragistics.Win.UltraWinGrid.UltraGridRow r in LocationGrid.Rows)
						if(r.Cells["Sel"].Text == "True")
							DeleteUD11TFOrderRecord(r);
				}catch{}
					
				 
				oTrans.PushStatusText("Step 4: Re-Allocating", true);	
				/*Step 4:
					Re-allocate and send to picking the lines that were not selected*/
					foreach(Infragistics.Win.UltraWinGrid.UltraGridRow r in LocationGrid.Rows)
						if(r.Cells["Sel"].Text == "False")
							if (ComponentRow.Cells["TFOrderOnly"].Text == "True")
								AllocateStock(ComponentRow, "False");
							else
								AllocateStock(ComponentRow);
				
				oTrans.PushStatusText("Step 5: Check Allocation Records", true);
				/*Step 5:
					determine if the order line needs to be marked as deallocated (CheckBox06 = false)*/
					if((bool)ComponentRow.Cells["OrderOnly"].Value == true)
						markSalesAllocated((int)ComponentRow.Cells["OrderNum"].Value, (int)ComponentRow.Cells["OrderLine"].Value, (int)ComponentRow.Cells["OrderRel"].Value, false);
				
				oTrans.PushStatusText("Step 6: Refresh Data", true);		
				/*Step 6:
					Re-click the get-allocated button to refresh everything*/
					cmdGetAllocated_Click(null, null);
					
			}
			else
				MessageBox.Show("You must first select a component row before you can de-allocate it.", "De-Allocate Inventory");
				
		}
		else
			MessageBox.Show("You must first select at least one inventory row to De-Allocated it.", "De-Allocate Inventory");
		
		oTrans.PushStatusText("", false);
	}

	private static void DeAllocate(Infragistics.Win.UltraWinGrid.UltraGridRow ComponentRow)
	{
			bool pages = false;
			string message = "";
			string error = "";

			Ice.Core.Session ses = (Ice.Core.Session)UD14Form.Session;
			//MessageBox.Show("JLP - Deallocate Procudure");

		using (var oa = Ice.Lib.Framework.WCFServiceSupport.CreateImpl &amp;lt;Erp.Proxy.BO.OrderAllocImpl&amp;gt;((Ice.Core.Session)oTrans.Session, Erp.Proxy.BO.OrderAllocImpl.UriPath)) 
		{ 
//			Ice.BO.OrderAlloc oa = new Ice.BO.OrderAlloc(ses.ConnectionPool);

			OrderAllocDataSet ds = null;		
			if (ComponentRow.Cells["TFOrderOnly"].Text == "True")
			{
				string pJobNum = ComponentRow.Cells["JobNumber"].Text;
				ds = oa.OrderAllocationGetRows(oa.GetList("TFOrdNum = '" + pJobNum.Split('/')[0].Trim() + "' and TFOrdLine = '" + pJobNum.Split('/')[1].Trim() + "'", 0, 0, out pages, ses.CompanyID), 0);
			}
			else if (ComponentRow.Cells["OrderOnly"].Text == "True")
				ds = oa.OrderAllocationGetRows(oa.GetList("OrderNum = " + ComponentRow.Cells["OrderNum"].Text + " and OrderLine = " + ComponentRow.Cells["OrderLine"].Text + " and OrderRelNum = " + ComponentRow.Cells["OrderRel"].Text, 0, 0, out pages, ses.CompanyID), 0);
			else
				ds = oa.OrderAllocationGetRows(oa.GetList("JobNum = '" + ComponentRow.Cells["JobNumber"].Text + "' and AssemblySeq = " + ComponentRow.Cells["AssemblySeq"].Text + " and PartNum = '" + ComponentRow.Cells["Component"].Text + "'", 0, 0, out pages, ses.CompanyID), 0);

			foreach (OrderAllocDataSet.OrderAllocSupplyRow row in ds.OrderAllocSupply)
			{
				if (row.AssemblySeq.ToString() == ComponentRow.Cells["AssemblySeq"].Text &amp;amp;&amp;amp; row.MtlSeq.ToString() == ComponentRow.Cells["MaterialSeq"].Text)
					row.RowMod = "U";
			}

			foreach (OrderAllocDataSet.OrderAllocRow row in ds.OrderAlloc)
			{
				if (row.AssemblySeq.ToString() == ComponentRow.Cells["AssemblySeq"].Text &amp;amp;&amp;amp; row.MtlSeq.ToString() == ComponentRow.Cells["MaterialSeq"].Text)
				{
					row.SelectedForAction = true;
					row.OrderFulfillmentPct = 1;
					row.RowMod = "U";
				}
			}					

			//MessageBox.Show("JLP - About to un allocate");				
			oa.UnallocateAndUnreserve(ds, out message);
			//MessageBox.Show("JLP - unallocate message:" + message);
			string xmlDataSet = ds.GetXml();

			txtDebugWindow.Text = xmlDataSet;
			//MessageBox.Show("JLP - finished un allocate");
		}
	}

	private static void DeleteUD11Record(Infragistics.Win.UltraWinGrid.UltraGridRow InventoryRow)
	{
		UD11Adapter ud = new UD11Adapter(UD14Form);
		ud.BOConnect();
		
		ud.DeleteByID(InventoryRow.Cells["PartNum"].Text, InventoryRow.Cells["JobNumber"].Text, "Belt", InventoryRow.Cells["RowID"].Text, InventoryRow.Cells["WarehouseCode"].Text);
		
		ud.Dispose();
	}

	private static void markSalesAllocated(int OrderNum, int OrderLine, int OrderRel, bool allocated)
	{
		/*E10
		Ice.Core.Session session = (Ice.Core.Session)oTrans.Session;
		
		//Progress.Open4GL.DynamicAPI.Session progressSession = session.ConnectionPool.Get();
		Ice.Proxy.BO.GetImpl progressSession = WCFServiceSupport.CreateImpl&amp;lt;Ice.Proxy.BO.GetImpl&amp;gt;((Ice.Core.Session)oTrans.Session, Ice.Proxy.BO.GetImpl.UriPath);
		
		PersistentProc proc = default(PersistentProc);
		
		ParameterSet parms = new ParameterSet(4);
		
		parms.setIntegerParameter(1, OrderNum, ParameterSet.INPUT);
		parms.setIntegerParameter(2, OrderLine, ParameterSet.INPUT);
		parms.setIntegerParameter(3, OrderRel, ParameterSet.INPUT);
		parms.setBooleanParameter(4, allocated, ParameterSet.INPUT);
		
		try
		{
			proc = progressSession.runPersistentProcedure(@"ud\AllocWorkbench\MarkSalesAlloc.p", parms);
			
			proc.delete();
		}
		catch(Exception ex)
		{
			EpiMessageBox.Show(ex.ToString(), "Allocatd Sales Order");
		}
		finally
		{
			//session.ConnectionPool.Release(progressSession);
			Ice.Proxy.BO.ReleaseImpl  = WCFServiceSupport.CreateImpl&amp;lt;Ice.Proxy.BO.ReleaseImpl&amp;gt;((Ice.Core.Session)oTrans.Session, Ice.Proxy.BO.ReleaseImpl.UriPath);
		}


		IF allocated = TRUE THEN allowUpdate = TRUE.
		ELSE DO:
		    IF CAN-FIND(FIRST PartAlloc WHERE PartAlloc.company = cur-comp AND PartAlloc.OrderNum = orderNumber AND partAlloc.OrderLIne = orderLineNum AND PartAlloc.OrderRelNum = OrderRelNumber NO-LOCK) THEN allowUpdate = FALSE.
		    ELSE allowUpdate = TRUE.
		END.
		
		IF allowUpdate = TRUE THEN DO:
		    FOR FIRST OrderRel WHERE OrderRel.Company = cur-comp AND OrderRel.OrderNum = orderNumber AND OrderRel.OrderLine = orderLineNum AND OrderRel.OrderRelNum = orderRelNumber EXCLUSIVE-LOCK.
		        OrderRel.CheckBox06 = allocated.
		    END.
		END.


		*/
		bool allowUpdate = false;

		if (allocated )
		{
			allowUpdate = true;
		}
		else
		{
			using (DynamicQueryAdapter dy = new DynamicQueryAdapter(UD14Form))
			{		
				dy.BOConnect();
				QueryExecutionDataSet parameters = new QueryExecutionDataSet();
				string baq = "AllocPartAlloc";
				parameters.Tables["ExecutionParameter"].Rows.Add("OrderNum",OrderNum,"Integer",false,null,"A");
				parameters.Tables["ExecutionParameter"].Rows.Add("OrderLine",OrderLine,"Integer",false,null,"A");
				parameters.Tables["ExecutionParameter"].Rows.Add("OrderRel",OrderRel,"Integer",false,null,"A");
				dy.ExecuteByID(baq,parameters);
	
				if (dy.QueryResults.Tables[0].Rows.Count == 0) allowUpdate = true;
			}

		}

		if (allowUpdate)
		{
			using (var so = Ice.Lib.Framework.WCFServiceSupport.CreateImpl &amp;lt;Erp.Proxy.BO.SalesOrderImpl&amp;gt;((Ice.Core.Session)oTrans.Session, Erp.Proxy.BO.SalesOrderImpl.UriPath)) 
			{ 
				//SalesOrder so = new Ice.BO.SalesOrder(session.ConnectionPool);
				
				Erp.BO.SalesOrderDataSet ds = so.GetByID(OrderNum);
				
				foreach(Erp.BO.SalesOrderDataSet.OrderRelRow row in ds.OrderRel.Select("OrderLine = " + OrderLine.ToString() + " and OrderRelNum = " + OrderRel.ToString()))
					row["CheckBox06"] = allocated;
					
				so.Update(ds);
				so.Dispose();
			}
		}



	}

	private static void LocationGrid_AfterSortChange(object sender, Infragistics.Win.UltraWinGrid.BandEventArgs args)
	{		
		foreach(Infragistics.Win.UltraWinGrid.UltraGridColumn c in args.Band.SortedColumns)
		{
			//MessageBox.Show(c.Key.ToString());
			//MessageBox.Show(c.SortIndicator.ToString());
			if(c.Key.ToString() == "OH Qty" &amp;amp;&amp;amp; doSort.Equals(true))
			{				
				doSort = false;
				
				LocationGrid.DisplayLayout.Bands[0].Columns["OH Qty"].SortComparer = new srtComparer();
				LocationGrid.DisplayLayout.Bands[0].Columns["OH Qty"].SortIndicator = c.SortIndicator;
				LocationGrid.DisplayLayout.Bands[0].Columns["OH Qty"].Band.SortedColumns.RefreshSort(true);
				LocationGrid.Refresh();				
			}
			else
			{
				doSort = true;
			}
		}
		
		
		//MessageBox.Show(args.Column.ToString());
		//e.Cancel = true;
	}

	private static void cmdRemoveOutOfStock_Click(object sender, System.EventArgs args)
	{
		string OrderNum = "";
		string OrderLine = "";
		string OrderRel = "";
		string JobNum = "";
		string AssemblySeq = "";
		string MaterialSeq = "";
		bool OrderOnly = false;
		string PartNum = "";
		
		bool TFOrderOnly = false;

		bool selected = false;
		
		Infragistics.Win.UltraWinGrid.UltraGridRow rr = null;
		
		foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in ComponentGrid.Rows)
			if(row.Cells["Sel"].Text == "True")
			{
				
				//make sure that the plant that has been selected matches the plant that they are currently logged into
				if(((Ice.Core.Session)UD14Form.Session).PlantID != row.Cells["Plant"].Text)
				{
					MessageBox.Show("The Plant that you are currently logged into is not the selected plant for this inventory line.  Unable to generate the cross dock.", "Plant Mis-match");
					return;
				}
				
				selected = true;
				OrderNum = row.Cells["OrderNum"].Text;
				OrderLine = row.Cells["OrderLine"].Text;
				OrderRel = row.Cells["OrderRel"].Text;
				JobNum = row.Cells["JobNumber"].Text;
				AssemblySeq = row.Cells["AssemblySeq"].Text;
				MaterialSeq = row.Cells["MaterialSeq"].Text;
				OrderOnly = row.Cells["OrderOnly"].Text == "True" ? true : false;
				PartNum = row.Cells["Component"].Text.Trim();
				TFOrderOnly = row.Cells["TFOrderOnly"].Text == "True" ? true : false;  
				rr = row;
			}
		
		if(!selected)
		{
			MessageBox.Show("You must first selected a component before generating a cross dock.","No Component Selected");
			oTrans.PushStatusText("", false);
			return;
		}
		
		//once we are at this point, we should be able to make the necessary updates.
		Ice.Core.Session session = (Ice.Core.Session)oTrans.Session;
		if(TFOrderOnly)//MT
			return;

		if(OrderOnly)
		{
			//the line is order only, update the release
			using (var so = Ice.Lib.Framework.WCFServiceSupport.CreateImpl &amp;lt;Erp.Proxy.BO.SalesOrderImpl&amp;gt;((Ice.Core.Session)oTrans.Session, Erp.Proxy.BO.SalesOrderImpl.UriPath)) 
			{ 
				//SalesOrder so = new Ice.BO.SalesOrder(session.ConnectionPool);
				
				Erp.BO.SalesOrderDataSet ds = so.GetByID(int.Parse(OrderNum));
				
				foreach(Erp.BO.SalesOrderDataSet.OrderRelRow row in ds.OrderRel.Select("OrderLine = " + OrderLine.ToString() + " and OrderRelNum = " + OrderRel.ToString()))
					row["CheckBox04"] = false;
					
				so.Update(ds);
			}
		}
		else
		{
			//this is a job, there is a bit more involved with this method, but, we can do it
			using (var job = Ice.Lib.Framework.WCFServiceSupport.CreateImpl &amp;lt;Erp.Proxy.BO.JobEntryImpl&amp;gt;((Ice.Core.Session)oTrans.Session, Erp.Proxy.BO.JobEntryImpl.UriPath)) 
			{ 
//			Ice.BO.JobEntry job = new Ice.BO.JobEntry(session.ConnectionPool);
			
				Erp.BO.JobEntryDataSet jobDS = job.GetByID(JobNum);
				
				jobDS.JobHead[0].JobEngineered = true;
				
				job.ChangeJobHeadJobEngineered(jobDS);
				
				jobDS.JobHead[0].JobReleased = true;
				
				job.ChangeJobHeadJobReleased(jobDS);
				
				jobDS.JobHead[0]["CheckBox04"] = false;
				
				job.Update(jobDS);
			}
		}		
	}
	
	private static decimal GetAccumTFOrder(string PartNumber, string JobNumber, bool TFOrderOnly)
	{
	    decimal AccumQty = 0;


	

		using (DynamicQueryAdapter dy = new DynamicQueryAdapter(UD14Form))
		{		
			dy.BOConnect();
			QueryExecutionDataSet parameters = new QueryExecutionDataSet();
			string baq = "";
			
			if(TFOrderOnly)
			{
				string TFOrderNum = JobNumber.Split('/')[0].Trim();
				int TFOrderLine = Convert.ToInt32(JobNumber.Split('/')[1].Trim());
				parameters.Tables["ExecutionParameter"].Rows.Add("TFOrderNum",TFOrderNum,"Character",false,null,"A");
				parameters.Tables["ExecutionParameter"].Rows.Add("TFOrderLine",TFOrderLine,"Integer",false,null,"A");
				parameters.Tables["ExecutionParameter"].Rows.Add("PartNum",PartNumber,"Character",false,null,"A");
				baq = "TFOrderAllocPartAlloc";
				dy.ExecuteByID(baq,parameters);

				foreach (DataRow dr in dy.QueryResults.Tables[0].Rows)
				{
					AccumQty = AccumQty + Convert.ToDecimal(dr["Calculated_Qty"]);
				}
			}
		}
		return AccumQty;
	}


	private static void FormatGrids()
	{
		epiUltraGrid1.DisplayLayout.Bands[0].Columns[2].Format = "###,##0.000";
		ComponentGrid.DisplayLayout.Bands[0].Columns[2].Format = "###,##0";
		LocationGrid.DisplayLayout.Bands[0].Columns[3].Format = "###,##0.00";
	}

	private static void fillTFOrdsDemandSql()
	{
		SqlConnection dbConn = new SqlConnection(sqlConnection);
		
		System.Text.StringBuilder sb = new System.Text.StringBuilder();

		string includeUD11 = "";
		string joinType = "";
		if(!chkSelected.Checked)
		{
			includeUD11 = " and UD11.Key1 is null ";
			joinType = "left outer join ";
		}
		else
			joinType = "inner join ";
		
        sb.Append("SELECT DISTINCT " 
        + " Cast(0 AS BIT) AS selected, " 
        + " (Cast(TFOrdDtl.TFOrdNum as Varchar(25)) + ' / ' + " 
        + " Cast(TFOrdDtl.TFOrdLine as Varchar(25)) + ' / ')     AS [Job Number], " 
        + " TFOrdDtl.OurStockQty AS Qty, " 
        + " TFOrdDtl.RequestDate AS [Start Date],   " 
        + " (Part.PartNum) AS Part, " 
        + " null AS [Due Date],     " 
        + " TFOrdDtl.ContractID AS [Contract],     " 
        + " ''   AS Code, " 
        + " ''   AS Style, " 
        + " ''   AS [Color], " 
        + " 0    AS Width, " 
        + " 0    AS Length, " 
        + " ''   AS Material,   " 
        + " 0   AS[Order #],   " 
		+ " 0   AS[Order Line],   " 
        + " 0   AS[Order Rel],    " 
        + " 'N'  AS CM, " 
        + " ''   AS SB, " 
        + " Cast(0 AS BIT) AS BTO, " 
        + " Cast(0 AS BIT) AS DS, " 
        + " 0    AS RowID, " 
        + " ''   AS Plant, " 
        + " ''   AS WarehouseCode, " 
        + " 0   AS[AssemblySeq],  " 
        + " Cast(0 AS BIT)  AS OrderOnly, "
		+ " Cast(1 AS BIT)  AS TFOrderOnly, " 
        + " ''  AS ColorCode, " 
        + " ''  AS MaterialCode " 
        + " FROM Erp.TFOrdDtl as TFOrdDtl   " 
		+ joinType 
		+ " Ice.UD11 (nolock) "  
		+ " on UD11.Company = TFOrdDtl.Company " + includeUD11 
		+ " and UD11.Character02 = TFOrdDtl.TFOrdNum " 
		+ " and UD11.Number07 = TFOrdDtl.TFOrdLine " 
		+ " and UD11.Key3 = 'Transfer' " 
        + " INNER JOIN Erp.Part as Part on TFOrdDtl.Company = Part.Company  "
        + " And  TFOrdDtl.PartNum = Part.PartNum   "
		+ " WHERE ");

			
	 	if (!chkSelected.Checked)
			sb.Append("NOT EXISTS (select * from Ice.UD11 where UD11.Company = TFOrdDtl.Company and UD11.Key1 = TFOrdDtl.PartNum and UD11.Character02 = TFOrdDtl.TFOrdNum and UD11.Number07 = TFOrdDtl.TFOrdLine ) and ");
		

		sb.Append("TFOrdDtl.OpenLine = 1 "); 
		/*sb.Append("and JobProd.OrderNum is null "); 
	    // BMB - allow credit holds
		//"and (Customer.CreditHold = 0 or OrderHed.CreditOverride = 1) " + */
		sb.Append("and TFOrdDtl.PartNum not in (select PartRev.PartNum from PartRev (nolock) where PartRev.Configured = 1) "); 
		sb.Append("and TFOrdDtl.OurStockQty &amp;gt; 0");
		
		sb.Append(" and (TFOrdDtl.WarehouseCode = '").Append(epiUltraCombo2.Value.ToString()).Append("' and (TFOrdDtl.Plant = '' OR TFOrdDtl.Plant = '").Append(epiUltraCombo1.Value.ToString()).Append("'))");
						
		try
		{
			DateTime startDate = DateTime.Parse(epiDueDate.Value.ToString());
			DateTime endDate = DateTime.Parse(epiToDueDate.Value.ToString());
			
			sb.Append(" and (TFOrdDtl.RequestDate &amp;gt;= '").Append(startDate.ToString("MM/dd/yyyy")).Append("'");
			sb.Append(" and TFOrdDtl.RequestDate &amp;lt;= '").Append(endDate.ToString("MM/dd/yyyy")).Append("')");
		}
		catch
		{}
			
		if(txtOrderNum.Text.Trim() != "")
			sb.Append(" and TFOrdDtl.TFOrdNum = '").Append(txtOrderNum.Text.Trim()).Append("'");
		if(txtLine.Text.Trim() != "")
			sb.Append(" and TFOrdDtl.TFOrdLine = '").Append(txtLine.Text.Trim()).Append("'");
			
		/* 
		if(chkSelected.Checked)
			sb.Append(" and UD11.Key1 is not null");
		*/	
			
		 
			if(((Ice.Core.Session)oTrans.Session).UserID.ToUpper() == "MANAGER")
				{
					txtInfo.Text = sb.ToString();
					txtInfo.Visible = true;
					 
					
				}	



		SqlCommand sqlCom = new SqlCommand(sb.ToString(), dbConn);
		
		SqlDataAdapter da = new SqlDataAdapter(sqlCom);
		
		DataTable dt = new DataTable();
		
		da.Fill(dt);
		
		//fill row/plant/warehouse data
		int i = 0;
		foreach(DataRow dr in dt.Rows)
		{
			dr["RowID"] = i;
			dr["Plant"] = epiUltraCombo1.Value.ToString();
			dr["WarehouseCode"] = epiUltraCombo2.Value.ToString();
			i++;
		}	
				
		DataView dv = new DataView(dt);
		
		epiUltraGrid1.DataSource = dv;
		
		da.Dispose();
		sqlCom.Dispose();
		dbConn.Dispose();

		 
	}

	private static DataTable getAllocRowsForTFOrder(Infragistics.Win.UltraWinGrid.UltraGridRow row)
	{
		DataTable dtResults = new DataTable();
		using (DynamicQueryAdapter dy = new DynamicQueryAdapter(UD14Form))
		{		
			dy.BOConnect(); 
			bool TFOrderOnly = Convert.ToBoolean(row.Cells["TFOrderOnly"].Text);
			
			if (TFOrderOnly) 
			{ 
				QueryExecutionDataSet parameters = new QueryExecutionDataSet();
				//MessageBox.Show(row.Cells["JobNumber"].Text); //TEST
				string TFOrderNum = row.Cells["JobNumber"].Text.Split('/')[0];
				string TFOrderLine = row.Cells["JobNumber"].Text.Split('/')[1];
				parameters.Tables["ExecutionParameter"].Rows.Add("TFOrderNum",TFOrderNum,"Varchar",false,null,"A");
				parameters.Tables["ExecutionParameter"].Rows.Add("TFOrderLine",TFOrderLine,"Integer",false,null,"A");
				string baq = "GetAllocTFOLines";			
				dy.ExecuteByID(baq,parameters);
				dtResults = dy.QueryResults.Tables[0];
	
				//MessageBox.Show(" getAllocRowsForTFOrder = " + dtResults.Rows.Count.ToString());
			} 	
		}
		
		return dtResults;
	}

	private static void CreateNewTFOrderUD11(Infragistics.Win.UltraWinGrid.UltraGridRow ComponentRow, DataRow InventoryRow, decimal ASRSTranID)
	{
		
		//Need to use SQl to update Number02, as it's not in the Adapter.

		Ice.Core.Session session = (Ice.Core.Session)oTrans.Session;

		SqlConnection dbConn = new SqlConnection(sqlConnection);
		dbConn.Open();
		 
		SqlCommand sqlCom = new SqlCommand("select distinct Company_UD.Number02 as Number02, Company.SysRowID as SysRowID from Company inner join Company_UD on Company.SysRowID = Company_UD.ForeignSysRowID where Company.Company = '" + session.CompanyID + "'", dbConn);
		
		SqlDataAdapter da = new SqlDataAdapter(sqlCom);
		
		DataTable dt = new DataTable();
		
		da.Fill(dt);
 
		int nextUD11 = Convert.ToInt32(dt.Rows[0][0]);
		string companyRowID = dt.Rows[0][1].ToString();
	 
		string pComponent =  ComponentRow.Cells["Component"].Text.Trim();
		string pJobNumber = ComponentRow.Cells["JobNumber"].Text.Trim();
		string pWarehouse = InventoryRow["PartAlloc_WarehouseCode"].ToString();
		int pOrderNum =  (int)ComponentRow.Cells["OrderNum"].Value;
		int pOrderLine = (int)ComponentRow.Cells["OrderLine"].Value;
		int pOrderRel = (int)ComponentRow.Cells["OrderRel"].Value;
		decimal pQty = (decimal)ComponentRow.Cells["Qty"].Value;
		decimal pAllocatedQty =  (decimal)InventoryRow["PartAlloc_AllocatedQty"] + (decimal)InventoryRow["PartAlloc_PickingQty"] + (decimal)InventoryRow["PartAlloc_PickedQty"] + (decimal)InventoryRow["PartAlloc_ReservedQty"];
		decimal pASRSTranID =  ASRSTranID;
		string pBin =  InventoryRow["PartAlloc_BinNum"].ToString();
		string pLot =  InventoryRow["PartAlloc_LotNum"].ToString();
		string pAssemblySeq = "0";
		string pPlant =  ComponentRow.Cells["Plant"].Text.Trim();
		try {
			pAssemblySeq = InventoryRow["PartAlloc_AssemblySeq"].ToString();
			}
		catch (Exception e) {}
		string pMtlSeq =  InventoryRow["PartAlloc_MtlSeq"].ToString();
		bool pOrderOnly = (bool)ComponentRow.Cells["OrderOnly"].Value;
		bool pTFOrderOnly = (bool)ComponentRow.Cells["TFOrderOnly"].Value;
		string pUser =  session.UserID;
		bool pASRS = false;
		if (pASRSTranID &amp;gt; 0) pASRS = true;

		UD11Adapter adpUD11 = new UD11Adapter(UD14Form);
		adpUD11.BOConnect();

		adpUD11.GetaNewUD11();
		// MessageBox.Show("nextUD11 = "+ nextUD11.ToString()); //TEST
		adpUD11.UD11Data.UD11.Rows[0]["Key1"] = pComponent;
		adpUD11.UD11Data.UD11.Rows[0]["Key2"] = pJobNumber;
		adpUD11.UD11Data.UD11.Rows[0]["Key3"] = "Transfer";
		adpUD11.UD11Data.UD11.Rows[0]["Key4"] = nextUD11.ToString();
		adpUD11.UD11Data.UD11.Rows[0]["Key5"] = pWarehouse;
		adpUD11.UD11Data.UD11.Rows[0]["Number01"] = pOrderNum;
		adpUD11.UD11Data.UD11.Rows[0]["Number02"] = pOrderLine;
		adpUD11.UD11Data.UD11.Rows[0]["Number03"] = pOrderRel;
		adpUD11.UD11Data.UD11.Rows[0]["Number04"] = pQty;
		adpUD11.UD11Data.UD11.Rows[0]["Number05"] = pAllocatedQty;
		adpUD11.UD11Data.UD11.Rows[0]["Number06"] = pASRSTranID; 
		adpUD11.UD11Data.UD11.Rows[0]["ShortChar01"] = session.PlantID;
		adpUD11.UD11Data.UD11.Rows[0]["ShortChar02"] = pWarehouse;
		adpUD11.UD11Data.UD11.Rows[0]["ShortChar03"] = pBin;
		adpUD11.UD11Data.UD11.Rows[0]["ShortChar04"] = pLot;
		adpUD11.UD11Data.UD11.Rows[0]["ShortChar05"] = pAssemblySeq;
		adpUD11.UD11Data.UD11.Rows[0]["ShortChar06"] = pMtlSeq;
		adpUD11.UD11Data.UD11.Rows[0]["ShortChar07"] = pUser;
		adpUD11.UD11Data.UD11.Rows[0]["CheckBox01"] = pOrderOnly;
		adpUD11.UD11Data.UD11.Rows[0]["CheckBox02"] = pASRS; 
		adpUD11.UD11Data.UD11.Rows[0]["CheckBox03"] = pTFOrderOnly;
		adpUD11.UD11Data.UD11.Rows[0]["Character01"] = pUser;
		adpUD11.UD11Data.UD11.Rows[0]["Character10"] = pPlant;

		
		try
		{
			adpUD11.UD11Data.UD11.Rows[0]["Character02"] = pJobNumber.Split('/')[0].Trim(); //TFOrderNum
			adpUD11.UD11Data.UD11.Rows[0]["Number07"] = Convert.ToInt32(pJobNumber.Split('/')[1].Trim()); //TFOrderLine
		}catch{}
		adpUD11.UD11Data.UD11.Rows[0]["Character10"] = session.PlantID;
		adpUD11.UD11Data.UD11.Rows[0]["Date01"] = DateTime.Now;

		adpUD11.Update();
		adpUD11.Dispose();

		nextUD11++;

		try {
		sqlCom = new SqlCommand("update Company_UD set Number02 = " + nextUD11.ToString()+ "where Company_UD.ForeignSysRowID = '" + companyRowID + "'", dbConn);
		sqlCom.ExecuteNonQuery();
		}
		catch (Exception e)
		{
			MessageBox.Show("1876: " + e.Message);
		}

		dbConn.Close();
		dbConn.Dispose();


	}

	private static void DeleteUD11TFOrderRecord(Infragistics.Win.UltraWinGrid.UltraGridRow InventoryRow)
	{
		UD11Adapter ud = new UD11Adapter(UD14Form);
		ud.BOConnect();
		
		ud.DeleteByID(InventoryRow.Cells["PartNum"].Text, InventoryRow.Cells["JobNumber"].Text, "Transfer", InventoryRow.Cells["RowID"].Text, InventoryRow.Cells["WarehouseCode"].Text);
		
		ud.Dispose();
	}
	
	private static DataTable getAllocatedTFOrders(Infragistics.Win.UltraWinGrid.UltraGridRow row)
	{
		  
		DataTable dt = GetInventoryTable();
		
		SqlConnection dbConn = new SqlConnection(sqlConnection);
		
		System.Text.StringBuilder sb = new System.Text.StringBuilder();
		
		sb.Append("SELECT ud11.key1, "
				   + " ud11.key2, "
				   + " ud11.key3, "
				   + " ud11.key4, "
				   + " ud11.key5, "
				   + " ud11.number01, "
				   + " ud11.number02, "
				   + " ud11.number03, "
				   + " ud11.number05, "
				   + " ud11.shortchar01,"
				   + " ud11.shortchar02,"
				   + " ud11.shortchar03,"
				   + " ud11.shortchar04,"
				   + " ud11.shortchar05,"
				   + " ud11.shortchar06,"
				   + " ud11.shortchar07, " 
				   + " ud11.date01, "
				   + " ud11.date02, "
				   + " TFOrdAlloc.partnum, "
				   + " Isnull(TFOrdAlloc.pickingqty, 0)    AS PickingQty,  "
				   + " Isnull(TFOrdAlloc.pickedqty, 0)     AS PickedQty,   "
				   + " Isnull(TFOrdAlloc.allocatedqty, 0)  AS AllocatedQty,"
				   + " Isnull(TFOrdAlloc.reservedqty, 0)   AS ReservedQty, "
				   + " TFOrdAlloc.warehousecode, "
				   + " TFOrdAlloc.binnum "
				+ " FROM   ice.ud11 "
				+ " LEFT OUTER JOIN partalloc TFOrdAlloc(nolock) "
				  + " ON TFOrdAlloc.company = ud11.company "
				  + " AND TFOrdAlloc.TFOrdNum = ud11.Character02 " //UD11 TFOrderNum
				  + " AND TFOrdAlloc.TFOrdLine = ud11.Number07 "  //UD11 TFOrderLine
				  + " AND TFOrdAlloc.partnum = ud11.key1 "
				  + " AND TFOrdAlloc.warehousecode = ud11.shortchar02 "
				  + " AND TFOrdAlloc.binnum = ud11.shortchar03 "
				  + " AND TFOrdAlloc.lotnum = ud11.shortchar04 "
				  + " WHERE ud11.key3 = 'Transfer' ");

		sb.Append(" and UD11.Key2 = '").Append(row.Cells["JobNumber"].Text.Trim()).Append("'");
		
		if(((Ice.Core.Session)oTrans.Session).UserID.ToUpper() == "MANAGER")
		{
			txtInfo.Text = sb.ToString();
			txtInfo.Visible = true;
			 
		}
		 
		SqlCommand sqlCom = new SqlCommand(sb.ToString(), dbConn);		
		SqlDataAdapter da = new SqlDataAdapter(sqlCom);		
		DataTable data = new DataTable();		
		da.Fill(data);			
		DataRow newRow;		
		foreach(DataRow temp in data.Rows)
		{
			newRow = dt.NewRow();
			
			newRow["Sel"] = false;
			newRow["Batch"] = temp["ShortChar07"].ToString();
			try
			{
				newRow["ReceiptDate"] = (DateTime)temp["Date01"];
			}
			catch{}
			
			try
			{
				newRow["Age"] = (DateTime)temp["Date02"];			
			}
			catch{}
			
			newRow["PartNum"] = temp["Key1"].ToString();
			newRow["JobNumber"] = temp["Key2"].ToString();
			newRow["Lot"] = temp["ShortChar04"].ToString();
			newRow["WarehouseCode"] = temp["Key5"].ToString();
			try
			{
			newRow["OrderNum"] = int.Parse(temp["Number01"].ToString());
			newRow["OrderLine"] = int.Parse(temp["Number02"].ToString());			
			newRow["OrderRel"] = int.Parse(temp["Number03"].ToString());
			}
			catch{}
			newRow["AssemblySeq"] = int.Parse(temp["ShortChar05"].ToString());
			newRow["MaterialSeq"] = int.Parse(temp["ShortChar06"].ToString());
			newRow["Default"] = false;
			newRow["Plant"] = temp["ShortChar01"].ToString();
			try
			{
			newRow["RowID"] = int.Parse(temp["Key4"].ToString());
			}
			catch{}
			newRow["Location"] = temp["ShortChar03"].ToString();
			
			decimal parser = 0;
			decimal total = 0;
			decimal.TryParse(temp["ReservedQty"].ToString(), out parser);
				
			total += parser;			
			decimal.TryParse(temp["AllocatedQty"].ToString(), out parser);					
			total += parser;			
			decimal.TryParse(temp["PickingQty"].ToString(), out parser);			
			total += parser;			
			decimal.TryParse(temp["PickedQty"].ToString(), out parser);			
			newRow["OH Qty"] = total;	

			
			
			dt.Rows.Add(newRow);
		}
		
		return dt;
	}

 

	private static void PrintTFOrderPickTicket(string TFOrdNum, bool TFOrderOnly = false)
	{	 
		Ice.Core.Session session = (Ice.Core.Session)oTrans.Session;
		  
		string agentID;
		string UserID =  session.UserID; 
		string reportLocation = "";
		string dataLocation = "";
		string printerName = "";
		string workstationID =  Ice.Lib.Report.EpiReportFunctions.GetWorkStationID(session);

        using(Ice.Proxy.BO.SysAgentImpl agentImpl = (Ice.Proxy.BO.SysAgentImpl)WCFServiceSupport.CreateImpl&amp;lt;Ice.Proxy.BO.SysAgentImpl&amp;gt;(session, (string)Epicor.ServiceModel.Channels.ImplBase&amp;lt;Ice.Contracts.SysAgentSvcContract&amp;gt;.UriPath))
		{ 
        	agentImpl.GetDefaultTaskAgentID(out agentID);
		}
        /*Crystal Report*/
        using ( Erp.Proxy.Rpt.TOPickImpl toPickImpl = (Erp.Proxy.Rpt.TOPickImpl)WCFServiceSupport.CreateImpl&amp;lt;Erp.Proxy.Rpt.TOPickImpl&amp;gt;(session, (string)Epicor.ServiceModel.Channels.ImplBase&amp;lt;Erp.Contracts.TOPickSvcContract&amp;gt;.UriPath))
		{
			var TOPickDataSet = toPickImpl.GetNewParameters();
	        TOPickDataSet.TOPickParam[0].OrderNumbers = TFOrdNum;
	        TOPickDataSet.TOPickParam[0].DispBarCodes = true;
	        TOPickDataSet.TOPickParam[0].SysRowID =new Guid("00000000-0000-0000-0000-000000000000");
	        TOPickDataSet.TOPickParam[0].AutoAction = (printerName == "") ? "PREVIEW" : "PRINT";
	        TOPickDataSet.TOPickParam[0].PrinterName = printerName;
	        TOPickDataSet.TOPickParam[0].AgentSchedNum = 0;
	        TOPickDataSet.TOPickParam[0].AgentID = agentID; 
	        TOPickDataSet.TOPickParam[0].AgentTaskNum = 0;
	        TOPickDataSet.TOPickParam[0].RecurringTask = false;
	        TOPickDataSet.TOPickParam[0].RptPageSettings = string.Empty;
	        TOPickDataSet.TOPickParam[0].RptPrinterSettings = string.Empty;
	        TOPickDataSet.TOPickParam[0].RptVersion = string.Empty;
	        TOPickDataSet.TOPickParam[0].ReportStyleNum = 1003;
	        TOPickDataSet.TOPickParam[0].WorkstationID = workstationID;
	        TOPickDataSet.TOPickParam[0].TaskNote = string.Empty;
	        TOPickDataSet.TOPickParam[0].ArchiveCode = 0;
	        TOPickDataSet.TOPickParam[0].DateFormat = "m/d/yyyy";
	        TOPickDataSet.TOPickParam[0].NumericFormat = ",.";
	        TOPickDataSet.TOPickParam[0].AgentCompareString = string.Empty;
	        TOPickDataSet.TOPickParam[0].ProcessID = string.Empty;
	        TOPickDataSet.TOPickParam[0].ProcessCompany = string.Empty;
	        TOPickDataSet.TOPickParam[0].ProcessSystemCode = string.Empty;
	        TOPickDataSet.TOPickParam[0].ProcessTaskNum = 0;
	        TOPickDataSet.TOPickParam[0].DecimalsGeneral = 0;
	        TOPickDataSet.TOPickParam[0].DecimalsCost = 0;
	        TOPickDataSet.TOPickParam[0].DecimalsPrice = 0;
	        TOPickDataSet.TOPickParam[0].GlbDecimalsGeneral = 0;
	        TOPickDataSet.TOPickParam[0].GlbDecimalsCost = 0;
	        TOPickDataSet.TOPickParam[0].GlbDecimalsPrice = 0;
	        TOPickDataSet.TOPickParam[0].FaxSubject = string.Empty;
	        TOPickDataSet.TOPickParam[0].FaxTo = string.Empty;
	        TOPickDataSet.TOPickParam[0].FaxNumber = string.Empty;
	        TOPickDataSet.TOPickParam[0].EMailTo = string.Empty;
	        TOPickDataSet.TOPickParam[0].EMailCC = string.Empty;
	        TOPickDataSet.TOPickParam[0].EMailBCC = string.Empty;
	        TOPickDataSet.TOPickParam[0].EMailBody = string.Empty;
	        TOPickDataSet.TOPickParam[0].AttachmentType = string.Empty;
	        TOPickDataSet.TOPickParam[0].ReportCurrencyCode = "USD";
	        TOPickDataSet.TOPickParam[0].ReportCultureCode = "en-US";
	        TOPickDataSet.TOPickParam[0].SSRSRenderFormat = string.Empty; 
	        TOPickDataSet.TOPickParam[0].UIXml = string.Empty;
	        TOPickDataSet.TOPickParam[0].PrintReportParameters = false;
	        TOPickDataSet.TOPickParam[0].SSRSEnableRouting = false;
	        TOPickDataSet.TOPickParam[0].RowMod = "A";
	
			
		
			toPickImpl.SubmitToAgent(TOPickDataSet,agentID,0,0, "Erp.UIRpt.TOPick");
		} 
	}

	private static List&amp;lt;string[]&amp;gt; DeAllocatePreTFORows(Infragistics.Win.UltraWinGrid.UltraGridRow ComponentRow)
	{
		bool TFOrderOnly = (bool)ComponentRow.Cells["TFOrderOnly"].Value ? true : false; //MT
		DataTable dtTFORowsAlloc = new DataTable();

		List&amp;lt;string[]&amp;gt; lstLots = new List&amp;lt;string[]&amp;gt;();
		if(TFOrderOnly)
		{ 
			foreach(Infragistics.Win.UltraWinGrid.UltraGridRow row in LocationGrid.Rows)
			{		
				if(row.Cells["Sel"].Text == "True" )
				{
					//MessageBox.Show("JLP - Row Selected");

					Infragistics.Win.UltraWinGrid.UltraGridRow InventoryRow = row;
					string[] strParams = new string[3];
					string pWhs =  InventoryRow.Cells["WarehouseCode"].Text;
					string pLocation =  InventoryRow.Cells["Location"].Text;
					string pLot =  InventoryRow.Cells["Lot"].Text;
					strParams[0] = pWhs;
					strParams[1] = pLocation;
					strParams[2] = pLot;
					lstLots.Add(strParams);
				}
			}


			dtTFORowsAlloc = getAllocRowsForTFOrder(ComponentRow);
			if(dtTFORowsAlloc.Rows.Count&amp;gt; 0)
			{
				
				for(int i=0; i &amp;lt; dtTFORowsAlloc.Rows.Count; i++)
				{
					//lstLots.Add(dtTFORowsAlloc.Rows[i]["PartAlloc_LotNum"].ToString());
					UD11Adapter ud = new UD11Adapter(UD14Form);
					ud.BOConnect();
					string whereClause =  " Key1 = '" + dtTFORowsAlloc.Rows[i]["PartAlloc_PartNum"].ToString() + "' and Key2 = '" + dtTFORowsAlloc.Rows[i]["PartAlloc_TFOrdNum"].ToString()+ " / "+dtTFORowsAlloc.Rows[i]["PartAlloc_TFOrdLine"].ToString() + " / ' and Key3='Transfer' ";                                        
					//MessageBox.Show(whereClause);
					SearchOptions optsLd = new SearchOptions(SearchMode.AutoSearch);
					optsLd.PreLoadSearchFilter = whereClause;
					optsLd.SelectMode = SelectMode.SingleSelect;                  
					optsLd.DataSetMode = DataSetMode.RowsDataSet;                                                                        
					ud.InvokeSearch(optsLd);
					for(int j = 0; j &amp;lt; ud.UD11Data.UD11.Rows.Count; j++)
					{
						string[] strParams = new string[3];
						string pWhs =  ud.UD11Data.UD11.Rows[j]["ShortChar02"].ToString();
						string pLocation =  ud.UD11Data.UD11.Rows[j]["ShortChar03"].ToString();
						string pLot =  ud.UD11Data.UD11.Rows[j]["ShortChar04"].ToString();
						strParams[0] = pWhs;
						strParams[1] = pLocation;
						strParams[2] = pLot;
						lstLots.Add(strParams);
						oTrans.PushStatusText("Delete row " + ud.UD11Data.UD11.Rows[j]["Key4"].ToString() + " ...", true);
						ud.UD11Data.UD11.Rows[j].Delete();				
						
					}
                    ud.Update();
					ud.Dispose();            
					 
				}

				DeAllocate( ComponentRow); 
				//MessageBox.Show("Dealllocate");
			}  
			
		}
		
		return lstLots;
	}
	
	//TW_JD 01202021 Start ************************
	private static void RemoveMBSoftAllocations(string JobNum, string PartNum, decimal Qty)
	{
		// ** Place Event Handling Code Here **
		try{
			// var confirmResult =  MessageBox.Show("Are you sure to remove soft allocations for JobNum "+ JobNum +" and Part " + PartNum + "?","Confirm Deallocate", MessageBoxButtons.YesNo);
			// if (confirmResult == DialogResult.Yes)
			// {
				// string msqlConnection = GetConnectionInfoMSOM(); //sqlConnection;
				// var softAllocMB = new Habasit.MSOM_Unalloc_MB.SoftAllocations(msqlConnection);
            	// softAllocMB.RemoveMBSoftAllocationsQty(JobNum, PartNum, Qty);
			// }
			string msqlConnection = GetConnectionInfoMSOM(); //sqlConnection;
			var softAllocMB = new Habasit.MSOM_Unalloc_MB.SoftAllocations(msqlConnection, ((Ice.Core.Session)UD14Form.Session).CompanyID);
            softAllocMB.RemoveMBSoftAllocationsQty(JobNum, PartNum, Qty);
		}
		catch (Exception e)
	    {
	            MessageBox.Show(e.Message, "Problem removing soft allocations");
	    }
	}
	private static void RemoveMBSoftAllocations(int OrderNum, int OrderLine, int OrderRel, string PartNum, decimal Qty)
	{
		// ** Place Event Handling Code Here **
		try{
			// var confirmResult =  MessageBox.Show("Are you sure to remove soft allocations for JobNum "+ JobNum +" and Part " + PartNum + "?","Confirm Deallocate", MessageBoxButtons.YesNo);
			// if (confirmResult == DialogResult.Yes)
			// {
				// string msqlConnection = GetConnectionInfoMSOM(); //sqlConnection;
				// var softAllocMB = new Habasit.MSOM_Unalloc_MB.SoftAllocations(msqlConnection);
            	// softAllocMB.RemoveMBSoftAllocationsQty(JobNum, PartNum, Qty);
			// }
			string msqlConnection = GetConnectionInfoMSOM(); //sqlConnection;
			var softAllocMB = new Habasit.MSOM_Unalloc_MB.SoftAllocations(msqlConnection, ((Ice.Core.Session)UD14Form.Session).CompanyID);
			softAllocMB.RemoveMBSoftAllocationsQty(OrderNum, OrderLine, OrderRel, PartNum, Qty);
		}
		catch (Exception e)
	    {
	            MessageBox.Show(e.Message, "Problem removing soft allocations");
	    }
	}


	public static string GetConnectionInfoMSOM() {
	  string strProgramName = "MSOM";
	  string connString = "";
	
	  try {
	    bool recSelected = false;
	
	    Ice.Core.Session session = (Ice.Core.Session) UD14Form.Session;
	    string company = session.CompanyID;
	
	    string whereClause = "Company = '" + company + "' AND Key1 = 'PROGRAM' AND Key2 = '" + strProgramName + "' AND Key3 = '1' AND Key4 = '' AND Key5 = ''";
	    DataSet dsUD01 = Ice.UI.FormFunctions.SearchFunctions.listLookup(oTrans, "UD01Adapter", out recSelected, false, whereClause);
	    if ((recSelected)) {
	      connString = dsUD01.Tables[0].Rows[0]["Character01"].ToString();
	    }
	  }
	  catch(Exception e) {
	    connString = "";
	  }
	  return connString;
	}
	//TW_JD 01202021 End   ************************
}










































