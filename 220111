		DataTable dt = new DataTable();
		
		dt.Columns.Add("OrderNum", typeof(int));
		dt.Columns.Add("OrderLine", typeof(int));
        dt.Columns.Add("OrderRelNum", typeof(int));
		dt.Columns.Add("PartNum", typeof(string));
		dt.Columns.Add("OurReqQty", typeof(decimal));
        dt.Columns.Add("ShortChar10", typeof(string));
		dt.Columns.Add("Key4", typeof(string));
		dt.Columns.Add("Key5", typeof(string));
		dt.Columns.Add("Number05", typeof(decimal));
		dt.Columns.Add("Date01", typeof(DateTime));
		dt.Columns.Add("Date02", typeof(DateTime));
		dt.Columns.Add("Batch", typeof(string));
		dt.Columns.Add("LotNum", typeof(string));
		dt.Columns.Add("ShortChar03", typeof(string));
		dt.Columns.Add("BinNum", typeof(string));
		dt.Columns.Add("LineDesc", typeof(string));
		dt.Columns.Add("ShipDate", typeof(DateTime));
		dt.Columns.Add("OrderDate", typeof(DateTime));
		dt.Columns.Add("EnteredBy", typeof(string));
		dt.Columns.Add("ShipToAddress1", typeof(string));
		dt.Columns.Add("ShipToAddress2", typeof(string));
		dt.Columns.Add("ShipToAddress3", typeof(string));
		dt.Columns.Add("ShipToCity", typeof(string));
		dt.Columns.Add("ShipToState", typeof(string));
		dt.Columns.Add("ShipToZip", typeof(string));
		dt.Columns.Add("ShipVia", typeof(string));
		dt.Columns.Add("CustName", typeof(string));
		dt.Columns.Add("SoldToAddress1", typeof(string));
		dt.Columns.Add("SoldToAddress2", typeof(string));
		dt.Columns.Add("SoldToAddress3", typeof(string));
		dt.Columns.Add("SoldToCity", typeof(string));
		dt.Columns.Add("SoldToState", typeof(string));
		dt.Columns.Add("SoldToZip", typeof(string));
		dt.Columns.Add("SoldToName", typeof(string));		
		dt.Columns.Add("OrderComment", typeof(string));
		dt.Columns.Add("ShipComment", typeof(string));
		dt.Columns.Add("InvoiceComment", typeof(string));
		dt.Columns.Add("PickListComment", typeof(string));
		dt.Columns.Add("WarrantyComment", typeof(string));
        dt.Columns.Add("FromWhse", typeof(string));
        dt.Columns.Add("MtlQueueSeq", typeof(int));  
		dt.Columns.Add("PickedQty", typeof(decimal));


		using (DynamicQueryAdapter dy = new DynamicQueryAdapter(UD14Form))
		{		
			dy.BOConnect();
			QueryExecutionDataSet parameters = new QueryExecutionDataSet();
			string baq = "AllocList";
			parameters.Tables["ExecutionParameter"].Rows.Add("OrderNum",OrderNum,"Integer",false,null,"A");
			dy.ExecuteByID(baq,parameters);

			foreach (DataRow dr in dy.QueryResults.Tables[0].Rows)
			{
				DataRow newRow = dt.NewRow();
				newRow["OrderNum"] = dr["OrderRel_OrderNum"];
				newRow["OrderLine"] = dr["OrderRel_OrderLine"];
				newRow["OrderRelNum"] = dr["OrderRel_OrderRelNum"];
				newRow["PartNum"] = dr["OrderRel_PartNum"];
                newRow["OurReqQty"] = dr["OrderRel_OurReqQty"];
                newRow["ShortChar10"] = dr["OrderRel_ShortChar10"];
                newRow["LineDesc"] = dr["OrderDtl_LineDesc"];
                newRow["Key4"] = dr["UD11_Key4"];
                newRow["Key5"] = dr["UD11_Key5"];
                newRow["Number05"] = dr["UD11_Number05"];
                newRow["Date01"] = dr["OrderRel_ReqDate"];
                newRow["Date02"] = dr["UD11_Date02"];
                newRow["BATCH"] = dr["UD11_ShortChar07"];
                newRow["LotNum"] = dr["UD11_ShortChar04"];
                newRow["ShortChar03"] = dr["UD11_ShortChar03"];
                newRow["BinNum"] = dr["UD11_ShortChar03"];
                newRow["ShipDate"] = dr["OrderHed_RequestDate"];
                newRow["OrderDate"] = dr["OrderHed_OrderDate"];
                newRow["CustName"] = dr["ShipTo_Name"];
                newRow["SoldToName"] = dr["Customer_Name"];
                newRow["SoldToAddress1"] = dr["Customer_Address1"];
                newRow["SoldToAddress2"] = dr["Customer_Address2"];
                newRow["SoldToAddress3"] = dr["Customer_Address3"];
                newRow["SoldToCity"] = dr["Customer_City"];
                newRow["SoldtoState"] = dr["Customer_State"];
                newRow["SoldToZip"] = dr["Customer_Zip"];
                newRow["OrderComment"] = dr["OrderDtl_OrderComment"];
                newRow["ShipComment"] = dr["OrderDtl_ShipComment"];
                newRow["InvoiceComment"] = dr["OrderDtl_InvoiceComment"];
                newRow["PickListComment"] = dr["OrderDtl_PickListComment"];
                newRow["WarrantyComment"] = dr["OrderDtl_WarrantyComment"];
                if (dr["MtlQueue_MtlQueueSeq"] != DBNull.Value) newRow["MtlQueueSeq"] = dr["MtlQueue_MtlQueueSeq"]; else newRow["MtlQueueSeq"] = 999;
                if (dr["MtlQueue_FromWhse"] != DBNull.Value) newRow["FromWhse"] = dr["MtlQueue_FromWhse"]; else newRow["FromWhse"] = "";
                if (dr["PartAlloc_PickedQty"] != DBNull.Value) newRow["PickedQty"] = dr["PartAlloc_PickedQty"]; else newRow["PickedQty"] = 0;
                if (dr["UserFile_Name"] != DBNull.Value) newRow["EnteredBy"] = dr["UserFile_Name"]; else newRow["EnteredBy"] = "";
                if (dr["ShipVia_Description"] != DBNull.Value) newRow["ShipVia"] = dr["ShipVia_Description"]; else newRow["ShipVia"] = "";

				if ((bool)dr["OrderHed_UseOTS"])
				{
                    newRow["ShipToAddress1"] = dr["OrderHed_OTSAddress1"];
                    newRow["ShipToAddress2"] = dr["OrderHed_OTSAddress2"];
                    newRow["ShipToAddress3"] = dr["OrderHed_OTSAddress3"];
                    newRow["ShipToCity"] = dr["OrderHed_OTSCity"];
                    newRow["ShipToState"] = dr["OrderHed_OTSState"];
                    newRow["ShipToZip"] = dr["OrderHed_OTSZip"];
			    	newRow["CustName"] = dr["OrderHed_OTSName"];
				}
				else 
				{
                    newRow["ShipToAddress1"] = dr["ShipTo_Address1"];
                    newRow["ShipToAddress2"] = dr["ShipTo_Address2"];
                    newRow["ShipToAddress3"] = dr["ShipTo_Address3"];
                    newRow["ShipToCity"] = dr["ShipTo_City"];
                    newRow["ShipToState"] = dr["ShipTo_State"];
                    newRow["ShipToZip"] = dr["ShipTo_Zip"];
				}
		
				dt.Rows.Add(newRow);
			}
		}

		dt.TableName = "Results";
		
		ds.Tables.Add(dt);

		return ds;
	}

	private static string saveXML(DataSet ds)
	{
		string retValue = "";
		bool selected = false; 
		
		string fileName = "HaOrderAlloc_" + ((DateTime.Now.Hour * 3600) + (DateTime.Now.Minute * 60) + DateTime.Now.Second).ToString() + ".xml";
		
		string location = SearchFunctions.listLookup(oTrans.EpiBaseForm, "SysAgentAdapter", out selected, false, "").Tables[0].Rows[0]["ClientFileRootDir"].ToString() + "\\Reports\\" + ((Ice.Core.Session)oTrans.Session).UserID;
		
		if(!System.IO.Directory.Exists(location))
			System.IO.Directory.CreateDirectory(location);
			
		retValue = location + "\\" + fileName;
		
		ds.WriteXml(retValue, XmlWriteMode.WriteSchema);
		
		return retValue;
	}

